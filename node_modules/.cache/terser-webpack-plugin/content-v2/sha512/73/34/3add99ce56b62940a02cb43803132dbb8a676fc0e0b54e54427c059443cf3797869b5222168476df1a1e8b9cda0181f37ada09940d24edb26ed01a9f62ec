                                               {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select category\\\", this.state.category, \\\"required\\\")\\r\\n                                                                        }\\r\\n\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Select Sub Category<span style={{color: 'red' }}>*</span>\\r\\n                                                            </label>\\r\\n                                                            <select\\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"subCategory\\\"\\r\\n                                                                value={this.state.subCategory}\\r\\n                                                                onChange={this.onChange}>\\r\\n                                                                <option>Select Sub Category</option>\\r\\n                                                                {this.state.subcategoryList && this\\r\\n                                                                    .state\\r\\n                                                                    .subcategoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.subCategory} key={index}>{data.subCategory}</option>\\r\\n                                                                        )\\r\\n\\r\\n                                                                    })}\\r\\n                                                                {this\\r\\n                                                                    .validator\\r\\n                                                                    .message(\\\"Select Sub category\\\", this.state.subCategory, \\\"required\\\")\\r\\n}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>GST</label>\\r\\n\\r\\n                                                            <select\\r\\n                                                                name=\\\"select\\\"\\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"gstValue\\\"\\r\\n                                                                value={this.state.gstValue}\\r\\n                                                                onChange={this.onChange}>\\r\\n                                                                <option>Select GST<span style={{color: 'red' }}>*</span></option>\\r\\n                                                                {this.state.GSTList && this\\r\\n                                                                    .state\\r\\n                                                                    .GSTList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.gst} key={index}>{data.gst}</option>\\r\\n                                                                        )\\r\\n                                                                        {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select GST\\\", this.state.gstValue, \\\"required\\\")\\r\\n                                                                        }\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Price<span style={{color: 'red' }}>*</span></label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"price\\\"\\r\\n                                                                id=\\\"product_price\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.price}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Price Value\\\", this.state.price, \\\"required|numeric|min:1|max:5\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Selling Price</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"sellingPrice\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.sellingPrice}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\" Selling Price\\\", this.state.sellingPrice, \\\"required|prod