       onUploadStart={this.handleUploadStart}\\r\\n            onUploadError={this.handleUploadError}\\r\\n            onUploadSuccess={this.handleUploadSuccess}\\r\\n            onProgress={this.handleProgress}\\r\\n          />\\r\\n                         {this.validator.message(\\r\\n                  \\\"image upload\\\",\\r\\n                  this.state.avatarURL,\\r\\n                  \\\"required\\\"\\r\\n                )}\\r\\n                       \\r\\n                         </div>\\r\\n\\r\\n                            </div>\\r\\n\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n                                                    <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Add\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t\\t</button>\\r\\n                                                 \\r\\n                                                </div>\\r\\n                                            </div>\\r\\n                                        </div>\\r\\n                                        </Form>\\r\\n                                    </div>\\r\\n                                 \\r\\n\\r\\n\\t\\t\\t\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\t\\t\\t\\t\\t\\t\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div>\\r\\n        </div>\\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default AddSitePage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport FadeIn from \\\"react-fade-in\\\";\\r\\nimport axios from \\\"axios\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport swal from 'sweetalert';\\r\\nimport $ from 'jquery';\\r\\n$.DataTable = require( 'datatables.net' );\\r\\n\\r\\n\\r\\nclass EmailContentListPage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        emailContentList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"emailContent/\\\");\\r\\n\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        // console.log(snapshot);\\r\\n        // console.log(snapshot.val);\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const EmailData = {\\r\\n            emailContentId: childSnapShot.key.toString(),\\r\\n            emailContentName: childSnapShot.val().emailContentName,\\r\\n            emailDesc: childSnapShot.val().emailDesc,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(EmailData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ emailContentList:data, countPage: data.length,loading: false });\\r\\n        console.log(this.state.emailContentList);\\r\\n        this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n    });\\r\\n}\\r\\ncomponentWillUnmount() {\\r\\n    clearTimeout(this.interval);\\r\\n  }\\r\\n  deleteItem = id => {\\r\\n    \\r\\n    swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n      if (willDelete) {\\r\\n        console.log(id);\\r\\n        var playersRef = firebase.database().ref(`/emailContent/${id}`);\\r\\n        playersRef.remove();\\r\\n        \\r\\n  \\r\\n      } else {}\\r\\n     \\r\\n     \\r\\n  });\\r\\n  \\r\\n    };\\r\\n   \\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Merchants</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Email Content List</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddEmailContentPage'>Add Email Content</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Name</th>\\r\\n                                    <th>Description</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.emailContentList && this.state.emailContentList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.emailContentName}</td>\\r\\n                                         <td>{data.emailDesc}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditEmailContentPage/${data.emailConte