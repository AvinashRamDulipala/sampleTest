          <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>GST List</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddGSTPage'>Add GST</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>GST value</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.gstList && this.state.gstList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.gst}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditGSTPage/${data.gstId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\