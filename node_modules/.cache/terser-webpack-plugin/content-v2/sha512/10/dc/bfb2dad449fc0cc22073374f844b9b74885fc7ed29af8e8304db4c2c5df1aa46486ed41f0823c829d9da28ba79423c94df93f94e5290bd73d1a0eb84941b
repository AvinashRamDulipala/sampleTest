                        <div className=\\\"col-sm-10\\\">\\r\\n                                                            <input\\r\\n                                                type=\\\"text\\\"\\r\\n                                                className=\\\"form-control\\\"\\r\\n                                                placeholder=\\\"Contact Number\\\"\\r\\n                                                name=\\\"contact_number\\\"\\r\\n                                                onChange={this.onChange}\\r\\n                                                value={this.state.contact_number} readOnly/> {this\\r\\n                                                .validator\\r\\n                                                .message(\\\"Contact Number\\\", this.state.contact_number, \\\"required|numeric|min:4|max:15\\\")}\\r\\n                                            <div className=\\\"text-danger\\\">\\r\\n                                                {\\\" \\\"}\\r\\n                                                {this.state.mobile_message}\\r\\n                                            </div>\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                        {/* <div className=\\\"form-group\\\">\\r\\n                                                            <label htmlFor=\\\"inputEmail3\\\" className=\\\"col-sm-2 control-label\\\">status</label>\\r\\n                                                            <div className=\\\"col-sm-10\\\">\\r\\n                                                                <label className=\\\"radio-inline no-padd\\\">\\r\\n                                                                    <input\\r\\n                                                                        type=\\\"radio\\\"\\r\\n                                                              