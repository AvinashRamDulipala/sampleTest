                                                            placeholder=\\\"\\\"\\r\\n                                                                name=\\\"productName\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.productName}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Product Name\\\", this.state.productName, \\\"required|whitespace|min:2|max:70\\\")}\\r\\n                                                        </div>\\r\\n\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Select Category</label>\\r\\n                                                            <select\\r\\n                                                              \\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"category\\\"\\r\\n                                                                onChange={this.setCategoryName}>\\r\\n                                                                <option>Select Category</option>\\r\\n                                                                {this\\r\\n                                                                    .state\\r\\n                                                                    .categoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.category} key={index}>{data.category}</option>\\r\\n                                                                        )\\r\\n                                                                        {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select category\\\", data.category, \\\"required\\\")\\r\\n                                                                        }\\r\\n\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n\\r\\n                            \\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Select Sub Category</label>\\r\\n                                                            <select\\r\\n                                                              \\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"subCategory\\\"\\r\\n                                                                onChange={this.onChange}>\\r\\n                                                                <option>Select Sub Category</option>\\r\\n                                                                {this.state.subcategoryList && this\\r\\n                                                                    .state\\r\\n                                                                    .subcategoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.subCategory} key={index}>{data.subCategory}</option>\\r\\n                                                                        )\\r\\n                                                                        {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select Sub category\\\", data.subCategory, \\\"required\\\")\\r\\n                                                                        }\\r\\n\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>GST</label>\\r\\n\\r\\n                                                            <select\\r\\n                                                                name=\\\"select\\\"\\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"gstValue\\\"\\r\\n                                                                onChange={this.onChange}>\\r\\