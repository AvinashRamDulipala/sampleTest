        .ref(\\\"merchant/\\\");\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        // console.log(snapshot); console.log(snapshot.val);\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n\\r\\n            const Merchants = {\\r\\n                merchantId: childSnapShot.key.toString(),\\r\\n                merchantName: childSnapShot.val().merchantName,\\r\\n                gstValue: childSnapShot.val().gstValue,\\r\\n                products: childSnapShot.val().products,\\r\\n                orders: childSnapShot.val().orders,\\r\\n                delivery: childSnapShot.val().delivery,\\r\\n                coupons: childSnapShot.val().coupons,\\r\\n                contactNumber: childSnapShot.val().contactNumber,\\r\\n                notification: childSnapShot.val().notification,\\r\\n                status: childSnapShot.val().status,\\r\\n                created_on: childSnapShot .val().created_on\\r\\n            };\\r\\n\\r\\n            data.push(Merchants);\\r\\n        });\\r\\n        console.log(data.length);\\r\\n        this.setState({merchantList: data, countPage: data.length,loading: false});\\r\\n        console.log(this.state.merchantList);\\r\\n        console.log(data.length);\\r\\n    });\\r\\n\\r\\n}\\r\\ndeleteItem = id => {\\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase\\r\\n        .database()\\r\\n        .ref(`/merchant/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n};\\r\\nhandlePageChange(pageNumber) {\\r\\n    console.log(`active page is ${pageNumber}`);\\r\\n    this.setState({ activePage: pageNumber });\\r\\n  }\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div c