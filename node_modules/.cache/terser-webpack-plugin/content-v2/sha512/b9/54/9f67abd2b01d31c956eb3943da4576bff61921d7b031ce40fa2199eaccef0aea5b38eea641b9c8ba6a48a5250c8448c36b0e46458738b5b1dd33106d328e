/i>\\r\\n                                                                        <h2>Upload File<span style={{\\r\\n                color: 'red'\\r\\n            }}>*</span>\\r\\n                                                                        </h2>\\r\\n\\r\\n                                                                    </div>\\r\\n\\r\\n                                                                    {this.state.downloadURLs && this\\r\\n                                                                        .state\\r\\n                                                                        .downloadURLs\\r\\n                                                                        .map((downloadURL, i) => {\\r\\n                                                                            return <img\\r\\n                                                                                key={i}\\r\\n                                                                                src={downloadURL}\\r\\n                                                                                style={{\\r\\n                                                                                height: \\\"200px\\\",\\r\\n                                                                                width: \\\"250px\\\"\\r\\n                                                                            }}/>;\\r\\n                                                                        })}\\r\\n                                                                   <div className=\\\"image_block\\\">\\r\\n                                                                {this.state.isUploading && <p>Filenames: {this\\r\\n                                                                        .state\\r\\n                                                                        .filenames\\r\\n                                                                        .join(\\\", \\\")}</p>}\\r\\