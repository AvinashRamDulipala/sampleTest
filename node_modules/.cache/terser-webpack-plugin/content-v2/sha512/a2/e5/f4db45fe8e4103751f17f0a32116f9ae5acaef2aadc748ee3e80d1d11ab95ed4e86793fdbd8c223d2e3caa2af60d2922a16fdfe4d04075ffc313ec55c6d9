 if (this.validator.allValid()) {\\r\\n            var user = null;\\r\\n            var ref = firebase\\r\\n            .database()\\r\\n            .ref('admin_users/')\\r\\n            .orderByChild(\\\"contact_number\\\")\\r\\n            .equalTo(this.state.contact_number);\\r\\n        ref.on('value', snapshot => {\\r\\n            var user_exist = snapshot.numChildren();\\r\\n            console.log(user_exist);\\r\\n\\r\\n            if (user_exist > 0) {\\r\\n                this.setState({mobile_message: \\\"Mobile number alredy exist\\\", contact_number: ''});\\r\\n            } else {\\r\\n\\r\\n            firebase\\r\\n                .auth()\\r\\n                .createUserWithEmailAndPassword(this.state.email, this.state.password)\\r\\n                .then((result) => {\\r\\n                  console.log(result);\\r\\n\\r\\n                    var userId = result.user;\\r\\n                    user = firebase\\r\\n                        .auth()\\r\\n                        .currentUser;\\r\\n                    // user.sendEmailVerification();\\r\\n                   console.log(user);\\r\\n                    return firebase\\r\\n                        .database()\\r\\n                        .ref('admin_users/' + userId.uid)\\r\\n                        .set({\\r\\n\\r\\n                            fullName: this.state.fullName,\\r\\n                            email: this.state.email,\\r\\n                            contact_number: this.state.contact_number,\\r\\n                            username: this.state.username,\\r\\n                            password: this.state.password,\\r\\n                            status: this.state.status,\\r\\n                            created_on: this.state.date,\\r\\n                            Role: \\\"Merchant\\\",\\r\\n                            status: \\\"InActive\\\",\\r\\n                        });\\r\\n\\r\\n                })\\r\\n                .then(() => {\\r\\n                    retur