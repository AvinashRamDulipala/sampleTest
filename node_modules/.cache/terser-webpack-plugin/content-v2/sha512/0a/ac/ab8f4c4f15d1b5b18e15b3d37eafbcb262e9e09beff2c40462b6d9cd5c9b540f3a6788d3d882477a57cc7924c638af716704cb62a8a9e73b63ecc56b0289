  );\\r\\n    }\\r\\n}\\r\\n\\r\\nexport default AddSubSlider;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport swal from 'sweetalert';\\r\\nimport $ from 'jquery';\\r\\n$.DataTable = require('datatables.net');\\r\\nclass SubSliderList extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            categoryList: [],\\r\\n            loading: false,\\r\\n            count: 1,\\r\\n            activePage: \\\"1\\\",\\r\\n            countPage: \\\"\\\",\\r\\n            status: ''\\r\\n        };\\r\\n        this.deleteItem = this\\r\\n            .deleteItem\\r\\n            .bind(this);\\r\\n\\r\\n    }\\r\\n\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"SubSlider/\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n                    cmsId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    pageTitle: childSnapShot\\r\\n                        .val()\\r\\n                        .pageTitle,\\r\\n                    userName: childSnapShot\\r\\n                        .val()\\r\\n                        .userName,\\r\\n                    status: childSnapShot\\r\\n                        .val()\\r\\n                        .status,\\r\\n                        Category: childSnapShot\\r\\n                        .val()\\r\\n                        .category,\\r\\n                    sliderUrl: childSnapShot\\r\\n                        .val()\\r\\n                        .sliderUrl,\\r\\n                    uploadImage: childSnapShot\\r\\n                        .val()\\r\\n                        .uploadImage,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({gstList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.gstList);\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destroy(true);\\r\\n        clearTimeout(this.interval);\\r\\n      }\\r\\n    deleteItem = id => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(id);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/SubSlider/${id}`);\\r\\n                playersRef.remove();\\r\\n                this\\r\\n                .props\\r\\n                .history\\r\\n                .push(\\\"/SubSliderList/Deleted\\\");\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n\\r\\n    };\\r\\n   \\r\\n\\r\\n    onChange = (event) => {\\r\\n\\r\\n        var productId = event.target.id;\\r\\n        console.log(productId);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n        console.log(event.target.value);\\r\\n\\r\\n        let dbCon = firebase\\r\\n            .database()\\r\\n            .ref(`/SubSlider/${productId}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update({status: event.target.value});\\r\\n    };\\r\\n    render() {\\r\\n        var pathname = this\\r\\n        .props\\r\\n        .location\\r\\n        .pathname\\r\\n        .split(\\\"/\\\");\\r\\n    var alertName = pathname[2];\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Slider</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"product-status mg-b-30\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"product-status-wrap\\\">\\r\\n                                        <h4>Sub Slider List</h4>\\r\\n                                        <div className=\\\"add-product\\\">\\r\\n                                            <Link to='/AddSubSlider'>Add SubSlider</Link>\\r\\n                                        </div>\\r\\n                                        {alertName\\r\\n                                            ? <div\\r\\n                                                    className=\\\"alert alert-success display-none\\\"\\r\\n                                                    style={{\\r\\n                                                    display: \\\"block\\\"\\r\\n                                                }}>\\r\\n                                                    <a data-dismiss=\\\"alert\\\" href=\\\"#\\\" aria-hidden=\\\"true\\\" className=\\\"close\\\"></a>\\r\\n                                                    Sub Slider {alertName}\\r\\n                                                    Successfully\\r\\n                                                </div>\\r\\n                                            : ''}\\r\\n                                        <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                                            <thead>\\r\\n                                                <tr>\\r\\n                                                    <th>S.No</th>\\r\\n                                                    <th>Slider Name</th>\\r\\n                                                    <th>Slider Image</th>\\r\\n                                                    <th>Slider Url</th>\\r\\n                                                    <th>Category</th>\\r\\n                                                    <th>Status</th>\\r\\n                                                    <th>Created On</th>\\r\\n                                                    <th>Action</th>\\r\\n                                                </tr>\\r\\n                                            </thead>\\r\\n                                            <tbody>\\r\\n\\r\\n                                                {this.state.gstList && this\\r\\n                                                    .state\\r\\n                                                    .gstList\\r\\n                                                    .map((data, index) => {\\r\\n                                                        console.log(data);\\r\\n\\r\\n                                                        return (\\r\\n                                                            <tr key={index}>\\r\\n                                                                <td>{index + 1}</td>\\r\\n                                                                <td>{data.pageTitle}</td>\\r\\n                                                                <td className=\\\"img-product\\\"><img src={data.uploadImage} alt=\\\"\\\"/></td>\\r\\n                                                                <td>{data.sliderUrl}</td>\\r\\n                                                                <td>{data.Category}\\r\\n                                                                </td>\\r\\n                                                                <td>\\r\\n                                                                    <select\\r\\n                                                                        name=\\\"status\\\"\\r\\n                                                                        onChange={this.onChange}\\r\\n                                                                        id={data.cmsId}\\r\\n                                                                        className=\\\"form-control pro-edt-select form-control-primary active\\\">\\r\\n                                                          