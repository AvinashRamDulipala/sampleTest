         \\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.siteName}</td>\\r\\n                                         <td>{data.siteURL}</td>\\r\\n                                         <td><img height=\\\"80\\\" width=\\\"100\\\" src={data.siteImage}/></td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditSitePage/${data.siteId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.siteId}  onClick={this.deleteItem.bind(this, data.siteId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    {/* <div className=\\\"custom-pagination\\\">\\r\\n                            <Pagination\\r\\n                          hideNavigation\\r\\n                          activePage={this.state.activePage}\\r\\n                          itemsCountPerPage={3}\\r\\n                          totalItemsCount={this.state.countPage}\\r\\n                   