\\r\\n                .orderByChild(\\\"sessionId\\\")\\r\\n                .equalTo(sessionId);\\r\\n\\r\\n        } else {\\r\\n            var ref = firebase\\r\\n                .database()\\r\\n                .ref(\\\"products/\\\");\\r\\n        }\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n\\r\\n            const data = [];\\r\\n            // console.log(snapshot); console.log(snapshot.val);\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const Products = {\\r\\n                    productId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    productName: childSnapShot\\r\\n                        .val()\\r\\n                        .productName,\\r\\n                    category: childSnapShot\\r\\n                        .val()\\r\\n                        .category,\\r\\n                    userName: childSnapShot\\r\\n                        .val()\\r\\n                        .userName,\\r\\n                    price: childSnapShot\\r\\n                        .val()\\r\\n                        .price,\\r\\n                    sellingPrice: childSnapShot\\r\\n                        .val()\\r\\n                        .sellingPrice,\\r\\n                    status: childSnapShot\\r\\n                        .val()\\r\\n                        .status,\\r\\n                    stock: childSnapShot\\r\\n                        .val()\\r\\n                        .stock,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(Products);\\r\\n            });\\r\\n\\r\\n            this.setState({productList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.productList);\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });