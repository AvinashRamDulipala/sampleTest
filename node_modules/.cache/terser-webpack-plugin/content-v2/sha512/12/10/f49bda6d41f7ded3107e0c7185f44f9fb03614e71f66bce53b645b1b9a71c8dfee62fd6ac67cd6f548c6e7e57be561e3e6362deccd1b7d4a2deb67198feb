+Telangana!5e0!3m2!1sen!2sin!4v1555583258581!5m2!1sen!2sin\\\"\\r\\n                                                            width=\\\"100%\\\"\\r\\n                                                            height=\\\"450\\\"\\r\\n                                                            frameBorder=\\\"0\\\"\\r\\n                                                            style={{\\r\\n                                                            border: \\\"0\\\"\\r\\n                                                        }}\\r\\n                                                            allowFullScreen></iframe>\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12 m-t-28\\\">\\r\\n                                                        <div className=\\\"row m-b-20\\\">\\r\\n                                                            <span className=\\\"col-md-6\\\">\\r\\n                                                                <label>Latitude</label>\\r\\n                                                                <input type=\\\"text\\\" className=\\\"form-control\\\" placeholder=\\\"\\\"  name=\\\"latitude\\\"\\r\\n                                                            onChange={this.onChange}\\r\\n                                                            value={this.state.latitude}/></span>\\r\\n                                                            <span className=\\\"col-md-6\\\">\\r\\n                                                                <label>Longitude</label>\\r\\n                                                                <input type=\\\"text\\\" className=\\\"form-control\\\" placeholder=\\\"\\\"  name=\\\"longitude\\\"\\r\\n                                                            onChange={this.onChange}\\r\\n                                                            value={t