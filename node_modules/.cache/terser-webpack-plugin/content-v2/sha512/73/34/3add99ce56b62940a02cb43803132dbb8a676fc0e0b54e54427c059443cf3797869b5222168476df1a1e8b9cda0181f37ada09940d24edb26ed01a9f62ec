  sessionStorage.setItem(\\\"RoleId\\\", user.uid);\\r\\n          var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n\\r\\n          var cartDataId = localStorage.getItem('cartOrdersData');\\r\\n\\r\\n          firebase\\r\\n              .database()\\r\\n              .ref(\\\"orders/\\\")\\r\\n              .orderByChild('cartDataId')\\r\\n              .equalTo(cartDataId)\\r\\n              .once('value', function (snapshot) {\\r\\n                  snapshot\\r\\n                      .forEach(function (child) {\\r\\n                          child\\r\\n                              .ref\\r\\n                              .update({sessionId: sessionId, cartDataId: \\\"\\\"});\\r\\n                      });\\r\\n              });\\r\\n     \\r\\n             \\r\\n            \\r\\n        })\\r\\n        .catch(error => {\\r\\n            this.setState({error});\\r\\n            console.log(this.state.error);\\r\\n            this.setState({employer_sevice_message: this.state.error.message});\\r\\n        });\\r\\n    } else {\\r\\n        this.validator.showMessages();\\r\\n        this.forceUpdate();\\r\\n      }\\r\\n\\r\\n};\\r\\nonChange = (event) => {\\r\\n    this.setState({\\r\\n        [event.target.name]: event.target.value\\r\\n    });\\r\\n};\\r\\n\\r\\n  render() {\\r\\n    const { email, password, error } = this.state;\\r\\n  \\r\\n    return (\\r\\n      <div>\\r\\n        <div className=\\\"color-line\\\"></div>\\r\\n\\r\\n    <div className=\\\"container-fluid\\\">\\r\\n     <Form\\r\\n      onSubmit={this.handleSubmit}\\r\\n        id=\\\"login-form\\\"\\r\\n      >\\r\\n        <div className=\\\"row\\\">\\r\\n            <div className=\\\"col-lg-4 col-md-4 col-sm-4 col-xs-12\\\"></div>\\r\\n            <div className=\\\"col-md-4 col-md-4 col-sm-4 col-xs-12\\\">\\r\\n                <div className=\\\"text-center m-b-md custom-login m-t-80 m-b-20\\\">\\r\\n               <h1><b>Shopality -Sign Up</b></h1>\\r\\n                </di