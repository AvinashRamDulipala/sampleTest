    sessionId: sessionId,\\r\\n\\r\\n\\r\\n               \\r\\n                 loading: false\\r\\n                });\\r\\n            console.log(this.state.SalesTracker);\\r\\n\\r\\n            this.employeeSListView();\\r\\n        });\\r\\n\\r\\n    }\\r\\n\\r\\n    employeeSListView() {\\r\\n\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref('Employees/');\\r\\n        ref.once(\\\"value\\\", snapshot => {\\r\\n            const data = [];\\r\\n            console.log(snapshot.val());\\r\\n            snapshot.forEach(element => {\\r\\n                const usersData = {\\r\\n\\r\\n                    value: element\\r\\n                        .val()\\r\\n                        .email,\\r\\n                    label: element\\r\\n                        .val()\\r\\n                        .first_name + \\\" \\\" + element\\r\\n                        .val()\\r\\n                        .last_name + \\\"(\\\" + element\\r\\n                        .val()\\r\\n                        .email + \\\")\\\"\\r\\n                }\\r\\n                data.unshift(usersData);\\r\\n            });\\r\\n\\r\\n            this.setState({\\r\\n                EmployeesList: data\\r\\n            }, () => {\\r\\n                console.log(this.state.EmployeesList, 'Employees');\\r\\n\\r\\n            });\\r\\n\\r\\n        })\\r\\n    }\\r\\n    handleChange = (event) => {\\r\\n        console.log(this.state.validError);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n\\r\\n       \\r\\n\\r\\n    };\\r\\n\\r\\n   \\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid() && this.state.validError == true) {\\r\\n            const {salestrackerId} = this.props.match.params;\\r\\n            var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n            var userName = sessionStorage.getItem(\\\"email\\\");\\r\\n            let dbCon = firebase\\r\\n                .database()\\r\\n                .ref(`/SalesTracker/${salestrackerId}`);\\r\\n            console.log(dbCon);\\r\\n            dbCon.update({\\r\\n                prospect_Name:this.state.prospect_Name,\\r\\n            company_Name:this.state.company_Name,\\r\\n            city:this.state.city,\\r\\n            contact_Number:this.state.contact_Number,\\r\\n            email:this.state.email,\\r\\n            source_Of_Enquiry:this.state.source_Of_Enquiry,\\r\\n            contacted:this.state.contacted,\\r\\n            email_Sent:this.state.email_Sent,\\r\\n            follow_Up_Date:this.state.follow_Up_Date,\\r\\n            follow_Up_Time:this.state.follow_Up_Time,\\r\\n            remarks:this.state.remarks,\\r\\n           created_on:this.state.created_on,\\r\\n            sessionId: sessionId,\\r\\n            userName:userName,\\r\\n            });\\r\\n\\r\\n            this\\r\\n                .props\\r\\n                .history\\r\\n                .push(\\\"/SalesTracker/Updated\\\");\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n        const {EmployeesList, selectedOption} = this.state;\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '40px',\\r\\n            'resize':'none',\\r\\n        };\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Locations</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit Location</h4>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                            <Form onSubmit={this.handleSubmit} id=\\\"Add-products\\\">\\r\\n\\r\\n<div className=\\\"row m-b-20 class-row\\\">\\r\\n    <div className=\\\"col-lg-2 col-md-2 col-sm-2 col-xs-12\\\">\\r\\n        <label>Prospect Name <span style={{color: 'red'}}>*</span>\\r\\n        </label>\\r\\n        </div>\\r\\n        