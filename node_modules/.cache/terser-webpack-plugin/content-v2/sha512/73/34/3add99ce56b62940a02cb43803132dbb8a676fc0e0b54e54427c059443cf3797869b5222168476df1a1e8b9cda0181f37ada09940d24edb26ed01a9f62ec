) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n    componentDidMount() {\\r\\n        this.setState({ loading: true });\\r\\n        var ref = firebase.database().ref(\\\"category/\\\");\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n          \\r\\n    \\r\\n            snapshot.forEach(childSnapShot => {\\r\\n               \\r\\n              const Categories = {\\r\\n                categoryId: childSnapShot.key.toString(),\\r\\n                category: childSnapShot.val().category,\\r\\n                created_on: childSnapShot.val().created_on,\\r\\n              };\\r\\n      \\r\\n              data.push(Categories);\\r\\n            });\\r\\n      \\r\\n            this.setState({ categoryList:data,loading: false });\\r\\n            console.log(this.state.categoryList);\\r\\n          }); }\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n    addNotification() {\\r\\n        this.notificationDOMRef.current.addNotification({\\r\\n          title: \\\"Success\\\",\\r\\n          message: \\\"Succesfully added!\\\",\\r\\n          type: \\\"success\\\",\\r\\n          insert: \\\"bottom\\\",\\r\\n          container: \\\"top-center\\\",\\r\\n          animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n          animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n          dismiss: { duration: 2000 },\\r\\n          dismissable: { click: true }\\r\\n        });\\r\\n      }\\r\\n    handleUploadStart = () => this.setState({isUploading: true, progress: 0});\\r\\n    handleProgress = progress => this.setState({progress});\\r\\n    handleUploadError = error => {\\r\\n        this.setState({isUploading: false});\\r\\n        console.error(error);\\r\\n    };\\r\\n    handleUploadSuccess = filename => {\\r\\n        this.setState({avatar: filename, progress: 100, isUplo