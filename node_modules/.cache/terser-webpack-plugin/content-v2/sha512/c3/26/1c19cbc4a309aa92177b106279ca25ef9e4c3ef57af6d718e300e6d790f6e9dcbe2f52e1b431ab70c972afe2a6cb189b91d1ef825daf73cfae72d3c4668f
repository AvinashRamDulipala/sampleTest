   rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        const {locationId} = this.props.match.params;\\r\\n        console.log(locationId);\\r\\n        var sessionId = sessionStorage.getItem(\\\"RoleId\\\"); \\r\\n        var userName = sessionStorage.getItem(\\\"user_name\\\");\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`Locations/${locationId}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var categories = snapshot.val();\\r\\n\\r\\n             console.log(categories);\\r\\n\\r\\n            this.setState({\\r\\n                location_Name: categories.location_Name,\\r\\n                addressl: categories.addressl,\\r\\n                address2: categories.address2,\\r\\n                land_Mark: categories.land_Mark,\\r\\n                city: categories.city,\\r\\n                pincode: categories.pincode,\\r\\n                contact_Person_Name: categories.contact_Person_Name,\\r\\n                contact_Number: categories.contact_Number,\\r\\n                email: categories.email,\\r\\n                rental_Deposit_Amount: categories.rental_Deposit_Amount,\\r\\n                rent_Per_Month: categories.rent_Per_Month,\\r\\n                electricity_Per_Month: categories.electricity_Per_Month,\\r\\n                wifi_Per_Month: categories.wifi_Per_Month,\\r\\n                additional_Expenses: categories.additional_Expenses,\\r\\n                notes: categories.notes,\\r\\n                userName:userName,\\r\\n                downloadURLs: categories.uploadImage,\\r\\n                created_on: categories.created_on, \\r\\n                sessionId: sessionId,\\r\\n\\r\\n                permission_type: categories.permission_type,\\r\\n                selectedOption: categories.specifEmpName,\\r\\n                locality: categories.locality,\\r\\n            landMark: categories.land_mark,\\r\\n            fullAddress: categories.full_address,\\r\\n            latitude: categories.latitude,\\r\\n            longitude: categories.longitude,\\r\\n\\r\\n          \\r\\n\\r\\n\\r\\n               \\r\\n                 loading: false\\r\\n                });\\r\\n            console.log(this.state.latitude);\\r\\n            console.log(this.state.longitude);\\r\\n\\r\\n            this.employeeSListView();\\r\\n        });\\r\\n\\r\\n    }\\r\\n\\r\\n    employeeSListView() {\\r\\n\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref('Employees/');\\r\\n        ref.once(\\\"value\\\", snapshot => {\\r\\n            const data = [];\\r\\n            console.log(snapshot.val());\\r\\n            snapshot.forEach(element => {\\r\\n                const usersData = {\\r\\n\\r\\n                    value: element\\r\\n                        .val()\\r\\n                        .email,\\r\\n                    label: element\\r\\n                        .val()\\r\\n                        .first_name + \\\" \\\" + element\\r\\n                        .val()\\r\\n                        .last_name + \\\"(\\\" + element\\r\\n                        .val()\\r\\n                        .email + \\\")\\\"\\r\\n                }\\r\\n                data.unshift(usersData);\\r\\n            });\\r\\n\\r\\n            this.setState({\\r\\n                EmployeesList: data\\r\\n            }, () => {\\r\\n                console.log(this.state.EmployeesList, 'Employees');\\r\\n\\r\\n            });\\r\\n\\r\\n        })\\r\\n    }\\r\\n    handleChange = (event) => {\\r\\n        console.log(this.state.validError);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n\\r\\n       \\r\\n\\r\\n    };\\r\\n\\r\\n    handleUploadStart = () => {\\r\\n\\r\\n        this.setState({isUploading: true, uploadProgress: 0});\\r\\n\\r\\n    }\\r\\n    handleFrontImageUploadStart = () => this.setState({isUploading: true, uploadProgress: 0, avatarURL: ''});\\r\\n    handleProgress = progress => this.setState({uploadProgress: progress});\\r\\n\\r\\n    handleUploadError = error => {\\r\\n        this.setState({\\r\\n            isUploading: false\\r\\n            // Todo: handle error\\r\\n        });\\r\\n        console.error(error);\\r\\n    };\\r\\n\\r\\n    handleUploadSuccess = async filename => {\\r\\n        const downloadURL = await firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL();\\r\\n\\r\\n        this.setState(oldState => ({\\r\\n            filenames: [\\r\\n                ...oldState.filenames,\\r\\n                filename\\r\\n            ],\\r\\n            downloadURLs: [\\r\\n                ...oldState.downloadURLs,\\r\\n                downloadURL\\r\\n            ],\\r\\n            uploadProgress: 100,\\r\\n            isUploading: false\\r\\n        }));\\r\\n\\t};\\r\\n\\r\\n  handleSelect = selectedOption => {\\r\\n    this.setState({selectedOption});\\r\\n    console.log(`Option selected:`, selectedOption);\\r\\n};\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid() && this.state.validError == true) {\\r\\n            const {locationId} = this.props.match.params;\\r\\n            var sessionId = sessionStorage.getItem(\\\"RoleId\\\"); \\r\\n            let dbCon = firebase\\r\\n                .database()\\r\\n                .ref(`/Locations/${locationId}`);\\r\\n            console.log(dbCon);\\r\\n            dbCon.update({\\r\\n                location_Name:this.state.location_Name,\\r\\n                addressl:this.state.addressl,\\r\\n                address2:this.state.address2,\\r\\n                land_Mark:this.state.land_Mark,\\r\\n                city: this.state.city,\\r\\n                pincode: this.state.pincode,\\r\\n                contact_Person_Name:this.state.contact_Person_Name,\\r\\n                contact_Number: this.state.contact_Number,\\r\\n                email:this.state.email,\\r\\n                rental_Deposit_Amount:this.state.rental_Deposit_Amount,\\r\\n                rent_Per_Month:this.state.rent_Per_Month,\\r\\n                electricity_Per_Month:this.state.electricity_Per_Month,\\r\\n                wifi_Per_Month:this.state.wifi_Per_Month,\\r\\n                additional_Expenses:this.state.additional_Expenses,\\r\\n                notes:this.state.notes,\\r\\n                // photo: this.state.avatarURL,\\r\\n                uploadImage: this.state.downloadURLs,\\r\\n                created_on: this.state.created_on, \\r\\n                sessionId: sessionId,\\r\\n                permission_type: this.state.permission_type,\\r\\n                specifEmpName: this.state.selectedOption,\\r\\n                locality: this.props.posts.locality,\\r\\n            landMark: this.props.posts.land_mark,\\r\\n            fullAddress: this.props.posts.full_address,\\r\\n            latitude: this.props.posts.latitude,\\r\\n            longitude: this.props.posts.longitude,\\r\\n            });\\r\\n\\r\\n            this\\r\\n                .props\\r\\n                .history\\r\\n                .push(\\\"/Locations/Updated\\\");\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n        const {EmployeesList, selectedOption} = this.state;\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '40px',\\r\\n            'resize':'none',\\r\\n        };\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Locations</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit Location</h4>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                            <Form onSubmit={this.handleSubmit} id=\\\"Add-products\\\">\\r\\n\\r\\n<div className=\\\"row m-b-20 class-row\\\">\\r\\n    <div className=\\\"col-lg-2 col-md-2 col-sm-2 col-xs-12\\\">\\r\\n        <label>Location Name <span style={{color: 'red'}}>*</span>\\r\\n        </label>\\r\\n        </div>\\r\\n        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n             <input\\r\\n            type=\\\"text\\\"\\r\\n            className=\\\"form-control\\\"\\r\\n            placeholder=\\\"Enter Location Name\\\"\\r\\n            name=\\\"location_Name\\\"\\r\\n            onChange={this.handleChange}\\r\\n            value={