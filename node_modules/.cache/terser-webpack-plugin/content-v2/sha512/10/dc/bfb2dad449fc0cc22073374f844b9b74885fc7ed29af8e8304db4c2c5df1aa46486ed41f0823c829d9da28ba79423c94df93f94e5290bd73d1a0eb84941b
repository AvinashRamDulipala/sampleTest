('value', snapshot => {\\r\\n            const data = [];\\r\\n            // console.log(snapshot); console.log(snapshot.val);\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const Categories = {\\r\\n                    locationId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                   \\r\\n                    department: childSnapShot\\r\\n                        .val()\\r\\n                        .department,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.unshift(Categories);\\r\\n            });\\r\\n\\r\\n            this.setState({departmentList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.departmentList);\\r\\n            // this.$el = $(this.el);\\r\\n\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destroy(true);\\r\\n        clearTimeout(this.interval);\\r\\n    }\\r\\n    deleteItem = id => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(id);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/departments/${id}`);\\r\\n                playersRef.remove();\\r\\n\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/DepartmentList/Deleted\\\");\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n    