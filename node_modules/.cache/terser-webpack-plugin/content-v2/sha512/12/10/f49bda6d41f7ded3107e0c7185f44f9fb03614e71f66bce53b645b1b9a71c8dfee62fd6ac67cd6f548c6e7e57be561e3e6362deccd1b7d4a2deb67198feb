pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"category\\\"\\r\\n                                                                onChange={this.setCategoryName}>\\r\\n                                                                <option>Select Category</option>\\r\\n                                                                {this\\r\\n                                                                    .state\\r\\n                                                                    .categoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.category} key={index}>{data.category}</option>\\r\\n                                                                        )\\r\\n                                                                        {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select category\\\", data.category, \\\"required\\\")\\r\\n                                                                        }\\r\\n\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n\\r\\n                            \\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Select Sub Category</label>\\r\\n                                                            <select\\r\\n                                                              \\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"subCategory\\\"\\r\\n                                                                onChange={this.onChange}>\\r\\n                                                                <option>Select Sub Category</option>\\r\\n                                                                {this.state.subcategoryList && this\\r\\n                                                                    .state\\r\\n                                                                    .subcategoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.subCategory} key={index}>{data.subCategory}</option>\\r\\n                                                                        )\\r\\n                                                                        {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select Sub category\\\", data.subCategory, \\\"required\\\")\\r\\n                                                                        }\\r\\n\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>GST</label>\\r\\n\\r\\n                                                            <select\\r\\n                                                                name=\\\"select\\\"\\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"gstValue\\\"\\r\\n                                                                onChange={this.onChange}>\\r\\n                                                                <option>Select GST</option>\\r\\n                                                                {this.state.GSTList && this\\r\\n                                                                    .state\\r\\n                                                                    .GSTList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.gst} key={index}>{data.gst}</option>\\r\\n                                                                        )\\r\\n                                                                        {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select GST\\\", data.gst, \\\"required\\\")\\r\\n                                                                        }\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                   \\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Price</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"price\\\"\\r\\n                                                                id=\\\"price_cost\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.price}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Price Value\\\", this.state.price, \\\"required|numeric|min:1|max:5\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Selling Price</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"sellingPrice\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.sellingPrice}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\" Selling Price\\\", this.state.sellingPrice, \\\"required|priceCostValid|numeric|min:1|max:5\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n                                                      \\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                   \\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Pin Code</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"pincode\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pincode}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Pin Code\\\", this.state.pincode, \\\"required|numeric|min:5|max:6\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Quantity Instock</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"quantityStock\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                id=\\\"quantity_stock\\\"\\r\\n                                                                value={this.state.quantityStock}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\" Quantity Price\\\", this.state.quantityStock, \\\"required|numeric|min:1|max:5\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Packaging Cost</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"packagingCost\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.packagingCost}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Packaging Cost\\\", this.state.packagingCost, \\\"required|numeric|min:1|max:5\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Alert Quantity</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"alertQuantity\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.alertQuantity}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Alert Quantity\\\", this.state.alertQuantity, \\\"required|AlertquantityStock|numeric\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <div className=\\\"dz-message needsclick download-custom\\\">\\r\\n                                                                <i className=\\\"fa fa-cloud-download\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                <h2>Front Image Upload</h2>\\r\\n\\r\\n                                                            </div>\\r\\n                                                            {this.state.isUploading && <p>Progress: {this.state.progress}</p>}\\r\\n                                                            {/* {this.state.avatarURL && <img src={this.state.avatarURL} />} */}\\r\\n                                                            {this.state.avatarURL && <img\\r\\n                                                                src={this.state.avatarURL}\\r\\n                                                                style={{\\r\\n                                                                height: \\\"200px\\\",\\r\\n                                                                width: \\\"200px\\\"\\r\\n                                                            }}/>}\\r\\n                                                            <FileUploader\\r\\n                                                                accept=\\\"image/*\\\"\\r\\n                                                                name=\\\"avatar\\\"\\r\\n  