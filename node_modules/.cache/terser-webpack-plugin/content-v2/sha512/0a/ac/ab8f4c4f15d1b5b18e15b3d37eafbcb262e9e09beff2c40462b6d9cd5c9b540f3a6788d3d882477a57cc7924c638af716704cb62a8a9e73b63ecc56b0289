                         return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.name}</td>\\r\\n                                         <td>{data.email}</td>\\r\\n                                         <td>{data.subject}</td>\\r\\n                                         <td>{data.message}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/viewEnquiries/${data.cmsId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"View\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-eye\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.cmsId}  onClick={this.deleteItem.bind(this, data.cmsId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    \\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default websiteEnquiries;\\r\\