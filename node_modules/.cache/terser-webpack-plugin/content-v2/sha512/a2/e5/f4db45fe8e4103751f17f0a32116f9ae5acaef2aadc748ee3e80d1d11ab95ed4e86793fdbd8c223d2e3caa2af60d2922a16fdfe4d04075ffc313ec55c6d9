                                        .message(\\\"Upload Product Image\\\", this.state.ogurl, \\\"required\\\")}\\r\\n                                                                    <span\\r\\n                                                                        style={{\\r\\n                                                                        color: 'red'\\r\\n                                                                    }}/>\\r\\n                                                                </div>\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"row\\\">\\r\\n                                                            <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                                <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n                                                                    <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Save\\r\\n                                                                    </button>\\r\\n\\r\\n                                                                </div>\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n                                                    </form>\\r\\n\\r\\n                                                </div>\\r\\n\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\n\\r\\nexport default AddSEOPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\n\\r\\nclass SEOPages extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        gstList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"SEOPages/\\\");\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const GSTData = {\\r\\n            cmsId: childSnapShot.key.toString(),\\r\\n            pageTitle: childSnapShot.val().pageTitle,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data,countPage: data.length,loading: false });\\r\\n        console.log(this.state.gstList);\\r\\n      });\\r\\n}\\r\\n    deleteItem = id => {\\r\\n        \\r\\n\\r\\n    var playersRef = firebase.database().ref(`/SEOPages/${id}`);\\r\\n    playersRef.remove();\\r\\n    this\\r\\n    .props\\r\\n    .history\\r\\n    .push(\\\"/SEOPages/Deleted\\\");\\r\\n    };\\r\\n\\r\\n   \\r\\n\\r\\n\\r\\n  render() {\\r\\n    \\r\\n    var pathname = this\\r\\n    .props\\r\\n    .location\\r\\n    .pathname\\r\\n    .split(\\\"/\\\");\\r\\nvar alertName = pathname[2];\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=