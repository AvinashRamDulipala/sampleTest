                Coupons {alertName}\\r\\n                                                    Successfully\\r\\n                                                </div>\\r\\n                                            : ''}\\r\\n                                        <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                                            <thead>\\r\\n                                                <tr>\\r\\n                                                    <th>S.No</th>\\r\\n                                                    <th>Coupon Code</th>\\r\\n                                                    <th>Coupon Type</th>\\r\\n                                                    <th>Usage</th>\\r\\n                                                    <th>Created on</th>\\r\\n                                                    {this.state.usePermList == \\\"read&write\\\"\\r\\n                                                        ? <th>Action</th>\\r\\n                                                        : \\\"\\\"}\\r\\n                                                </tr>\\r\\n                                            </thead>\\r\\n                                            <tbody>\\r\\n\\r\\n                                                {this.state.gstList && this\\r\\n                                                    .state\\r\\n                                                    .gstList\\r\\n                                                    .map((data, index) => {\\r\\n                                                        console.log(data);\\r\\n\\r\\n                                                        return (\\r\\n                                                            <tr key={index}>\\r\\n                                                                <td>{index + 1}</td>\\r\\n                                                                <td>{data.couponCode}</td>\\r\\n                                                                <td>{data.type}</td>\\r\\n                                                                <td>{data.usageType}</td>\\r\\n                                                                <td>{data.created_on}\\r\\n                                                                </td>\\r\\n                                                                {this.state.usePermList == \\\"read&write\\\"\\r\\n                                                                    ? <td>\\r\\n\\r\\n                                                                            <Link to={`/EditCoupon/${data.cmsId}`}>\\r\\n                                                                                <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\">\\r\\n                                                                                    <i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                                </button>\\r\\n                                                                            </Link>\\r\\n\\r\\n                                                                            <button\\r\\n                                                                                data-toggle=\\\"tooltip\\\"\\r\\n                                                                                title=\\\"Trash\\\"\\r\\n                                                                                className=\\\"pd-setting-ed\\\"\\r\\n                                                                                data-id={data.cmsId}\\r\\n                                                                                onClick={this\\r\\n                                                                                .deleteItem\\r\\n                                                                                .bind(this, data.cms