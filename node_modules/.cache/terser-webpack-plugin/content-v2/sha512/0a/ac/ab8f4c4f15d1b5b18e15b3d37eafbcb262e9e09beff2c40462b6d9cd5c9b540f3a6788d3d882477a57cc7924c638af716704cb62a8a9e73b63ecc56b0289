     <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\nexport default EditCoupon;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport swal from 'sweetalert';\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport Pagination from \\\"react-js-pagination\\\";\\r\\n\\r\\nimport $ from 'jquery';\\r\\n$.DataTable = require('datatables.net');\\r\\nclass MerchantAccount extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            gstList: [],\\r\\n            loading: false,\\r\\n            count: 1,\\r\\n            activePage: \\\"1\\\",\\r\\n            countPage: \\\"\\\"\\r\\n        };\\r\\n        this.deleteItem = this\\r\\n            .deleteItem\\r\\n            .bind(this);\\r\\n\\r\\n    }\\r\\n\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"admin_users/\\\")\\r\\n            .orderByChild(\\\"Role\\\")\\r\\n            .equalTo(\\\"Merchant\\\");\\r\\n        var refOrder = firebase\\r\\n            .database()\\r\\n            .ref(\\\"orders/\\\");\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n                    cmsId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    fullName: childSnapShot\\r\\n                        .val()\\r\\n                        .fullName,\\r\\n                    email: childSnapShot\\r\\n                        .val()\\r\\n                        .email,\\r\\n                    username: childSnapShot\\r\\n                        .val()\\r\\n                        .username,\\r\\n                    password: childSnapShot\\r\\n                        .val()\\r\\n                        .password,\\r\\n                    status: childSnapShot\\r\\n                        .val()\\r\\n                        .status,\\r\\n                    createdAt: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n                refOrder\\r\\n                    .orderByChild(\\\"merchantId\\\")\\r\\n                    .equalTo(childSnapShot.key.toString())\\r\\n                    .on('value', snap => {\\r\\n                        GSTData.orders = snap.numChildren();\\r\\n                    });\\r\\n                data.unshift(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({gstList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.gstList);\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n\\r\\n        clearTimeout(this.interval);\\r\\n    }\\r\\n    deleteItem = uid => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(uid);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/admin_users/${uid}`);\\r\\n                playersRef.remove();\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/MerchantAccount/Deleted\\\");\\r\\n\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n    }\\r\\n\\r\\n    onChange = (event) => {\\r\\n\\r\\n        var cmsId = event.target.id;\\r\\n        console.log(cmsId);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n        console.log(event.target.value);\\r\\n\\r\\n        let dbCon = firebase\\r\\n            .database()\\r\\n            .ref(`/admin_users/${cmsId}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update({status: event.target.value});\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        var pathname = this\\r\\n            .props\\r\\n            .location\\r\\n            .pathname\\r\\n            .split(\\\"/\\\");\\r\\n        var alertName = pathname[2];\\r\\n        console.log(alertName);\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Admin Merchant</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"product-status mg-b-30\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"product-status-wrap\\\">\\r\\n                                        <h4>Merchant Account</h4>\\r\\n                                        <div className=\\\"add-product\\\">\\r\\n                                            <Link to='/AddMerchantAccount'>Add Merchant Account</Link>\\r\\n                                        </div>\\r\\n                                        {alertName\\r\\n                                            ? <div\\r\\n                                                    className=\\\"alert alert-success display-none\\\"\\r\\n                                                    style={{\\r\\n                                                    display: \\\"block\\\"\\r\\n                                                }}>\\r\\n                                                    <a data-dismiss=\\\"alert\\\" href=\\\"#\\\" aria-hidden=\\\"true\\\" className=\\\"close\\\"></a>\\r\\n                                                    Merchant Account {alertName}\\r\\n                                                    Successfully\\r\\n                                                </div>\\r\\n                                            : ''}\\r\\n                                        <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                                            <thead>\\r\\n                                                <tr>\\r\\n                                                    <th>S.No</th>\\r\\n                        