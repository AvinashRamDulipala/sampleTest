 '../config';\\r\\nimport Pagination from \\\"react-js-pagination\\\";\\r\\n\\r\\n\\r\\n\\r\\nclass ServiceProviderListPage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        serviceProviderList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"serviceProvider/\\\");\\r\\n\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        // console.log(snapshot);\\r\\n        // console.log(snapshot.val);\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const ServiceData = {\\r\\n            serviceId: childSnapShot.key.toString(),\\r\\n            serviceProviderName: childSnapShot.val().serviceProviderName,\\r\\n            trackingURL: childSnapShot.val().trackingURL,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(ServiceData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ serviceProviderList:data, countPage: data.length,loading: false });\\r\\n        console.log(this.state.serviceProviderList);\\r\\n      });\\r\\n}\\r\\n    deleteItem = id => {\\r\\n        \\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase.database().ref(`/serviceProvider/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n    };\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>service Providers</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Service Provider List</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddServicePage'>Add Service Provider</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Name</th>\\r\\n                                    <th>Tracking URL</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.serviceProviderList && this.state.serviceProviderList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.serviceProviderName}</td>\\r\\n                                         <td>{data.trackingURL}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditServicePage/${data.serviceId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.serviceId}  onClick={this.deleteItem.bind(this, data.serviceId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    {/* <div className=\\\"custom-pagination\\\">\\r\\n                            <Pagination\\r\\n                          hideNavigation\\r\\n                          activePage={this.state.activePage}\\r\\n                          itemsCountPerPage={3}\\r\\n                          totalItemsCount={this.state.countPage}\\r\\n                          pageRangeDisplayed={3}\\r\\n                          onChange={this.handlePageChange.bind(this)}\\r\\n                        />\\r\\n                            </div> */}\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default ServiceProviderListPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Form } from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\n\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\n\\r\\n\\r\\nclass EditServicePage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        serviceProviderName:'',\\r\\n        trackingURL:'',\\r\\n        loading: false,\\r\\n        date:new Date().toLocaleString(),\\r\\n\\r\\n    }\\r\\n   \\r\\n    this.validator = new SimpleReactValidator({\\r\\n      className: \\\"text-danger\\\",\\r\\n      validators: {\\r\\n\\r\\n          whitespace: {\\r\\n              message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          specialChar: {\\r\\n              message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          website: {\\r\\n            message: \\\"The Url should be example.com \\\",\\r\\n            rule: function(val, params, validator) {\\r\\n              //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n              return (\\r\\n                validator.helpers.testRegex(\\r\\n                  val,\\r\\n                  /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g\\r\\n                ) && params.indexOf(val) === -1\\r\\n              );\\r\\n            }\\r\\n          },\\r\\n          specialCharText: {\\r\\n              message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          }\\r\\n      }\\r\\n  });\\r\\n\\r\\n  }\\r\\n  componentDidMount(){\\r\\n    this.setState({loading: true});\\r\\n      const {serviceID}=this.props.match.params;\\r\\n      console.log(serviceID);\\r\\n      \\r\\n      var ref = firebase.database().ref(`serviceProvider/${serviceID}`);\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            var services = snapshot.val();\\r\\n            this.setState({ serviceProviderName:services.serviceProviderName,\\r\\n                trackingURL:services.trackingURL,\\r\\n                 });\\r\\n            console.log(this.state.serviceProviderName);\\r\\n          });\\r\\n     \\r\\n  }\\r\\n \\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({ [event.target.name]: event.target.value });\\r\\n  };\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n    \\r\\n    if (this.validator.allValid()) {\\r\\n    const {serviceID}=this.props.match.params;\\r\\n        event.preventDefault();\\r\\n        let dbCon = firebase.database().ref(`serviceProvider/${serviceID}`);\\r\\n        dbCon.update ({\\r\\n            serviceProviderName: this.state.serviceProviderName,\\r\\n            trackingURL: this.state.trackingURL,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n        console.log(dbCon);\\r\\n\\r\\n      \\r\\n      this.setState({\\r\\n        serviceProviderName:'',\\r\\n        trackingURL:'',\\r\\n      });\\r\\n      this.props.history.push(\\\"/ServiceProviderListPage\\\");\\r\\n    } else {\\r\\n      this\\r\\n          .validator\\r\\n          .showMessages();\\r\\n      this.forceUpdate();\\r\\n    }\\r\\n    \\r\\n  };\\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\\"bottom\\\",\\r\\n      container: \\\"top-center\\\",\\r\\n      animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n      animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n      dismiss: { duration: 2000 },\\r\\n      dismissable: { click: true }\\r\\n    });\\r\\n  }\\r\\n  componentWillUnmount() {\\r\\n    this.setState({loading: false});\\r\\n  }\\r\\n  \\r\\n  \\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Merchants</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n          \\r\\n            <div className=\\\"single-pro-review-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"review-tab-pro-inner\\\">\\r\\n                   \\r\\n\\t\\t\\t\\t   <h4>Edit Service Provider</h4>\\r\\n\\t\\t\\t\\t   \\r\\n\\t\\t\\t\\t   \\r\\n                            \\r\\n                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                            <Form onSubmit={this.handleSubmit} id=\\\"Edit-service\\\">\\r\\n                            \\r\\n                            <div className=\\\"row m-b-20\\\">\\r\\n                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                         \\r\\n                            <input type=\\\"text\\\" className=\\\"form-control\\\" placeholder=\\\"Service Provider Name*\\\"  id=\\\"add_service\\\"\\r\\n                            name=\\\"serviceProviderName\\\" onChange={ this.onChange } value={ this.state.serviceProviderName } required=\\\"\\\"/>\\r\\n {this.validator.message(\\r\\n                  \\\"Service Provider Name\\\",\\r\\n                  this.state.serviceProviderName,\\r\\n                  \\\"required|specialCharText|whitespace|min:2|max:70\\\"\\r\\n                )}\\r\\n                          \\r\\n                            </div>\\r\\n                            </div>\\r\\n                            <div className=\\\"row m-b-20\\\">\\r\\n                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                         \\r\\n                         <input type=\\\"url\\\" className=\\\"form-control\\\" placeholder=\\\"trackingURL*\\\"  id=\\\"add_tracking\\\"\\r\\n                         name=\\\"trackingURL\\\" onChange={ this.onChange } value={ this.state.trackingURL } required=\\\"\\\" />\\r\\n            {this.validator.message(\\r\\n                  \\\"trackingURL\\\",\\r\\n                  this.state.trackingURL,\\r\\n                  \\\"website|min:3|max:255\\\"\\r\\n                )}             \\r\\n                       \\r\\n                         </div>\\r\\n\\r\\n                            </div>\\r\\n\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n                                                    <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Save</button>\\r\\n                                                 \\r\\n                                                </div>\\r\\n                                            </div>\\r\\n                                        </div>\\r\\n                                        </Form>\\r\\n                                    </div>\\r\\n                                 \\r\\n\\r\\n\\t\\t\\t\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\t\\t\\t\\t\\t\\t\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div>\\r\\n        </div>\\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default EditServicePage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Form } from 'reactstrap';\\r\\n\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\n\\r\\n\\r\\nclass AddServicePage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        serviceProviderName:'',\\r\\n        trackingURL:'',\\r\\n        date:new Date().toLocaleString(),\\r\\n\\r\\n    }\\r\\n    this.addNotification = this.addNotification.bind(this);\\r\\n        this.notificationDOMRef = React.createRef();\\r\\n    this.validator = new SimpleReactValidator({\\r\\n      className: \\\"text-danger\\\",\\r\\n      validators: {\\r\\n\\r\\n          whitespace: {\\r\\n              message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          specialChar: {\\r\\n              message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          website: {\\r\\n            message: \\\"The Url should be example.com \\\",\\r\\n            rule: function(val, params, validator) {\\r\\n              //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n              return (\\r\\n                validator.helpers.testRegex(\\r\\n                  val,\\r\\n                  /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g\\r\\n                ) && params.indexOf(val) === -1\\r\\n              );\\r\\n            }\\r\\n          },\\r\\n          specialCharText: {\\r\\n              message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          }\\r\\n      }\\r\\n  });\\r\\n\\r\\n  }\\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({ [event.target.name]: event.target.value });\\r\\n  };\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n    //   console.log(this.state.trackingURL);\\r\\n    event.preventDefault();\\r\\n\\r\\n    if (this.validator.allValid()) {\\r\\n    if (this.state.serviceProviderName && this.state.serviceProviderName.trim().length !== 0) {\\r\\n      this.addNotification();\\r\\n\\r\\n        let dbCon = firebase.database().ref('/serviceProvider');\\r\\n        dbCon.push({\\r\\n            serviceProviderName: this.state.serviceProviderName,\\r\\n            trackingURL: this.state.trackingURL,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n\\r\\n      \\r\\n      this.setState({\\r\\n        serviceProviderName:'',\\r\\n        trackingURL:'',\\r\\n      });\\r\\n      this.props.history.push(\\\"/ServiceProviderListPage\\\");\\r\\n    }\\r\\n  } else {\\r\\n    this\\r\\n        .validator\\r\\n        .showMessages();\\r\\n    this.forceUpdate();\\r\\n  }\\r\\n  };\\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\\"bottom\\\",\\r\\n      container: \\\"top-center\\\",\\r\\n      animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n      animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n      dismiss: { duration: 2000 },\\r\\n      dismissable: { click: true }\\r\\n    });\\r\\n  }\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Service Provider</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n          \\r\\n            <div className=\\\"single-pro-review-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div cla