                                               placeholder='Enter Text'\\r\\n                                                                onChange={this.handleChange}\\r\\n                                                                className=\\\"add-new-post__editor mb-1\\\"\\r\\n                                                                style={divStyle}/>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        {this\\r\\n                                                            .validator\\r\\n                                                            .message(\\\"Page Content\\\", this.state.content, \\\"required|min:150\\\")}\\r\\n                                                        <div className=\\\"text-danger\\\">\\r\\n                                                            {\\\" \\\"}\\r\\n                                                            {this.state.employer_sevice_message}\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                            <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n\\r\\n                                                                <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\