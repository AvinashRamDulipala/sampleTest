    value={this.state.mailingAddress}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Mailing Address\\\", this.state.mailingAddress, \\\"required|whitespace|specialChar|specialChar|min:2|max:70\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Pick Up Address</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"pickupAddress\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pickupAddress}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Pick up address\\\", this.state.pickupAddress, \\\"required|whitespace|specialChar|min:2|max:70\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <div className=\\\"dz-message n