                               .bind(this)}\\r\\n                                                                        selectedOptionsStyles={selectedOptionsStyles}\\r\\n                                                                        optionsListStyles={optionsListStyles}\\r\\n                                                                        isTextWrap={false}/>\\r\\n                                                                </div>\\r\\n\\r\\n                                                            </div>\\r\\n                                                        </div>:''}\\r\\n\\r\\n                                                   \\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <ReactQuill\\r\\n                                                                theme={this.state.theme}\\r\\n                                                                value={this.state.content}\\r\\n                                                                modules={this.modules}\\r\\n                                                                formats={this.formats}\\r\\n                                                                placeholder='Enter Text'\\r\\n                                                                onChange={this.handleChange}\\r\\n                                                                className=\\\"add-new-post__editor mb-1\\\"\\r\\n                                                                style={divStyle}/>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        {this\\r\\n                                                            .validator\\r\\n                                                            .message(\\\"Page Content\\\", this.state.content, \\\"required|max:50\\\")}\\r\\n                                                        <div className=\\\"text-danger\\\">\\r\\n                                                            {\\\" \\\"}\\r\\n                                                            {this.state.employer_sevice_message}\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                            <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n\\r\\n                                                                <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Add</button>\\r\\n                                                                <Link to=\\\"/Notifications\\\">\\r\\n                                                                    <button type=\\\"button\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Cancel\\r\\n                                                                    </button>\\r\\n                                                                </Link>\\r\\n\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                </Form>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\n\\r\\nexport default AddNotifications;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport Moment from 'moment';\\r\\n\\r\\nimport Pagination from \\\"react-js-pagination\\\";\\r\\n\\r\\n\\r\\n\\r\\n\\r\\nclass Notifications extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        gstList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"Notifications/\\\");\\r\\n\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        // console.log(snapshot);\\r\\n        // console.log(snapshot.val);\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const GSTData = {\\r\\n            cmsId: childSnapShot.key.toString(),\\r\\n            pageTitle: childSnapShot.val().pageTitle,\\r\\n            pageName: childSnapShot.val().pageName,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data,countPage: data.length,loading: false });\\r\\n        console.log(this.state.gstList);\\r\\n      });\\r\\n}\\r\\n    deleteItem = id => {\\r\\n        \\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase.database().ref(`/Notifications/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n    };\\r\\n\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Notifications</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Notifications</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddNotifications'>Add Notifications</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Sent On</th>\\r\\n                                    <th> title</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Sent To</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.gstList && this.state.gstList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{Moment(data.created_on).format('D MMM Y')}\\t</td>\\r\\n                                         <td>{data.pageName}</td>\\r\\n                                         <td>All</td>\\r\\n                                       \\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditNotification/${data.cmsId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.cmsId}  onClick={this.deleteItem.bind(this, data.cmsId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                   \\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default Notifications;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\n\\r\\nimport {Form} from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport ReactQuill from \\\"react-quill\\\";\\r\\nimport 'react-quill/dist/quill.snow.css';\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\nimport Select from \\\"react-select\\\";\\r\\nclass EditNotification extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            pageTitle: '',\\r\\n            pageName: '',\\r\\n            date: new Date().toLocaleString(),\\r\\n            content: '',\\r\\n            theme: 'snow',\\r\\n            notification_type:'',\\r\\n            merchantList: [],\\r\\n            merchants: '',\\r\\n            multiSelect: []\\r\\n\\r\\n        }\\r\\n        this.handleChange = this\\r\\n            .handleChange\\r\\n            .bind(this);\\r\\n        this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    modules = {\\r\\n        toolbar: [\\r\\n            [\\r\\n                {\\r\\n                    'header': [\\r\\n                        1,\\r\\n                        2,\\r\\n                        3,\\r\\n                        4,\\r\\n                        5,\\r\\n                        6,\\r\\n                        false\\r\\n                    ]\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'bold', 'italic', 'underline', 'strike', 'blockquote'\\r\\n            ],\\r\\n            [\\r\\n                {\\r\\n                    'list': 'ordered'\\r\\n                }, {\\r\\n                    'list': 'bullet'\\r\\n                }, {\\r\\n                    'indent': '-1'\\r\\n                }, {\\r\\n                    'indent': '+1'\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'link', 'image'\\r\\n            ],\\r\\n            ['clean'],\\r\\n            [\\r\\n                {\\r\\n                    'color': []\\r\\n                }, {\\r\\n                    'background': []\\r\\n                }\\r\\n            ]\\r\\n        ]\\r\\n    };\\r\\n\\r\\n    formats = [\\r\\n        'header',\\r\\n        'bold',\\r\\n        'italic',\\r\\n        'underline',\\r\\n        'strike',\\r\\n        'blockquote',\\r\\n        'list',\\r\\n        'bullet',\\r\\n        'indent',\\r\\n        'link',\\r\\n        'image',\\r\\n        'color',\\r\\n        'background'\\r\\n    ];\\r\\n    componentDidMount() {\\r\\n        const {CMSID} = this.props.match.params;\\r\\n        console.log(CMSID);\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`Notifications/${CMSID}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var gstData = snapshot.val();\\r\\n            console.log(gstData)\\r\\n            this.setState({pageTitle: gstData.pageTitle,\\r\\n                notification_type:gstData.notification_type, \\r\\n                pageName: gstData.pageName,\\r\\n                 content: gstData.content,\\r\\n                 multiSelect:gstData.merchantList,\\r\\n                  loading: false});\\r\\n            //console.log(this.state.pageTitle);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid()) {\\r\\n            const {CMSID} = this.props.match.params;\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0) {\\r\\n                event.preventDefault();\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref(`/Notifications/${CMSID}`);\\r\\n                console.log(dbCon);\\r\\n                dbCon.update({pageTitle: this.state.pageTitle,merchantList:this.state.multiSelect,notification_type:this.state.notification_type, pageName: this.state.pageName, content: this.state.content, created_on: this.state.date});\\r\\n\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Notifications\\\");\\r\\n\\r\\n            }\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n   \\r\\n    \\r\\n    \\r\\n      handleMultiChange = selectedOption => {\\r\\n        this.setState({ multiSelect: selectedOption});\\r\\n        console.log(`Option selected:`, selectedOption);\\r\\n      };\\r\\n    componentWillMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"admin_users/\\\")\\r\\n            .orderByChild(\\\"Role\\\")\\r\\n            .equalTo(\\\"Merchant\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n\\r\\n                    label: childSnapShot\\r\\n                    .val()\\r\\n                    .email,\\r\\n                    \\r\\n                    value: childSnapShot\\r\\n                .val()\\r\\n                .email,\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({merchantList: data});\\r\\n            console.log(this.state.merchantList);\\r\\n        });\\r\\n    }\\r\\n\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n        const selectedOptionsStyles = {\\r\\n            color: \\\"#3c763d\\\",\\r\\n            backgroundColor: \\\"#dff0d8\\\"\\r\\n        };\\r\\n        const optionsListStyles = {\\r\\n            backgroundColor: \\\"#dff0d8\\\",\\r\\n            color: \\\"#3c763d\\\"\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Settings</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit Notification Page</h4>\\r\\n\\r\\n                                            <ReactNotification ref={this.notificationDOMRef}/>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Edit-GST\\\">\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Page Title*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageTitle\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageTitle}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Title\\\", this.state.pageTitle, \\\"required|specialChar|min:3|max:70\\\")}\\r\\n                                                            <div className=\\\"text-danger\\\">\\r\\n                                                                {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-