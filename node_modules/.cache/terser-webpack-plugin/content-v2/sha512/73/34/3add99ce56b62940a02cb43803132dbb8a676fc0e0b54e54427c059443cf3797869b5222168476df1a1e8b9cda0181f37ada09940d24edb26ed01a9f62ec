        {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>Description<span style={{color: 'red' }}>*</span></label>\\r\\n                                                            <ReactQuill\\r\\n                                                                theme={this.state.theme}\\r\\n                                                                value={this.state.content}\\r\\n                                                                modules={this.modules}\\r\\n                                                                formats={this.formats}\\r\\n                                                                placeholder='Enter Text'\\r\\n                                                                onChange={this.handleChange}\\r\\n                                                                className=\\\"add-new-post__editor mb-1\\\"\\r\\n                                                                style={divStyle}/>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        {this\\r\\n                                                            .validator\\r\\n                                                            .message(\\\"Page Content\\\", this.state.content, \\\"required|min:150\\\")}\\r\\n                                                        <div className=\\\"text-danger\\\">\\r\\n                                                            {\\\" \\\"}\\r\\n                                                            {this.state.employer_sevice_message}\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                            <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n\\r\\n                                                                <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Add</button>\\r\\n                                                                <Link to=\\\"/CMSPages\\\">\\r\\n                                                                    <button type=\\\"button\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Cancel\\r\\n                                                                    </button>\\r\\n                                                                </Link>\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                </Form>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\nexport default EditCMSPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Form, Input, Button, Alert} from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport ReactQuill from \\\"react-quill\\\";\\r\\nimport 'react-quill/dist/quill.snow.css';\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\nimport MultiSelectReact from 'multi-select-react';\\r\\n\\r\\nclass AddNotifications extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n\\r\\n        this.state = {\\r\\n            pageTitle: '',\\r\\n            pageName: '',\\r\\n            date: new Date().toLocaleString(),\\r\\n            content: '',\\r\\n            theme: 'snow',\\r\\n            notification_type: '',\\r\\n            merchantList: [],\\r\\n            merchants: '',\\r\\n            multiSelect: []\\r\\n        }\\r\\n        this.handleChange = this\\r\\n            .handleChange\\r\\n            .bind(this);\\r\\n        this.addNotification = this\\r\\n            .addNotification\\r\\n            .bind(this);\\r\\n        this.notificationDOMRef = React.createRef();\\r\\n        this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    modules = {\\r\\n        toolbar: [\\r\\n            [\\r\\n                {\\r\\n                    'header': [\\r\\n                        1,\\r\\n                        2,\\r\\n                        3,\\r\\n                        4,\\r\\n                        5,\\r\\n                        6,\\r\\n                        false\\r\\n                    ]\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'bold', 'italic', 'underline', 'strike', 'blockquote'\\r\\n            ],\\r\\n            [\\r\\n                {\\r\\n                    'list': 'ordered'\\r\\n                }, {\\r\\n                    'list': 'bullet'\\r\\n                }, {\\r\\n                    'indent': '-1'\\r\\n                }, {\\r\\n                    'indent': '+1'\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'link', 'image'\\r\\n            ],\\r\\n            ['clean'],\\r\\n            [\\r\\n                {\\r\\n                    'color': []\\r\\n                }, {\\r\\n                    'background': []\\r\\n                }\\r\\n            ]\\r\\n        ]\\r\\n    };\\r\\n\\r\\n    formats = [\\r\\n        'header',\\r\\n        'bold',\\r\\n        'italic',\\r\\n        'underline',\\r\\n        'strike',\\r\\n        'blockquote',\\r\\n        'list',\\r\\n        'bullet',\\r\\n        'indent',\\r\\n        'link',\\r\\n        'image',\\r\\n        'color',\\r\\n        'background'\\r\\n    ];\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid()) {\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0 && this.state.content.trim().length !== 0) {\\r\\n                this.addNotification();\\r\\n                event.preventDefault();\\r\\n\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref('/Notifications');\\r\\n                dbCon.push({\\r\\n                    pageTitle: this.state.pageTitle,\\r\\n                     notification_type: this.state.notification_type,\\r\\n                      pageName: this.state.pageName, \\r\\n                    content: this.state.content,\\r\\n                    merchantList:this.state.multiSelect,\\r\\n                     created_on: this.state.date});\\r\\n\\r\\n                this.setState({pageTitle: '', content: '', pageName: ''});\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Notifications\\\");\\r\\n            }\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n    addNotification() {\\r\\n        this\\r\\n            .notificationDOMRef\\r\\n            .current\\r\\n            .addNotification({\\r\\n                title: \\\"Success\\\",\\r\\n                message: \\\"Succesfully added!\\\",\\r\\n                type: \\\"success\\\",\\r\\n                insert: \\\"bottom\\\",\\r\\n                container: \\\"top-center\\\",\\r\\n                animationIn: [\\r\\n                    \\\"animated\\\", \\\"fadeIn\\\"\\r\\n                ],\\r\\n                animationOut: [\\r\\n                    \\\"animated\\\", \\\"fadeOut\\\"\\r\\n                ],\\r\\n                dismiss: {\\r\\n                    duration: 2000\\r\\n                },\\r\\n                dismissable: {\\r\\n                    click: true\\r\\n                }\\r\\n            });\\r\\n    }\\r\\n    optionClicked(optionsList) {\\r\\n        this.setState({multiSelect: optionsList});\\r\\n        console.log(this.state.multiSelect);\\r\\n    }\\r\\n    selectedBadgeClicked(optionsList) {\\r\\n       console.log(optionsList);\\r\\n        var sortedKeys = optionsList.filter(res=>{ \\r\\n         console.log(res);\\r\\n            return res.value; });\\r\\n            console.log(sortedKeys);\\r\\n      \\r\\n        this.setState({ multiSelect: sortedKeys}, () => {\\r\\n            console.log(this.state.multiSelect, 'multiSelect');\\r\\n            });\\r\\n        \\r\\n    }\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"admin_users/\\\")\\r\\n            .orderByChild(\\\"Role\\\")\\r\\n            .equalTo(\\\"Merchant\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n\\r\\n                    label: childSnapShot\\r\\n                        .val()\\r\\n                        .email,\\r\\n                    id: childSnapShot\\r\\n                        .key\\r\\n                        .toString()\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({merchantList: data});\\r\\n            console.log(this.state.merchantList);\\r\\n        });\\r\\n    }\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n        const selectedOptionsStyles = {\\r\\n            color: \\\"#3c763d\\\",\\r\\n            backgroundColor: \\\"#dff0d8\\\"\\r\\n        };\\r\\n        const optionsListStyles = {\\r\\n            backgroundColor: \\\"#dff0d8\\\",\\r\\n            color: \\\"#3c763d\\\"\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\