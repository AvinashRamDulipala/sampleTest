-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport swal from 'sweetalert';\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\nimport $ from 'jquery';\\r\\n$.DataTable = require('datatables.net');\\r\\n\\r\\nclass AdminStaff extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            gstList: [],\\r\\n            loading: false,\\r\\n            count: 1,\\r\\n            activePage: \\\"1\\\",\\r\\n            countPage: \\\"\\\"\\r\\n        };\\r\\n        this.deleteItem = this\\r\\n            .deleteItem\\r\\n            .bind(this);\\r\\n\\r\\n    }\\r\\n\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"admin_users/\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n                    cmsId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    fullName: childSnapShot\\r\\n                        .val()\\r\\n                        .fullName,\\r\\n                    email: childSnapShot\\r\\n                        .val()\\r\\n                        .email,\\r\\n                    username: childSnapShot\\r\\n                        .val()\\r\\n                        .username,\\r\\n                    password: childSnapShot\\r\\n                        .val()\\r\\n                        .password,\\r\\n                    status: childSnapShot\\r\\n                        .val()\\r\\n                        .status,\\r\\n                    Role: childSnapShot\\r\\n                        .val()\\r\\n                        .Role,\\r\\n                    createdAt: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({gstList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.gstList);\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n    }\\r\\n    deleteItem = uid => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(uid);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/admin_users/${uid}`);\\r\\n                playersRef.remove();\\r\\n\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/AdminStaff/Deleted\\\");\\r\\n\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n    }\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({activePage: pageNumber});\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        var cmsId = event.target.id;\\r\\n        console.log(cmsId);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n        console.log(event.target.value);\\r\\n\\r\\n        let dbCon = firebase\\r\\n            .database()\\r\\n            .ref(`/admin_users/${cmsId}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update({status: event.target.value});\\r\\n    };\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destroy(true);\\r\\n        clearTimeout(this.interval);\\r\\n    }\\r\\n    render() {\