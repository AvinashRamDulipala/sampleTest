\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport FadeIn from \\\"react-fade-in\\\";\\r\\nimport axios from \\\"axios\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport swal from 'sweetalert';\\r\\n\\r\\n\\r\\nimport $ from 'jquery';\\r\\n$.DataTable = require( 'datatables.net' );\\r\\n\\r\\n\\r\\nclass ServiceProviderListPage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        serviceProviderList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\",\\r\\n        cols:'',\\r\\n        rows:''\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"serviceProvider/\\\");\\r\\n\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        // console.log(snapshot);\\r\\n        // console.log(snapshot.val);\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const ServiceData = {\\r\\n            serviceId: childSnapShot.key.toString(),\\r\\n            serviceProviderName: childSnapShot.val().serviceProviderName,\\r\\n            trackingURL: childSnapShot.val().trackingURL,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(ServiceData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ serviceProviderList:data, countPage: data.length,loading: false });\\r\\n        console.log(this.state.serviceProviderList);\\r\\n        this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n    });\\r\\n}\\r\\ncomponentWillUnmount() {\\r\\n    clearTimeout(this.interval);\\r\\n  }\\r\\n  deleteItem = id => {\\r\\n    \\r\\n    swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n      if (willDelete) {\\r\\n        console.log(id);\\r\\n        var playersRef = firebase.database().ref(`/serviceProvider/${id}`);\\r\\n        playersRef.remove();\\r\\n        \\r\\n  \\r\\n      } else {}\\r\\n     \\r\\n     \\r\\n  });\\r\\n  \\r\\n    };\\r\\n  \\r\\n \\r\\n  render() {\\r\\n   \\r\\n\\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>service Providers</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-statu