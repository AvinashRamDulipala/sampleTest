 Link } from \\\"react-router-dom\\\";\\r\\nimport FadeIn from \\\"react-fade-in\\\";\\r\\nimport axios from \\\"axios\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport Pagination from \\\"react-js-pagination\\\";\\r\\nimport swal from 'sweetalert';\\r\\n\\r\\n\\r\\n\\r\\n\\r\\nclass OrderListPage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        orderList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\",\\r\\n        orderStatusList:[],\\r\\n    };\\r\\n    this.deleteItem = this\\r\\n        .deleteItem\\r\\n        .bind(this);\\r\\n\\r\\n}\\r\\n\\r\\ncomponentDidMount() {\\r\\n    this.setState({loading: true});\\r\\n    var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n    var userRole = sessionStorage.getItem(\\\"userRole\\\");\\r\\nif(userRole==\\\"Merchant\\\"){\\r\\n    var ref = firebase\\r\\n    .database()\\r\\n    .ref(\\\"orders/\\\").orderByChild(\\\"merchantId\\\")\\r\\n    .equalTo(sessionId);\\r\\n}\\r\\n    else{\\r\\n        var ref = firebase\\r\\n        .database()\\r\\n        .ref(\\\"orders/\\\");\\r\\n\\r\\n    }\\r\\n   \\r\\n\\r\\n    ref.once('value', snapshot => {\\r\\n        const data = [];\\r\\n       console.log(snapshot.val());\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n\\r\\n            const Merchants = {\\r\\n                fireorderId: childSnapShot.key.toString(),\\r\\n                orderId: childSnapShot.val().orderId,\\r\\n                customerName: childSnapShot.val().first_name+childSnapShot.val().last_name,\\r\\n                items: childSnapShot.val().cartDataItems.length,\\r\\n                status: childSnapShot.val().status,\\r\\n                created_on: childSnapShot .val().created_on,\\r\\n                emailId:childSnapShot .val().email_id\\r\\n            };\\r\\n\\r\\n            data.push(Merchants);\\r\\n        });\\r\\n\\r\\n        this.setState({orderList: data, countPage: data.length, loading: false});\\r\\n        console.log(this.state.orderList);\\r\\n    });\\r\\n\\r\\n    var ref = firebase.database().ref(\\\"orderStatus\\\");\\r\\n        \\r\\n        \\r\\n            ref.on('value', snapshot => {\\r\\n                const data = [];\\r\\n              \\r\\n        \\r\\n                snapshot.forEach(childSnapShot => {\\r\\n                   \\r\\n                  const orderStatus = {\\r\\n                    categoryId: childSnapShot.key.toString(),\\r\\n                    orderStatus: childSnapShot.val().orderStatus,\\r\\n                   \\r\\n                  };\\r\\n          \\r\\n                  data.push(orderStatus);\\r\\n                });\\r\\n          \\r\\n                this.setState({ orderStatusList:data,loading: false });\\r\\n                console.log(this.state.orderStatusList);\\r\\n              });\\r\\n\\r\\n}\\r\\ndeleteItem = id => {\\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase\\r\\n        .database()\\r\\n        .ref(`/orders/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n};\\r\\nonChange = (event) => {\\r\\n\\r\\n   console.log(event.target.value);\\r\\n   console.log(event.target.id);\\r\\n   var orderId = event.target.id;\\r\\n   var orderStatusValue = event.target.value;\\r\\n   var emailId=event.target.getAttribute('data-email');\\r\\n   var name=event.target.getAttribute('data-name');\\r\\n    \\r\\n   var orderRid=event.target.getAttribute('data-rid');\\r\\n   var username=name?name:emailId;\\r\\n   \\r\\n   swal({\\r\\n    title: \\\"Are you sure?\\\",\\r\\n    text: \\\"Do your really want to  change the status?\\\",\\r\\n    icon: \\\"warning\\\",\\r\\n    buttons: true,\\r\\n    dangerMode: true\\r\\n  }).then(willChange => {\\r\\n    if (willChange) {\\r\\n    let dbCon = firebase.database().ref(`/orders/${orderId}`);\\r\\n    dbCon.update({\\r\\n        status: orderStatusValue,\\r\\n \\r\\n});\\r\\n    let trackingNumber='';\\r\\n     let deliveredBy = '';\\r\\n    var headers = {\\r\\n        'Content-Type': 'application/json',\\r\\n       }\\r\\n          \\r\\n    if(orderStatusValue=='Dispatched'){\\r\\n        trackingNumber='6245454784';\\r\\n         deliveredBy = 'Fedex';\\r\\n    }\\r\\n\\r\\n   \\r\\n    let user= {\\\"orderStatus\\\":orderStatusValue,\\r\\n    \\\"username\\\":username,\\r\\n    \\\"to\\\":emailId,\\r\\n    \\\"orderId\\\":orderRid,\\r\\n    \\\"trackingNumber\\\":trackingNumber,\\r\\n    \\\"deliveredBy\\\":deliveredBy};\\r\\n\\r\\n    axios.post(`https://www.panepuri.com/api/service/sendEmail`,  user,{headers: headers})\\r\\n      .then(res => {\\r\\n        console.log(res);\\r\\n        console.log(res);\\r\\n      });\\r\\n    \\r\\n} else {\\r\\n    window.location.reload(); \\r\\n}\\r\\n});\\r\\n}\\r\\n\\r\\nhandlePageChange(pageNumber) {\\r\\n    console.log(`active page is ${pageNumber}`);\\r\\n    this.setState({ activePage: pageNumber });\\r\\n  }\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n    