                                         % &nbsp;&nbsp;&nbsp;</label>\\r\\n                                                                <label className=\\\"radio-inline no-padd\\\">\\r\\n                                                                    <input\\r\\n                                                                        type=\\\"radio\\\"\\r\\n                                                                        name=\\\"offerType\\\"\\r\\n                                                                        onChange={this.onChange}\\r\\n                                                                        checked={this.state.offerType === 'Inr'}\\r\\n                                                                        value=\\\"Inr\\\"\\r\\n                                                                        className=\\\"icheck\\\"/>\\r\\n                                                                    INR</label>\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"form-group\\\">\\r\\n                                                        <div className=\\\"col-sm-offset-2 col-sm-10\\\">\\r\\n                                                            <button type=\\\"submit\\\" className=\\\"btn btn-success\\\">Save</button>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                </form>\\r\\n\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\nexport default EditCoupon;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport swal from 'sweetalert';\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport Pagination from \\\"react-js-pagination\\\";\\r\\n\\r\\nclass MerchantAccount extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        gstList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"admin_users/\\\").orderByChild(\\\"Role\\\").equalTo(\\\"Merchant\\\");\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const GSTData = {\\r\\n            cmsId: childSnapShot.key.toString(),\\r\\n            fullName: childSnapShot.val().fullName,\\r\\n            email: childSnapShot.val().email,\\r\\n            username: childSnapShot.val().username,\\r\\n            password: childSnapShot.val().password,\\r\\n            status: childSnapShot.val().status,\\r\\n            createdAt: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data,countPage: data.length,loading: false });\\r\\n        console.log(this.state.gstList);\\r\\n      });\\r\\n}\\r\\ndeleteItem = uid => {\\r\\n//     var userUid = firebase.auth().currentUser.uid;\\r\\n// console.log(userUid)\\r\\n    \\r\\n// userUid.delete().then(function() {\\r\\n//   // User deleted.\\r\\n// }).catch(function(error) {\\r\\n//   // An error happened.\\r\\n// });\\r\\n\\r\\n    \\r\\n    swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n      if (willDelete) {\\r\\n        console.log(uid);\\r\\n        var playersRef = firebase\\r\\n        .database()\\r\\n        .ref(`/admin_users/${uid}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n        \\r\\n  \\r\\n      } else {}\\r\\n      // window.location.reload(\\\"/Home\\\");\\r\\n     \\r\\n  });\\r\\n}\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n        \\r\\n        var cmsId =event.target.id;\\r\\n        console.log(cmsId);\\r\\n        this.setState({ [event.target.name]: event.target.value });\\r\\n        console.log(event.target.value);\\r\\n\\r\\n        let dbCon = firebase.database().ref(`/admin_users/${cmsId}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update ({\\r\\n            status: event.target.value,\\r\\n        });\\r\\n    };\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div classN