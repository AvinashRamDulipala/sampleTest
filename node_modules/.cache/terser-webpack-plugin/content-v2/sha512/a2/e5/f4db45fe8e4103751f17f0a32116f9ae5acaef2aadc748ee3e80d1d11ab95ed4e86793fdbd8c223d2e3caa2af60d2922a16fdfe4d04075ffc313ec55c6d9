\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport swal from 'sweetalert';\\r\\nimport $ from 'jquery';\\r\\n$.DataTable = require('datatables.net');\\r\\nclass SubSliderList extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            categoryList: [],\\r\\n            loading: false,\\r\\n            count: 1,\\r\\n            activePage: \\\"1\\\",\\r\\n            countPage: \\\"\\\",\\r\\n            status: ''\\r\\n        };\\r\\n        this.deleteItem = this\\r\\n            .deleteItem\\r\\n            .bind(this);\\r\\n\\r\\n    }\\r\\n\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"SubSlider/\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n                    cmsId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    pageTitle: childSnapShot\\r\\n                        .val()\\r\\n                        .pageTitle,\\r\\n                    userName: childSnapShot\\r\\n                        .val()\\r\\n                        .userName,\\r\\n                    status: childSnapShot\\r\\n                        .val()\\r\\n                        .status,\\r\\n                        Category: childSnapShot\\r\\n                        .val()\\r\\n                        .category,\\r\\n                    sliderUrl: childSnapShot\\r\\n                        .val()\\r\\n                        .sliderUrl,\\r\\n                    uploadImage: childSnapShot\\r\\n                        .val()\\r\\n                        .uploadImage,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({gstList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.gstList);\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destroy(true);\\r\\n        clearTimeout(this.interval);\\r\\n      }\\r\\n    deleteItem = id => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(id);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/SubSlider/${id}`);\\r\\n                playersRef.remove();\\r\\n                this\\r\\n                .props\\r\\n                .history\\r\\n                .push(\\\"/SubSliderList/Deleted\\\");\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n\\r\\n    };\\r\\n   \\r\\n\\r\\n    onChange = (event) => {\\r\\n\\r\\n        var productId = event.target.id;\\r\\n        console.log(productId);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n        console.log(event.target.value);\\r\\n\\r\\n        let dbCon = firebase\\r\\n            .database()\\r\\n            .ref(`/SubSlider/${productId}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update({status: event.target.value});\\r\\n    };\\r\\n    render() {\\r\\n        var pathname = this\\r\\n        .props\\r\\n        .location\\r\\n        .pathname\\r\\n        .split(\\\"/\\\");\\r\\n    var alertName = pathname[2];\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Slider</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"product-status mg-b-30\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"product-status-wrap\\\">\\r\\n                                        <h4>Sub Slider List</h4>\\r\\n                                        <div className=\\\"add-product\\\">\\r\\n                                            <Link to='/AddSubSlider'>Add SubSlider</Link>\\r\\n                                        </div>\\r\\n                                        {alertName\\r\\n                                            ? <div\\r\\n                                                    className=\\\"alert alert-success display-none\\\"\\r\\n                                                    style={{\\r\\n                                                    display: \\\"block\\\"\\r\\n                                                }}>\\r\\n                                                    <a data-dismiss=\\\"alert\\\" href=\\\"#\\\" aria-hidden=\\\"true\\\" className=\\\"close\\\"></a>\\r\\n                                                    Sub Slider {alertName}\\r\\n                                                    Successfully\\r\\n                                                </div>\\r\\n                                            : ''}\\r\\n                                        <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                                            <thead>\\r\\n                                                <tr>\\r\\n                                                    <th>S.No</th>\\r\\n                                                    <th>Slider Name</th>\\r\\n                                                    <th>Slider Image</th>\\r\\n                                                    <th>Slider Url</th>\\r\\n                                                    <th>Category</th>\\r\\n                                                    <th>Status</th>\\r\\n                                                    <th>Created On</th>\\r\\n                                                    <th>Action</th>\\r\\n                                                </tr>\\r\\n                                            </thead>\\r\\n                                            <tbody>\\r\\n\\r\\n                                                {this.state.gstList && this\\r\\n                                                    .state\\r\\n                                                    .gstList\\r\\n                                                    .map((data, index) => {\\r\\n                                                        console.log(data);\\r\\n\\r\\n                                                        return (\\r\\n                                                            <tr key={index}>\\r\\n                                                                <td>{index + 1}</td>\\r\\n                                                                <td>{data.pageTitle}</td>\\r\\n                                                                <td className=\\\"img-product\\\"><img src={data.uploadImage} alt=\\\"\\\"/></td>\\r\\n                                                                <td>{data.sliderUrl}</td>\\r\\n                                                                <td>{data.Category}\\r\\n                                                                </td>\\r\\n                                                                <td>\\r\\n                                                                    <select\\r\\n                                                                        name=\\\"status\\\"\\r\\n                                                                        onChange={this.onChange}\\r\\n                                                                        id={data.cmsId}\\r\\n                                                                        className=\\\"form-control pro-edt-select form-control-primary active\\\">\\r\\n                                                                        <option\\r\\n                                                                            value=\\\"Active\\\"\\r\\n                                                                            selected={data.status == \\\"Active\\\"\\r\\n                                                                            ? true\\r\\n                                                                            : false}>Active</option>\\r\\n                                                                        <option\\r\\n                                                                            value=\\\"InActive\\\"\\r\\n                                                                            selected={data.status == \\\"InActive\\\"\\r\\n                                                                            ? true\\r\\n                                                                            : false}>InActive</option>\\r\\n\\r\\n                                                                    </select>\\r\\n                                                                </td>\\r\\n                                                                <td>{data.created_on}\\r\\n                                                                </td>\\r\\n                                                                <td>\\r\\n\\r\\n                                                                    <Link to={`/EditSubSlider/${data.cmsId}`}>\\r\\n                                                                        <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\">\\r\\n                                                                            <i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                        </button>\\r\\n                                                                    </Link>\\r\\n\\r\\n                                                                    <button\\r\\n                                                                        data-toggle=\\\"tooltip\\\"\\r\\n                                                                        title=\\\"Trash\\\"\\r\\n                                                                        className=\\\"pd-setting-ed\\\"\\r\\n                                                                        data-id={data.cmsId}\\r\\n                                                                        onClick={this\\r\\n                                                                        .deleteItem\\r\\n                                                                        .bind(this, data.cmsId)}>\\r\\n                                                                        <i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                    </button>\\r\\n                                                                </td>\\r\\n\\r\\n                                                            </tr>\\r\\n                                                        )\\r\\n\\r\\n                                                    })}\\r\\n\\r\\n                                            </tbody>\\r\\n                                        </table>\\r\\n                                      \\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                </div>\\r\\n\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\n\\r\\n\\r\\n\\r\\nexport default SubSliderList;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\n\\r\\nimport {Form} from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport ReactQuill from \\\"react-quill\\\";\\r\\nimport 'react-quill/dist/quill.snow.css';\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\nimport FileUploader from 'react-firebase-file-uploader';\\r\\n\\r\\nclass EditSubSlider extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            pageTitle: '',\\r\\n            category: '',\\r\\n            content: '',\\r\\n            date: new Date().toLocaleString(),\\r\\n            sliderUrl: '',\\r\\n            theme: 'snow',\\r\\n            uploadImage: '',\\r\\n            categoryList: '',\\r\\n\\r\\n            avatar: \\\"\\\",\\r\\n            isUploading: false,\\r\\n            progress: 0,\\r\\n            avatarURL: \\\"\\\",\\r\\n            filenames: [],\\r\\n            downloadURLs: [],\\r\\n            uploadProgress: 0\\r\\n        }\\r\\n        this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                website: {\\r\\n                    message: \\\"The Url should be example.com \\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentWillMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"category/\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const Categories = {\\r\\n                    categoryId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    category: childSnapShot\\r\\n                        .val()\\r\\n                        .category,\\r\\n\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(Categories);\\r\\n            });\\r\\n\\r\\n            this.setState({categoryList: data, loading: false});\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        const {CMSID} = this.props.match.params;\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`SubSlider/${CMSID}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var gstData = snapshot.val();\\r\\n            this.setState({\\r\\n                pageTitle: gstData.pageTitle,\\r\\n                category: gstData.category,\\r\\n                content: gstData.content,\\r\\n                avatarURL: gstData.uploadImage,\\r\\n                sliderUrl: gstData.sliderUrl,\\r\\n                loading: false\\r\\n            });\\r\\n        });\\r\\n\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n\\r\\n    modules = {\\r\\n        toolbar: [\\r\\n            [\\r\\n                {\\r\\n                    'header': [\\r\\n                        1,\\r\\n                        2,\\r\\n                        3,\\r\\n                        4,\\r\\n                        5,\\r\\n                        6,\\r\\n                        false\\r\\n                    ]\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'bold', 'italic', 'underline', 'strike', 'blockquote'\\r\\n            ],\\r\\n            [\\r\\n                {\\r\\n                    'list': 'ordered'\\r\\n                }, {\\r\\n                    'list': 'bullet'\\r\\n                }, {\\r\\n                    'indent': '-1'\\r\\n                }, {\\r\\n                    'indent': '+1'\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'link', 'image'\\r\\n            ],\\r\\n            ['clean'],\\r\\n            [\\r\\n                {\\r\\n                    'color': []\\r\\n                }, {\\r\\n                    'background': []\\r\\n                }\\r\\n            ]\\r\\n        ]\\r\\n    };\\r\\n\\r\\n    formats = [\\r\\n        'header',\\r\\n        'bold',\\r\\n        'italic',\\r\\n        'underline',\\r\\n        'strike',\\r\\n        'blockquote',\\r\\n        'list',\\r\\n        'bullet',\\r\\n        'indent',\\r\\n        'link',\\r\\n        'image',\\r\\n        'color',\\r\\n        'background'\\r\\n    ];\\r\\n\\r\\n    handleUploadStart = () => this.setState({isUploading: true, progress: 0});\\r\\n    handleProgress = progress => this.setState({progress});\\r\\n    handleUploadError = error => {\\r\\n        this.setState({isUploading: false});\\r\\n        console.error(error);\\r\\n    };\\r\\n    handleUploadSuccess = filename => {\\r\\n        this.setState({avatar: filename, progress: 100, isUploading: false});\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({avatarURL: url}));\\r\\n        console.log(this.state.avatarURL);\\r\\n    };\\r\\n    handleSubmit = (event) => {\\r\\n        if (this.validator.allValid()) {\\r\\n            const {CMSID} = this.props.match.params;\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0) {\\r\\n                event.preventDefault();\\r\\n                var userName = sessionStorage.getItem(\\\"userName\\\");\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref(`/SubSlider/${CMSID}`);\\r\\n                console.log(dbCon);\\r\\n                dbCon.update({\\r\\n                    pageTitle: this.state.pageTitle,\\r\\n                    category: this.state.category,\\r\\n                    userName: userName,\\r\\n                    content: this.state.content,\\r\\n                    uploadImage: this.state.avatarURL,\\r\\n                    sliderUrl: this.state.sliderUrl,\\r\\n                    created_on: this.state.date\\r\\n                });\\r\\n\\r\\n                this.setState({pageTitle: '', sliderUrl: '', content: ''});\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/SubSliderList/Updated\\\");\\r\\n            }\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Settings</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit SubSlider</h4>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Edit-GST\\\">\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Page Title<span style={{\\r\\n                color: 'red'\\r\\n            }}>*</span>\\r\\n                                                            </label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Page Title*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageTitle\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageTitle}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Title\\\", this.state.pageTitle, \\\"required|min:3|max:500\\\")}\\r\\n                                                            <div className=\\\"text-danger\\\">\\r\\n                                                                {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Category Name<span style={{\\r\\n                color: 'red'\\r\\n            }}>*</span>\\r\\n                                                            </label>\\r\\n                                                            <select\\r\\n                                                                name=\\\"select\\\"\\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"category\\\"\\r\\n                                                                onChange={this.onChange}>\\r\\n                                                                <option value={this.state.category}>{this.state.category}</option>\\r\\n                                                                {this.state.categoryList && this\\r\\n                                                                    .state\\r\\n                                                                    .categoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.category} key={index}>{data.category}</option>\\r\\n                                                                        )\\r\\n\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n                                                            {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Select category\\\", this.state.category, \\\"required\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Slider Url<span style={{\\r\\n                color: 'red'\\r\\n            }}>*</span>\\r\\n                                                            </label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Slider Url*\\\"\\r\\n                                                                name=\\\"sliderUrl\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.sliderUrl}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Slider Url\\\", this.state.sliderUrl, \\\"required|website\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <div className=\\\"dropzone-pro\\\">\\r\\n                                                                <div id=\\\"dropzone\\\">\\r\\n\\r\\n                                                                    <div className=\\\"dz-message needsclick download-custom\\\">\\r\\n                                                                        <i className=\\\"fa fa-cloud-download\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                        <h2>Upload Product Image</h2>\\r\\n\\r\\n                                                                    </div>\\r\\n\\r\\n                                                                    {this.state.isUploading && <p>Progress: {this.state.progress}</p>}\\r\\n                                                                    {this.state