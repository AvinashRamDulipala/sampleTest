);\\r\\n                event.preventDefault();\\r\\n\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref('/Notifications');\\r\\n                dbCon.push({\\r\\n                    pageTitle: this.state.pageTitle,\\r\\n                     notification_type: this.state.notification_type,\\r\\n                      pageName: this.state.pageName, \\r\\n                    content: this.state.content,\\r\\n                    merchantList:this.state.multiSelect,\\r\\n                     created_on: this.state.date});\\r\\n\\r\\n                this.setState({pageTitle: '', content: '', pageName: ''});\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Notifications\\\");\\r\\n            }\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n    addNotification() {\\r\\n        this\\r\\n            .notificationDOMRef\\r\\n            .current\\r\\n            .addNotification({\\r\\n                title: \\\"Success\\\",\\r\\n                message: \\\"Succesfully added!\\\",\\r\\n                type: \\\"success\\\",\\r\\n                insert: \\\"bottom\\\",\\r\\n                container: \\\"top-center\\\",\\r\\n                animationIn: [\\r\\n                    \\\"animated\\\", \\\"fadeIn\\\"\\r\\n                ],\\r\\n                animationOut: [\\r\\n                    \\\"animated\\\", \\\"fadeOut\\\"\\r\\n                ],\\r\\n                dismiss: {\\r\\n                    duration: 2000\\r\\n                },\\r\\n                dismissable: {\\r\\n                    click: true\\r\\n                }\\r\\n            });\\r\\n    }\\r\\n    optionClicked(optionsList) {\\r\\n        this.setState({multiSelect: optionsList});\\r\\n        console.log(this.state.