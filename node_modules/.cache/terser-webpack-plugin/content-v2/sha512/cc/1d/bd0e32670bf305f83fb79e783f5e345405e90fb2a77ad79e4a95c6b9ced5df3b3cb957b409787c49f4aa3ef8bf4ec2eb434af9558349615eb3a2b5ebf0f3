                                </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\n\\r\\nexport default AddMerchantPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport {Form} from 'reactstrap';\\r\\nimport FileUploader from 'react-firebase-file-uploader';\\r\\nimport SelectSearch from \\\"react-select-search\\\";\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\nimport ReactQuill from \\\"react-quill\\\";\\r\\nimport 'react-quill/dist/quill.snow.css';\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\n\\r\\nclass EditProductPage extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            productName: '',\\r\\n            category: '',\\r\\n            subCategory: '',\\r\\n            gstValue: '',\\r\\n            pincode: '',\\r\\n            price: '',\\r\\n            offerPrice: '',\\r\\n            sellingPrice: '',\\r\\n            quantityStock: '',\\r\\n            packagingCost: '',\\r\\n            alertQuantity: '',\\r\\n            uploadImage: '',\\r\\n            content: '',\\r\\n            cancellation: '',\\r\\n            theme: 'snow',\\r\\n            categoryList: [],\\r\\n            subCategoryList: [],\\r\\n            avatar: \\\"\\\",\\r\\n            isUploading: false,\\r\\n            progress: 0,\\r\\n            avatarURL: \\\"\\\",\\r\\n            filenames: [],\\r\\n            downloadURLs: [],\\r\\n            uploadProgress: 0,\\r\\n\\r\\n            date: new Date().toLocaleString()\\r\\n        }\\r\\n        this.handleChange = this\\r\\n            .handleChange\\r\\n            .bind(this);\\r\\n        this.addNotification = this\\r\\n            .addNotification\\r\\n            .bind(this);\\r\\n        this.notificationDOMRef = React.createRef();\\r\\n        this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n                passwordvalid: {\\r\\n                    message: \\\"The :attribute must be at least 6 and at most 30 with 1 numeric,1 special charac\\\" +\\r\\n                            \\\"ter and 1 alphabet.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^(?=.*\\\\d)(?=.*[!@#$%^&*])(?=.*[a-z]).{6,30}$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                passwordMismatch: {\\r\\n                    message: \\\"confirm password must match with password field.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        return document\\r\\n                            .getElementById(\\\"password_input\\\")\\r\\n                            .value === val\\r\\n                            ? true\\r\\n                            : false;\\r\\n                    }\\r\\n                },\\r\\n                AlertquantityStock: {\\r\\n                    message: \\\"alert quantity is less than quantity Instock.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        var qty = document\\r\\n                            .getElementById('quantity_stock')\\r\\n                            .value;\\r\\n\\r\\n                        if (Number(qty) > Number(val)) {\\r\\n\\r\\n                            return true;\\r\\n                        } else {\\r\\n                            return false;\\r\\n                        }\\r\\n\\r\\n                    }\\r\\n                },\\r\\n                productPriceMismatch: {\\r\\n                    message: \\\"Seller Price is less than Product Price.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        var qty = document\\r\\n                            .getElementById('product_price')\\r\\n                            .value;\\r\\n\\r\\n                        if (Number(qty) > Number(val)) {\\r\\n\\r\\n                            return true;\\r\\n                        } else {\\r\\n                            return false;\\r\\n                        }\\r\\n\\r\\n                    }\\r\\n                },\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n\\r\\n                zip: {\\r\\n                    message: \\\"Invalid Pin Code\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^(\\\\d{5}(\\\\d{4})?)?$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                website: {\\r\\n                    message: \\\"The Url should be example.com \\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                Fax: {\\r\\n                    message: \\\"Invalid fax number \\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        return (validator.helpers.testRegex(val, /^([\\\\w-\\\\.]+)@((\\\\[[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.)|(([\\\\w-]+\\\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\\\\]?)$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n    }\\r\\n\\r\\n    addNotification() {\\r\\n        this\\r\\n            .notificationDOMRef\\r\\n            .current\\r\\n            .addNotification({\\r\\n                title: \\\"Success\\\",\\r\\n                message: \\\"Succesfully added!\\\",\\r\\n                type: \\\"success\\\",\\r\\n                insert: \\\"bottom\\\",\\r\\n                container: \\\"top-center\\\",\\r\\n                animationIn: [\\r\\n                    \\\"animated\\\", \\\"fadeIn\\\"\\r\\n                ],\\r\\n                animationOut: [\\r\\n                    \\\"animated\\\", \\\"fadeOut\\\"\\r\\n                ],\\r\\n                dismiss: {\\r\\n                    duration: 2000\\r\\n                },\\r\\n                dismissable: {\\r\\n                    click: true\\r\\n                }\\r\\n            });\\r\\n    }\\r\\n\\r\\n    componentDidMount() {\\r\\n\\r\\n        const {productID} = this.props.match.params;\\r\\n        console.log(productID);\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`products/${productID}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var products = snapshot.val();\\r\\n            console.log(products);\\r\\n\\r\\n            this.setState({\\r\\n                productName: products.productName,\\r\\n                category: products.category,\\r\\n                subCategory: products.subCategory,\\r\\n                gstValue: products.gstValue,\\r\\n                pincode: products.pincode,\\r\\n                price: products.price,\\r\\n                cancellation: products.cancellation,\\r\\n                offerPrice: products.offerPrice,\\r\\n                sellingPrice: products.sellingPrice,\\r\\n                quantityStock: products.quantityStock,\\r\\n                alertQuantity: products.alertQuantity,\\r\\n                packagingCost: products.packagingCost,\\r\\n                downloadURLs: products.uploadImage,\\r\\n                avatarURL: products.frontImage,\\r\\n                content: products.description,\\r\\n                loading: false\\r\\n            }, () => {\\r\\n                this.setSubCategoryList(this.state.category)\\r\\n            });\\r\\n\\r\\n        });\\r\\n        {\\r\\n            this.categories()\\r\\n        }\\r\\n        console.log(this.state.category);\\r\\n\\r\\n    }\\r\\n    categories = (event) => {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"category/\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const Categories = {\\r\\n                    categoryId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    category: childSnapShot\\r\\n                        .val()\\r\\n                        .category,\\r\\n\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(Categories);\\r\\n            });\\r\\n\\r\\n            this.setState({categoryList: data, loading: false});\\r\\n            console.log(this.state.categoryList);\\r\\n        });\\r\\n\\r\\n        var GstData = firebase\\r\\n            .database()\\r\\n            .ref(\\\"GST/\\\");\\r\\n\\r\\n        GstData.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTList = {\\r\\n                    gstId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    gst: childSnapShot\\r\\n                        .val()\\r\\n                        .gst,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(GSTList);\\r\\n            });\\r\\n\\r\\n            this.setState({GSTList: data, loading: false});\\r\\n            console.log(this.state.GSTList);\\r\\n        });\\r\\n\\r\\n    };\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n    setCategoryName = (e) => {\\r\\n        console.log(e.target.value);\\r\\n\\r\\n        let selectedCategoryName = e.target.value;\\r\\n\\r\\n        {\\r\\n            this.setSubCategoryList(selectedCategoryName)\\r\\n        }\\r\\n\\r\\n    };\\r\\n    setSubCategoryList = (selectedCategoryName) => {\\r\\n\\r\\n        console.log(selectedCategoryName);\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"sub_category\\\")\\r\\n            .orderByChild(\\\"category\\\")\\r\\n            .equalTo(selectedCategoryName);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTList = {\\r\\n                    subCategoryId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    subCategory: childSnapShot\\r\\n                        .val()\\r\\n                        .subCategory\\r\\n                };\\r\\n\\r\\n                data.push(GSTList);\\r\\n            });\\r\\n\\r\\n            this.setState({subcategoryList: data, category: selectedCategoryName, loading: false});\\r\\n            console.log(this.state.subcategoryList);\\r\\n        });\\r\\n    }\\r\\n\\r\\n    handleChange = (value) => {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n    handleUploadStart = () => this.setState({isUploading: true, uploadProgress: 0, downloadURLs: ''});\\r\\n    handleFrontImageUploadStart = () => this.setState({isUploading: true, uploadProgress: 0, avatarURL: ''});\\r\\n\\r\\n    handleProgress = progress => this.setState({uploadProgress: progress});\\r\\n\\r\\n    handleUploadError = error => {\\r\\n        this.setState({\\r\\n            isUploading: false\\r\\n            // Todo: handle error\\r\\n        });\\r\\n        console.error(error);\\r\\n    };\\r\\n    handleFrontImageUploadSuccess = filename => {\\r\\n        this.setState({avatar: filename, progress: 100, isUploading: false});\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({avatarURL: url}));\\r\\n    };\\r\\n    handleUploadSuccess = async filename => {\\r\\n        const downloadURL = await firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL();\\r\\n\\r\\n        this.setState(oldState => ({\\r\\n            filenames: [\\r\\n                ...oldState.filenames,\\r\\n                filename\\r\\n            ],\\r\\n            downloadURLs: [\\r\\n                ...oldState.downloadURLs,\\r\\n                downloadURL\\r\\n            ],\\r\\n            uploadProgress: 100,\\r\\n            isUploading: false\\r\\n        }));\\r\\n        console.log(this.state.downloadURLs);\\r\\n    };\\r\\n\\r\\n    handleSubmit = (event) => {\\r\\n\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid()) {\\r\\n\\r\\n            this.addNotification();\\r\\n            const {productID} = this.props.match.params;\\r\\n            var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n            var userName = sessionStorage.getItem(\\\"userName\\\");\\r\\n\\r\\n            let dbCon = firebase\\r\\n                .database()\\r\\n                .ref(`products/${productID}`);\\r\\n            dbCon.update({\\r\\n                productName: this.state.productName,\\r\\n                userName: userName,\\r\\n                category: this.state.category,\\r\\n                subCategory: this.state.subCategory,\\r\\n                gstValue: this.state.gstValue,\\r\\n                pincode: this.state.pincode,\\r\\n                cancellation: this.state.cancellation,\\r\\n                packagingCost: this.state.packagingCost,\\r\\n                price: this.state.price,\\r\\n                offerPrice: this.state.offerPrice,\\r\\n                sellingPrice: this.state.sellingPrice,\\r\\n                quantityStock: this.state.quantityStock,\\r\\n\\r\\n                alertQuantity: this.state.alertQuantity,\\r\\n                frontImage: this.state.avatarURL,\\r\\n                uploadImage: this.state.downloadURLs,\\r\\n                description: this.state.content,\\r\\n                sessionId: sessionId,\\r\\n                created_on: this.state.date\\r\\n            });\\r\\n\\r\\n            this\\r\\n                .props\\r\\n                .history\\r\\n                .push(\\\"/ProductListPage\\\");\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Products</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit Products</h4>\\r\\n                                            <ReactNotification ref={this.notificationDOMRef}/>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Add-products\\\">\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Product Name</label>\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"productName\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.productName}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Product Name\\\", this.state.productName, \\\"required|whitespace|min:2|max:70\\\")}\\r\\n                                                        </div>\\r\\n\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Select Category</label>\\r\\n                                                            <select\\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"category\\\"\\r\\n                                                                value={this.state.category}\\r\\n                                                                onChange={this.setCategoryName}>\\r\\n                                                                <option>Select Category</option>\\r\\n                                                                {this.state.categoryList && this\\r\\n                                                                    .state\\r\\n                                                                    .categoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.category} key={index}>{data.category}</option>\\r\\n                                                                        )\\r\\n                                                                        {\\r\\n                                                                            this\\r\\n                                                                                .validator\\r\\n                                                                                .message(\\\"Select category\\\", this.state.category, \\\"required\\\")\\r\\n                                                                        }\\r\\n\\r\\n                                                                    })}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>Select Sub Category\\r\\n                                                            </label>\\r\\n                                                            <select\\r\\n                                                                className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                                name=\\\"subCategory\\\"\\r\\n                                                                value={this.state.subCategory}\\r\\n                                                                onChange={this.onChange}>\\r\\n                                                                <option>Select Sub Category</option>\\r\\n                                                                {this.state.subcategoryList && this\\r\\n                                                                    .state\\r\\n                                                                    .subcategoryList\\r\\n                                                                    .map((data, index) => {\\r\\n\\r\\n                                                                        return (\\r\\n                                                                            <option value={data.subCategory} key={index}>{data.subCategory}</option>\\r\\n                                                                        )\\r\\n\\r\\n                                                                    })}\\r\\n                                                                {this\\r\\n                                                                    .validator\\r\\n                                                                    .message(\\\"Select Sub category\\\", this.state.subCategory, \\\"required\\\")\\r\\n}\\r\\n\\r\\n                                                            </select>\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>GST</label>\\r\\