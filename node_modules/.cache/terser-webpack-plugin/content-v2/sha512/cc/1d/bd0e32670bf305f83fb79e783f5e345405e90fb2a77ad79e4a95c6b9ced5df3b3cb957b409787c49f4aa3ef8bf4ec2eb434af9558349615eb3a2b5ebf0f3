\\r\\n        </div>\\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default AddEmailContentPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\nimport Pagination from \\\"react-js-pagination\\\";\\r\\n\\r\\n\\r\\n\\r\\n\\r\\nclass SocialAccounts extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        gstList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"SocialAccounts/\\\");\\r\\n\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        // console.log(snapshot);\\r\\n        // console.log(snapshot.val);\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const GSTData = {\\r\\n            socilaId: childSnapShot.key.toString(),\\r\\n            gst: childSnapShot.val().gst,\\r\\n            title: childSnapShot.val().title,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data,countPage: data.length,loading: false });\\r\\n        console.log(this.state.gstList);\\r\\n      });\\r\\n}\\r\\n    deleteItem = id => {\\r\\n        \\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase.database().ref(`/GST/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n    };\\r\\n\\r\\n    handlePag