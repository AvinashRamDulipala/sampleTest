\\\"\\r\\n                                                                className=\\\"valid\\\"/>\\r\\n                                                            <span\\r\\n                                                                style={{\\r\\n                                                                color: 'red'\\r\\n                                                            }}/>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"form-group\\\">\\r\\n                                                        <label className=\\\"col-sm-2 control-label\\\">Minimum Value<span style={{\\r\\n                color: 'red'\\r\\n            }}>*</span>\\r\\n                                                        </label>\\r\\n                                                        <div className=\\\"col-sm-8\\\">\\r\\n                                                            <input\\r\\n                                                                className=\\\"form-control required\\\"\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.minValue}\\r\\n                                                                name=\\\"minValue\\\"\\r\\n                                                             \\r\\n                                                                placeholder=\\\"Minimum Value*\\\"\\r\\n                                                                maxLength={6}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Minimum Value\\\", this.state.minValu