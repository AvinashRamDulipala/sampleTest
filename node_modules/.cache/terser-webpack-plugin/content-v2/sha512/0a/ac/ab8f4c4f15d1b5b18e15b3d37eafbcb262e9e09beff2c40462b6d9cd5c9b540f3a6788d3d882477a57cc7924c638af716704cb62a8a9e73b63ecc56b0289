         // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n\\r\\n    onChange = (event) => {\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref('category/')\\r\\n            .orderByChild(\\\"category\\\")\\r\\n            .equalTo(event.target.value);\\r\\n        ref.on('value', snapshot => {\\r\\n            var user_exist = snapshot.numChildren();\\r\\n            console.log(user_exist);\\r\\n\\r\\n            if (user_exist > 0) {\\r\\n\\r\\n                this.setState({error_message: \\\"Category Name is already exist\\\", validError: false});\\r\\n            } else {\\r\\n                this.setState({error_message: \\\"\\\", validError: true});\\r\\n\\r\\n            }\\r\\n\\r\\n        })\\r\\n\\r\\n    };\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid() && this.state.validError == true) {\\r\\n            if (this.state.category && this.state.category.trim().length !== 0) {\\r\\n\\r\\n                var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n                var userName = sessionStorage.getItem(\\\"userName\\\");\\r\\n\\r\\n                // let dbCon = firebase.firestore().collection('/category');\\r\\n\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref('/category');\\r\\n                dbCon\\r\\n                    .push({category: this.state.category, userName: userName, created_on: this.state.date, sessionId: sessionId})\\r\\n                    .then(resp