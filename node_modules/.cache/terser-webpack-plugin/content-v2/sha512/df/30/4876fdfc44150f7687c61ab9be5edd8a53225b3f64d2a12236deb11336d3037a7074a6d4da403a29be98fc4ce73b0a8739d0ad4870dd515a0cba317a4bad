ildSnapShot.val().pageTitle,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data,countPage: data.length,loading: false });\\r\\n        console.log(this.state.gstList);\\r\\n      });\\r\\n}\\r\\n    deleteItem = id => {\\r\\n        \\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase.database().ref(`/Blog/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n    };\\r\\n\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Settings</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Blog</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddBlog'>Add Blog</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Title</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.gstList && this.state.gstList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.pageTitle}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditBlog/${data.cmsId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.cmsId}  onClick={this.deleteItem.bind(this, data.cmsId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    <div className=\\\"custom-pagination\\\">\\r\\n                            <Pagination\\r\\n                          hideNavigation\\r\\n                          activePage={this.state.activePage}\\r\\n                          itemsCountPerPage={3}\\r\\n                          totalItemsCount={this.state.countPage}\\r\\n                          pageRangeDisplayed={3}\\r\\n                          onChange={this.handlePageChange.bind(this)}\\r\\n                        />\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default Blog;\\r\\n\",\"import React from \\\"react\\\";\\r\\n\\r\\nimport {Form} from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport ReactQuill from \\\"react-quill\\\";\\r\\nimport 'react-quill/dist/quill.snow.css';\\r\\n\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\nimport FileUploader from 'react-firebase-file-uploader';\\r\\n\\r\\nclass EditBlog extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            pageTitle: '',\\r\\n            date: new Date().toLocaleString(),\\r\\n            content: '',\\r\\n            theme: 'snow',\\r\\n            uploadImage: '',\\r\\n            avatar: \\\"\\\",\\r\\n            isUploading: false,\\r\\n            progress: 0,\\r\\n            avatarURL: \\\"\\\"\\r\\n\\r\\n        }\\r\\n        this.handleChange = this\\r\\n            .handleChange\\r\\n            .bind(this);\\r\\n            this.addNotification = this.addNotification.bind(this);\\r\\n            this.notificationDOMRef = React.createRef();\\r\\n\\r\\n        this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                website: {\\r\\n                    message: \\\"The Url should be example.com \\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        const {CMSID} = this.props.match.params;\\r\\n        console.log(CMSID);\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`Blog/${CMSID}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var gstData = snapshot.val();\\r\\n            console.log(gstData)\\r\\n            this.setState({pageTitle: gstData.pageTitle,  uploadImage: gstData.uploadImage,content: gstData.content, loading: false});\\r\\n            //console.log(this.state.pageTitle);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({pageTitle: event.target.value});\\r\\n    };\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n    addNotification() {\\r\\n        this\\r\\n            .notificationDOMRef\\r\\n            .current\\r\\n            .addNotification({\\r\\n                title: \\\"Success\\\",\\r\\n                message: \\\"Succesfully added!\\\",\\r\\n                type: \\\"success\\\",\\r\\n                insert: \\\"bottom\\\",\\r\\n                container: \\\"top-center\\\",\\r\\n                animationIn: [\\r\\n                    \\\"animated\\\", \\\"fadeIn\\\"\\r\\n                ],\\r\\n                animationOut: [\\r\\n                    \\\"animated\\\", \\\"fadeOut\\\"\\r\\n                ],\\r\\n                dismiss: {\\r\\n                    duration: 2000\\r\\n                },\\r\\n                dismissable: {\\r\\n                    click: true\\r\\n                }\\r\\n            });\\r\\n    }\\r\\n    handleUploadStart = () => this.setState({ isUploading: true, progress: 0 });\\r\\n        handleProgress = progress => this.setState({ progress });\\r\\n        handleUploadError = error => {\\r\\n          this.setState({ isUploading: false });\\r\\n          console.error(error);\\r\\n        };\\r\\n        handleUploadSuccess = filename => {\\r\\n          this.setState({ avatar: filename, progress: 100, isUploading: false });\\r\\n          firebase\\r\\n            .database()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({ avatarURL: url }));\\r\\n            console.log(this.state.avatarURL);\\r\\n        };\\r\\n    handleSubmit = (event) => {\\r\\n        if (this.validator.allValid()) {\\r\\n            const {CMSID} = this.props.match.params;\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0) {\\r\\n                event.preventDefault();\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref(`/Blog/${CMSID}`);\\r\\n                console.log(dbCon);\\r\\n                dbCon.update({pageTitle: this.state.pageTitle,  uploadImage: this.state.uploadImage,content: this.state.content, created_on: this.state.date});\\r\\n\\r\\n                this.setState({pageTitle: '', content: ''});\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Blog\\\");\\r\\n            }\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Settings</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit Blog</h4>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Edit-GST\\\">\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Page Title*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageTitle\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageTitle}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Title\\\", this.state.pageTitle, \\\"required|min:3|max:500\\\")}\\r\\n                                                            <div className=\\\"text-danger\\\">\\r\\n                                                                {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <ReactQuill\\r\\n                                                                theme={this.state.theme}\\r\\n                                                                value={this.state.content}\\r\\n                                                                placeholder='Enter Description'\\r\\n                                                                onChange={this.handleChange}\\r\\n                                                                className=\\\"add-new-post__editor mb-1\\\"\\r\\n                                                                style={divStyle}/>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        {this\\r\\n                                                            .validator\\r\\n                                                            .message(\\\"Page Content\\\", this.state.content, \\\"required|min:150\\\")}\\r\\n                                                        <div className=\\\"text-danger\\\">\\r\\n                                                            {\\\" \\\"}\\r\\n                                                            {this.state.employer_sevice_message}\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <div className=\\\"dropzone-pro\\\">\\r\\n                                                                <div id=\\\"dropzone\\\">\\r\\n\\r\\n                                                                    <div className=\\\"dz-message needsclick download-custom\\\">\\r\\n                                                                        <i className=\\\"fa fa-cloud-download\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                        <h2>Upload Product Image</h2>\\r\\n\\r\\n                                                                    </div>\\r\\n\\r\\n                                                                    {this.state.isUploading && <p>Progress: {this.state.progress}</p>}\\r\\n                                                                    {this.state.avatarURL && <img\\r\\n                                                                        src={this.state.avatarURL}\\r\\n                                                                        style={{\\r\\n                                                                        height: \\\"200px\\\",\\r\\n                                                                        width: \\\"250px\\\"\\r\\n                                                                    }}/>}\\r\\n                                                                    <FileUploader\\r\\n                                                                        accept=\\\"image/*\\\"\\r\\n                                                                        name=\\\"avatar\\\"\\r\\n                                                                        randomizeFilename\\r\\n                                                                        storageRef={firebase.storage().ref(\\\"images\\\")}\\r\\n                                                                        onUploadStart={this.handleUploadStart}\\r\\n                                                                        onUploadError={this.handleUploadError}\\r\\n                                                                        onUploadSuccess={this.handleUploadSuccess}\\r\\n                                                                        onProgress={this.handleProgress}/> {this\\r\\n                                                                        .validator\\r\\n                                                                        .message(\\\"Upload Product Image\\\", this.state.avatarURL, \\\"required\\\")}\\r\\n\\r\\n                                                                </div>\\r\\n    