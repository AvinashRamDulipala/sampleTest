) {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Settings</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Pin Code List</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddPinCode'>Add Pin Code</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Pin Code</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.gstList && this.state.gstList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.pincode}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditPinCode/${data.gstId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.gstId}  onClick={this.deleteItem.bind(this, data.gstId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    {/* <div className=\\\"custom-pagination\\\">\\r\\n                            <Pagination\\r\\n                          hideNavigation\\r\\n                          activePage={this.state.activePage}\\r\\n                          itemsCountPerPage={3}\\r\\n                          totalItemsCount={this.state.countPage}\\r\\n                          pageRangeDisplayed={3}\\r\\n                          onChange={this.handlePageChange.bind(this)}\\r\\n                        />\\r\\n                            </div> */}\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default PincodeList;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\n\\r\\nimport { Form } from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\n\\r\\n\\r\\n\\r\\n\\r\\nclass EditPinCode extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        pincode:'',\\r\\n        date:new Date().toLocaleString(),\\r\\n\\r\\n    }\\r\\n    this.addNotification = this.addNotification.bind(this);\\r\\n    this.notificationDOMRef = React.createRef();\\r\\n    this.validator = new SimpleReactValidator({\\r\\n      className: \\\"text-danger\\\",\\r\\n      validators: {\\r\\n\\r\\n          whitespace: {\\r\\n              message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          specialChar: {\\r\\n              message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          specialCharText: {\\r\\n              message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          }\\r\\n      }\\r\\n  });\\r\\n    \\r\\n\\r\\n  }\\r\\n  componentDidMount(){\\r\\n      const {GstID}=this.props.match.params;\\r\\n      console.log(GstID);\\r\\n      \\r\\n      var ref = firebase.database().ref(`PinCode/${GstID}`);\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            var gstData = snapshot.val();\\r\\n            // console.log(categories)\\r\\n           \\r\\n            this.setState({ pincode:gstData.pincode,loading: false });\\r\\n            console.log(this.state.pincode);\\r\\n          });\\r\\n     \\r\\n  }\\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({ [event.target.name]: event.target.value });\\r\\n  };\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n    if (this.validator.allValid()) {\\r\\n    const {GstID}=this.props.match.params;\\r\\n    \\r\\n    if (this.state.pincode && this.state.pincode.trim().length !== 0) {\\r\\n        event.preventDefault();\\r\\n        let dbCon = firebase.database().ref(`/PinCode/${GstID}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update ({\\r\\n            pincode: this.state.pincode,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n        this.addNotification();\\r\\n      \\r\\n        \\r\\n\\r\\n      \\r\\n      this.setState({\\r\\n        pincode: ''\\r\\n      });\\r\\n      this.props.history.push(\\\"/GSTListPage\\\");\\r\\n    }\\r\\n  } else {\\r\\n    this\\r\\n        .validator\\r\\n        .showMessages();\\r\\n    this.forceUpdate();\\r\\n  }\\r\\n  };\\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\\"bottom\\\",\\r\\n      container: \\\"top-center\\\",\\r\\n      animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n      animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n      dismiss: { duration: 2000 },\\r\\n      dismissable: { click: true }\\r\\n    });\\r\\n  }\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n        <div>\\r\\n       <SidebarPage/>\\r\\n     \\r\\n      <div className=\\\"all-content-wrapper\\\">\\r\\n      <HeaderPage/>\\r\\n     \\r\\n            \\r\\n              <div className=\\\"breadcome-area\\\">\\r\\n                  <div className=\\\"container-fluid\\\">\\r\\n                      <div className=\\\"row\\\">\\r\\n                          <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                              <div className=\\\"breadcome-list\\\">\\r\\n                                  <div className=\\\"row\\\">\\r\\n                                      <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                          <div className=\\\"breadcomb-wp\\\">\\r\\n                                          \\r\\n                                              <div className=\\\"breadcomb-ctn\\\">\\r\\n                                              <h2>Pincode</h2>\\r\\n                                                  </div>\\r\\n                                          </div>\\r\\n                                      </div>\\r\\n                            \\r\\n                                  </div>\\r\\n                              </div>\\r\\n                          </div>\\r\\n                      </div>\\r\\n                  </div>\\r\\n              </div>\\r\\n        \\r\\n       \\r\\n              <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n            \\r\\n              <div className=\\\"single-pro-review-area\\\">\\r\\n                  <div className=\\\"container-fluid\\\">\\r\\n                      <div className=\\\"row\\\">\\r\\n                          <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                              <div className=\\\"review-tab-pro-inner\\\">\\r\\n                     \\r\\n                     <h4>Edit Pincode</h4>\\r\\n                     <ReactNotification ref={this.notificationDOMRef} />\\r\\n\\r\\n                     \\r\\n                              \\r\\n                              <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                              <Form\\r\\n        onSubmit={this.handleSubmit}\\r\\n          id=\\\"Edit-GST\\\"\\r\\n        >\\r\\n                              \\r\\n                              <div className=\\\"row m-b-20\\\">\\r\\n                              <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                         \\r\\n                         <input type=\\\"number\\\" className=\\\"form-control\\\" placeholder=\\\"Pincode Value*\\\"  id=\\\"add_GST\\\"\\r\\n                         name=\\\"pincode\\\" onChange={ this.onChange } value={ this.state.pincode } />\\r\\n                          {this.validator.message(\\\"Pincode Value\\\", this.state.pincode, \\\"required|numeric|min:5|max:10\\\")}\\r\\n                        \\r\\n                       \\r\\n                         </div>\\r\\n  \\r\\n                              </div>\\r\\n  \\r\\n                                          <div className=\\\"row\\\">\\r\\n                                              <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                  <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n                                                      <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Save\\r\\n                                                          </button>\\r\\n                                                          <Link to=\\\"/PincodeList\\\">\\r\\n                                                                    <button type=\\\"button\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Cancel\\r\\n                                                                    </button>\\r\\n                                                                </Link>\\r\\n                                                   \\r\\n                                                  </div>\\r\\n                                              </div>\\r\\n                                          </div>\\r\\n                                          </Form>\\r\\n                                      </div>\\r\\n                                   \\r\\n  \\r\\n              \\r\\n                                  \\r\\n                          \\r\\n                              </div>\\r\\n                          </div>\\r\\n                      </div>\\r\\n                  </div>\\r\\n              </div>\\r\\n          </div>\\r\\n          </div>\\r\\n           <FooterPage/>\\r\\n      \\r\\n        </div>\\r\\n      );\\r\\n    }\\r\\n  }\\r\\nexport default EditPinCode;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\nimport Pagination from \\\"react-js-pagination\\\";\\r\\n\\r\\n\\r\\n\\r\\n\\r\\nclass ShippingCharges extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        gstList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"ShippingCharges/\\\");\\r\\n\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        // console.log(snapshot);\\r\\n        // console.log(snapshot.val);\\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const GSTData = {\\r\\n            gstId: childSnapShot.key.toString(),\\r\\n            ShippingValue: childSnapShot.val().ShippingValue,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data });\\r\\n        console.log(this.state.gstList);\\r\\n      });\\r\\n}\\r\\n    deleteItem = id => {\\r\\n        \\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase.database().ref(`/PinCode/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n    };\\r\\n\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Settings</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Shipping Charges List</h4>\\r\\n                            {/* <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddPinCode'>Add Pin Code</Link>\\r\\n                            </div> */}\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Shipping Charge</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.gstList && this.state.gstList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.ShippingValue}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditShippingCharge/${data.gstId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         {/* <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.gstId}  onClick={this.deleteItem.bind(this, data.gstId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button> */}\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    {/* <div className=\\\"custom-pagination\\\">\\r\\n                            <Pagination\\r\\n                          hideNavigation\\r\\n                          activePage={this.state.activePage}\\r\\n                          itemsCountPerPage={3}\\r\\n                          totalItemsCount={this.state.countPage}\\r\\n                          pageRangeDisplayed={3}\\r\\n                          onChange={this.handlePageChange.bind(this)}\\r\\n                        />\\r\\n                            </div> */}\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default ShippingCharges;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\n\\r\\nimport { Form } from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\n