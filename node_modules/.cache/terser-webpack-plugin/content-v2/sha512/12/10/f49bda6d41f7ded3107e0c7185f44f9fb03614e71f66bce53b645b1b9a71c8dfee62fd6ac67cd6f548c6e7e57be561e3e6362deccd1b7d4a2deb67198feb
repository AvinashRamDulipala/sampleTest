shot.val();\\r\\n            // console.log(categories)\\r\\n           \\r\\n            this.setState({ gst:gstData.gst,loading: false });\\r\\n            console.log(this.state.gst);\\r\\n          });\\r\\n     \\r\\n  }\\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({ gst: event.target.value });\\r\\n  };\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n    if (this.validator.allValid()) {\\r\\n    const {GstID}=this.props.match.params;\\r\\n    \\r\\n    if (this.state.gst && this.state.gst.trim().length !== 0) {\\r\\n        event.preventDefault();\\r\\n        let dbCon = firebase.database().ref(`/GST/${GstID}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update ({\\r\\n            gst: this.state.gst,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n        this.addNotification();\\r\\n      \\r\\n        \\r\\n\\r\\n      \\r\\n      this.setState({\\r\\n        gst: ''\\r\\n      });\\r\\n      this.props.history.push(\\\"/GSTListPage\\\");\\r\\n    }\\r\\n  } else {\\r\\n    this\\r\\n        .validator\\r\\n        .showMessages();\\r\\n    this.forceUpdate();\\r\\n  }\\r\\n  };\\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\\"bottom\\\",\\r\\n      container: \\\"top-center\\\",\\r\\n      animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n      animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n      dismiss: { duration: 2000 },\\r\\n      dismissable: { click: true }\\r\\n    });\\r\\n  }\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n        <div>\\r\\n       <SidebarPage/>\\r\\n     \\r\\n      <div className=\\\"all-content-wrapper\\\">\\r\\n      <HeaderPage/>\\r\\n     \\r\\n            \\r\\n              <div className=\\\"breadcome-area\\\">\\r\\n                  <div className=\\\"container-fluid\\\">\\r\\