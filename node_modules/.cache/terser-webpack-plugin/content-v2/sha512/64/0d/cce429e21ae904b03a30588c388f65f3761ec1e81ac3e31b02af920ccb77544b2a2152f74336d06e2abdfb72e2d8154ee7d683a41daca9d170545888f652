\\r\\n                }\\r\\n            });\\r\\n    }\\r\\n    optionClicked(optionsList) {\\r\\n        this.setState({multiSelect: optionsList});\\r\\n        console.log(this.state.multiSelect);\\r\\n    }\\r\\n    selectedBadgeClicked(optionsList) {\\r\\n       console.log(optionsList);\\r\\n        var sortedKeys = optionsList.filter(res=>{ \\r\\n         console.log(res);\\r\\n            return res.value; });\\r\\n            console.log(sortedKeys);\\r\\n      \\r\\n        this.setState({ multiSelect: sortedKeys}, () => {\\r\\n            console.log(this.state.multiSelect, 'multiSelect');\\r\\n            });\\r\\n        \\r\\n    }\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"admin_users/\\\")\\r\\n            .orderByChild(\\\"Role\\\")\\r\\n            .equalTo(\\\"Merchant\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n\\r\\n                    label: childSnapShot\\r\\n                        .val()\\r\\n                        .email,\\r\\n                    id: childSnapShot\\r\\n                        .key\\r\\n                        .toString()\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({merchantList: data});\\r\\n            console.log(this.state.merchantList);\\r\\n        });\\r\\n    }\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n        const selectedOptionsStyles = {\\r\\n            color: \\\"#3c763d\\\",\\r\\n            backgroundColor: \\\"#dff0d8\\\"\\r\\n        };\\r\\n        const optionsListStyles = {\\r\\n            backgroundColor: \\\"#dff0d8\\\",\\r\\n            color: \\\"#3c763d\\\"\\r\\n        };\\r\\n\\