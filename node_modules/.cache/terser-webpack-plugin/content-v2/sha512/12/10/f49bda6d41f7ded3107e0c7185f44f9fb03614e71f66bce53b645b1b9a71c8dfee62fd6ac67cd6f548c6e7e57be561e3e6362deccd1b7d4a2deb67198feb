T\\\"\\r\\n        >\\r\\n                              \\r\\n                              <div className=\\\"row m-b-20\\\">\\r\\n                              <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                           \\r\\n                              <input type=\\\"number\\\" className=\\\"form-control\\\" placeholder=\\\"GST Value*\\\"  id=\\\"Edit_GST\\\"\\r\\n                              name=\\\"gst\\\" onChange={ this.onChange } value={ this.state.gst } />\\r\\n           {this.validator.message(\\\"GST Value\\\", this.state.gst, \\\"required|numeric|min:1|max:3\\\")}\\r\\n\\r\\n                             \\r\\n                            \\r\\n                              </div>\\r\\n  \\r\\n                              </div>\\r\\n  \\r\\n                                          <div className=\\\"row\\\">\\r\\n                                              <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                  <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n                                                      <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Save\\r\\n                                                          </button>\\r\\n                                                   \\r\\n                                                  </div>\\r\\n                                              </div>\\r\\n                                          </div>\\r\\n                                          </Form>\\r\\n                                      </div>\\r\\n                                   \\r\\n  \\r\\n              \\r\\n                                  \\r\\n                          \\r\\n                              </div>\\r\\n                          </div>\\r\\n                      </div>\\r\\n                  </div>\\r\\n              </div>\\r\\n          </div>\\r\\n          <