lue', snapshot => {\\r\\n        const data = [];\\r\\n     \\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const GSTData = {\\r\\n            cmsId: childSnapShot.key.toString(),\\r\\n            pageTitle: childSnapShot.val().pageTitle,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data,countPage: data.length,loading: false });\\r\\n        console.log(this.state.gstList);\\r\\n      });\\r\\n}\\r\\n    deleteItem = id => {\\r\\n        \\r\\n\\r\\n    console.log(id);\\r\\n    var playersRef = firebase.database().ref(`/Blog/${id}`);\\r\\n    playersRef.remove();\\r\\n\\r\\n    };\\r\\n\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>Settings</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Blog</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddBlog'>Add Blog</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Title</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.gstList && this.state.gstList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.pageTitle}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditBlog/${data.cmsId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.cmsId}  onClick={this.deleteItem.bind(this, data.cmsId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    <div className=\\\"custom-pagination\\\">\\r\\n                            <Pagination\\r\\n                          hideNavigation\\r\\n                          activePage={this.state.activePage}\\r\\n                          itemsCountPerPage={3}\\r\\n                          totalItemsCount={this.state.countPage}\\r\\n                          pageRangeDisplayed={3}\\r\\n                          onChange={this.handlePageChange.bind(this)}\\r\\n                        />\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\