                                          <th>Action</th>\\r\\n                                                </tr>\\r\\n                                            </thead>\\r\\n                                            <tbody>\\r\\n\\r\\n                                                {this.state.gstList && this\\r\\n                                                    .state\\r\\n                                                    .gstList\\r\\n                                                    .map((data, index) => {\\r\\n                                                        console.log(data);\\r\\n\\r\\n                                                        return (\\r\\n                                                            <tr key={index}>\\r\\n                                                                <td>{index + 1}</td>\\r\\n                                                                <td>{data.pageTitle}</td>\\r\\n                                                                <td className=\\\"img-product\\\"><img src={data.uploadImage} alt=\\\"\\\"/></td>\\r\\n                                                                <td>{data.sliderUrl}</td>\\r\\n                                                                <td>{data.Category}\\r\\n                                                                </td>\\r\\n                                                                <td>\\r\\n                                                                    <select\\r\\n                                                                        name=\\\"status\\\"\\r\\n                                                                        onChange={this.onChange}\\r\\n                                                                        id={data.cmsId}\\r\\n                                                                        className=\\\"form-control pro-edt-select form-control-primary active\\\">\\r\\n                                                                        <option\\r\\n                                                                            value=\\\"Active\\\"\\r\\n                                                                            selected={data.status == \\\"Active\\\"\\r\\n                                                                            ? true\\r\\n                                                                            : false}>Active</option>\\r\\n                                                                        <option\\r\\n                                                                            value=\\\"InActive\\\"\\r\\n                                                                            selected={data.status == \\\"InActive\\\"\\r\\n                                                                            ? true\\r\\n                                                                            : false}>InActive</option>\\r\\n\\r\\n                                                                    </select>\\r\\n                                                                </td>\\r\\n                                                                <td>{data.created_on}\\r\\n                                                                </td>\\r\\n                                                                <td>\\r\\n\\r\\n                                                                    <Link to={`/EditSubSlider/${data.cmsId}`}>\\r\\n                                                                        <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\">\\r\\n                                                                            <i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                        </button>\\r\\n                                                                    </Link>\\r\\n\\r\\n    