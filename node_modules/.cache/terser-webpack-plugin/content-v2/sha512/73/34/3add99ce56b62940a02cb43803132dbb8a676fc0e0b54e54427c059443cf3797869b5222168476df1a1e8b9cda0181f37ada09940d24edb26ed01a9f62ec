                       <Link to=\\\"/RoleListPage\\\">\\r\\n                                                                    <button type=\\\"button\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Cancel\\r\\n                                                                    </button>\\r\\n                                                                </Link>\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                </form>\\r\\n                                            </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\n\\r\\nexport default AddRolePage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\n// import HomePage from \\\"./home\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\nimport swal from 'sweetalert';\\r\\nimport $ from 'jquery';\\r\\n$.DataTable = require( 'datatables.net' );\\r\\n\\r\\n\\r\\n\\r\\nclass RoleListPage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        gstList: [],\\r\\n        loading: false,\\r\\n        count: 1,\\r\\n        activePage: \\\"1\\\",\\r\\n        countPage: \\\"\\\"\\r\\n      };\\r\\n      this.deleteItem = this.deleteItem.bind(this);\\r\\n\\r\\n  }\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.setState({ loading: true });\\r\\n    var ref = firebase.database().ref(\\\"Role/\\\");\\r\\n\\r\\n\\r\\n    ref.on('value', snapshot => {\\r\\n        const data = [];\\r\\n        \\r\\n\\r\\n        snapshot.forEach(childSnapShot => {\\r\\n           \\r\\n          const GSTData = {\\r\\n            gstId: childSnapShot.key.toString(),\\r\\n            Role: childSnapShot.val().Role,\\r\\n            created_on: childSnapShot.val().created_on,\\r\\n          };\\r\\n  \\r\\n          data.push(GSTData);\\r\\n        });\\r\\n  \\r\\n        this.setState({ gstList:data,countPage: data.length,loading: false });\\r\\n        console.log(this.state.gstList);\\r\\n        this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n    });\\r\\n}\\r\\ncomponentWillUnmount() {\\r\\n    clearTimeout(this.interval);\\r\\n  }\\r\\n  deleteItem = id => {\\r\\n    \\r\\n    swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n      if (willDelete) {\\r\\n        console.log(id);\\r\\n        var playersRef = firebase.database().ref(`/Role/${id}`);\\r\\n        playersRef.remove();\\r\\n        \\r\\n  \\r\\n      } else {}\\r\\n      // window.location.reload(\\\"/Home\\\");\\r\\n     \\r\\n  });\\r\\n  \\r\\n    };\\r\\n   \\r\\n\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({ activePage: pageNumber });\\r\\n    }\\r\\n\\r\\n\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n      <div>\\r\\n     <SidebarPage/>\\r\\n   \\r\\n    <div className=\\\"all-content-wrapper\\\">\\r\\n    <HeaderPage/>\\r\\n   \\r\\n          \\r\\n            <div className=\\\"breadcome-area\\\">\\r\\n                <div className=\\\"container-fluid\\\">\\r\\n                    <div className=\\\"row\\\">\\r\\n                        <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                            <div className=\\\"breadcome-list\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                        <div className=\\\"breadcomb-wp\\\">\\r\\n                                        \\r\\n                                            <div className=\\\"breadcomb-ctn\\\">\\r\\n                                            <h2>setting</h2>\\r\\n                                                </div>\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                          \\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n      \\r\\n     \\r\\n            <div className=\\\"product-status mg-b-30\\\">\\r\\n            <div className=\\\"container-fluid\\\">\\r\\n                <div className=\\\"row\\\">\\r\\n                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                        <div className=\\\"product-status-wrap\\\">\\r\\n                            <h4>Role List</h4>\\r\\n                            <div className=\\\"add-product\\\">\\r\\n                                <Link to='/AddRolePage'>Add Role</Link>\\r\\n                            </div>\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Role</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.gstList && this.state.gstList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.Role}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditRolePage/${data.gstId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" data-id={data.gstId}  onClick={this.deleteItem.bind(this, data.gstId)}><i className=\\\"fa fa-trash-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </td>\\r\\n                                         \\r\\n                                         </tr>)\\r\\n\\r\\n                                    } )}\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n\\r\\n                   \\r\\n                    </tbody>\\r\\n                    </table>\\r\\n                    \\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n            </div>\\r\\n        </div> \\r\\n            \\r\\n        </div>\\r\\n        \\r\\n         <FooterPage/>\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default RoleListPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\nimport { Form } from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\n\\r\\n\\r\\n\\r\\n\\r\\nclass EditRolePage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        Role:'',\\r\\n        dashboard: '',\\r\\n            orders: '',\\r\\n            products: '',\\r\\n            category: '',\\r\\n            subCategory: '',\\r\\n            invoices: '',\\r\\n            abandonedCart: '',\\r\\n            coupons: '',\\r\\n            merchantPayments: '',\\r\\n            AdminStaff: '',\\r\\n            merchantMangement: '',\\r\\n            notifications: '',\\r\\n            users: '',\\r\\n            settings: '',\\r\\n            Analytics: '',\\r\\n            sliders: '',\\r\\n            enquiry: '',\\r\\n        date:new Date().toLocaleString(),\\r\\n\\r\\n    }\\r\\n    this.addNotification = this.addNotification.bind(this);\\r\\n    this.notificationDOMRef = React.createRef();\\r\\n    this.validator = new SimpleReactValidator({\\r\\n      className: \\\"text-danger\\\",\\r\\n      validators: {\\r\\n\\r\\n          whitespace: {\\r\\n              message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          specialChar: {\\r\\n              message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          },\\r\\n          specialCharText: {\\r\\n              message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n              rule: function (val, params, validator) {\\r\\n                  // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                  // params.indexOf(val) === -1\\r\\n                  return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          }\\r\\n      }\\r\\n  });\\r\\n    \\r\\n\\r\\n  }\\r\\n  componentDidMount(){\\r\\n      const {GstID}=this.props.match.params;\\r\\n      console.log(GstID);\\r\\n      \\r\\n      var ref = firebase.database().ref(`Role/${GstID}`);\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            var gstData = snapshot.val();\\r\\n            // console.log(categories)\\r\\n           \\r\\n            this.setState({ Role:gstData.Role,\\r\\n                dashboard: gstData.dashboard,\\r\\n                orders: gstData.orders,\\r\\n                products: gstData.products,\\r\\n                category: gstData.category,\\r\\n                subCategory: gstData.subCategory,\\r\\n                invoices: gstData.invoices,\\r\\n                abandonedCart: gstData.abandonedCart,\\r\\n                coupons: gstData.coupons,\\r\\n                merchantPayments: gstData.merchantPayments,\\r\\n                AdminStaff: gstData.AdminStaff,\\r\\n                merchantMangement: gstData.merchantMangement,\\r\\n                notifications: gstData.notifications,\\r\\n                users: gstData.users,\\r\\n                settings: gstData.settings,\\r\\n                Analytics: gstData.Analytics,\\r\\n                sliders: gstData.sliders,\\r\\n                enquiry: gstData.enquiry,\\r\\n                loading: false });\\r\\n            console.log(this.state.Role);\\r\\n          });\\r\\n     \\r\\n  }\\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({\\r\\n        [event.target.name]: event.target.value\\r\\n    });\\r\\n};\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n    if (this.validator.allValid()) {\\r\\n    const {GstID}=this.props.match.params;\\r\\n    \\r\\n    if (this.state.Role && this.state.Role.trim().length !== 0) {\\r\\n        event.preventDefault();\\r\\n        let dbCon = firebase.database().ref(`/Role/${GstID}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update ({\\r\\n            Role: this.state.Role,\\r\\n            dashboard: this.state.dashboard,\\r\\n            orders: this.state.orders,\\r\\n            products: this.state.products,\\r\\n            category: this.state.category,\\r\\n            subCategory: this.state.subCategory,\\r\\n            invoices: this.state.invoices,\\r\\n            abandonedCart: this.state.abandonedCart,\\r\\n            coupons: this.state.coupons,\\r\\n            merchantPayments: this.state.merchantPayments,\\r\\n            AdminStaff: this.state.AdminStaff,\\r\\n            merchantMangement: this.state.merchantMangement,\\r\\n            notifications: this.state.notifications,\\r\\n            users: this.state.users,\\r\\n            settings: this.state.settings,\\r\\n            Analytics: this.state.Analytics,\\r\\n            sliders: this.state.sliders,\\r\\n            enquiry: this.state.enquiry,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n        this.addNotification();\\r\\n      \\r\\n        \\r\\n\\r\\n      \\r\\n      this.setState({\\r\\n        Role: ''\\r\\n      });\\r\\n      this.props.history.push(\\\"/RoleListPage\\\");\\r\\n    }\\r\\n  } else {\\r\\n    this\\r\\n        .validator\\r\\n        .showMessages();\\r\\n    this.forceUpdate();\\r\\n  }\\r\\n  };\\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\\"bottom\\\",\\r\\n      container: \\\"top-center\\\",\\r\\n      animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n      animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n      dismiss: { duration: 2000 },\\r\\n      dismissable: { click: true }\\r\\n    });\\r\\n  }\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n        <div>\\r\\n       <SidebarPage/>\\r\\n     \\r\\n      <div className=\\\"all-content-wrapper\\\">\\r\\n      <HeaderPage/>\\r\\n     \\r\\n            \\r\\n              <div className=\\\"breadcome-area\\\">\\r\\n                  <div className=\\\"container-fluid\\\">\\r\\n                      <div className=\\\"row\\\">\\r\\n                          <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                              <div className=\\\"breadcome-list\\\">\\r\\n                                  <div className=\\\"row\\\">\\r\\n                                      <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                          <div className=\\\"breadcomb-wp\\\">\\r\\n                                          \\r\\n                                              <div className=\\\"breadcomb-ctn\\\">\\r\\n                                              <h2>Role</h2>\\r\\n                                                  </div>\\r\\n                                          </div>\\r\\n                                      </div>\\r\\n                            \\r\\n                                  </div>\\r\\n                              </div>\\r\\n                          </div>\\r\\n                      </div>\\r\\n                  </div>\\r\\n              </div>\\r\\n        \\r\\n       \\r\\n              <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n            \\r\\n              <div className=\\\"single-pro-review-area\\\">\\r\\n                  <div className=\\\"container-fluid\\\">\\r\\n                      <div className=\\\"row\\\">\\r\\n                          <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                              <div className=\\\"review-tab-pro-inner\\\">\\r\\n                     \\r\\n                     <h4>Edit Role</h4>\\r\\n                     <ReactNotification ref={this.notificationDOMRef} />\\r\\n\\r\\n                     \\r\\n                              \\r\\n                              <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                              <Form\\r\\n        onSubmit={this.handleSubmit}\\r\\n          id=\\\"Edit-Role\\\"\\r\\n        >\\r\\n                              \\r\\n                              <div className=\\\"row m-b-20\\\">\\r\\n                              <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                              <label >Role<span style={{\\r\\n                color: 'red'\\r\\n            }}>*</span>\\r\\n                                                            </label>\\r\\n                              <input type=\\\"text\\\" className=\\\"form-control\\\" placeholder=\\\"Role Value*\\\"  id=\\\"Edit_Role\\\"\\r\\n                              name=\\\"Role\\\" onChange={ this.onChange } value={ this.state.Role } />\\r\\n           {this.validator.message(\\\"Role Value\\\", this.state.Role, \\\"required|whitespace|specialChar|min:3|max:70\\\")}\\r\\n\\r\\n                             \\r\\n                            \\r\\n                              </div>\\r\\n  \\r\\n                              </div>\\r\\n                              <h4>Access To</h4>\\r\\n                                                    <div className=\\\"row\\\">\\r\\n                                                        <div className=\\\"form-group\\\">\\r\\n                                                            <label htmlFor=\\\"inputEmail3\\\" className=\\\"col-sm-3 control-label\\\">Dashboard</label>\\r\\n                                                            <div className=\\\"col-sm-9\\\">\\r\\n                                                                <label className=\\\"radio-inline \\\">\\r\\n                                                                    <input\\r\\n                                                                        type=\\\"radio\\\"\\r\\n                                                                        name=\\\"dashboard\\\"\\r\\n                                                                        value=\\\"read\\\"\\r\\n                                                                        onChange={this.onChange}\\r\\n                                                                        checked={this.state.dashboard === \\\"read\\\"}\\r\\n                                                                        className=\\\"icheck\\\"/>\\r\\n                                                                    Read &nbsp;&nbsp;</label>\\r\\n                                                                <label className=\\\"radio-inline \\\">\\r\\n                                                                    <input\\r\\n                                                                        type=\\\"radio\\\"\\r\\n       