                     </thead>\\r\\n                                            <tbody>\\r\\n\\r\\n                                                {this.state.gstList && this\\r\\n                                                    .state\\r\\n                                                    .gstList\\r\\n                                                    .map((data, index) => {\\r\\n                                                        console.log(data);\\r\\n\\r\\n                                                        return (\\r\\n                                                            <tr key={index}>\\r\\n                                                                <td>{index + 1}</td>\\r\\n                                                                <td>{data.gst}</td>\\r\\n                                                                <td>{data.created_on}\\r\\n                                                                </td>\\r\\n                                                                <td>\\r\\n\\r\\n                                                                    <Link to={`/EditGSTPage/${data.gstId}`}>\\r\\n                                                                        <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\">\\r\\n                                                                            <i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                        </button>\\r\\n                                                                    </Link>\\r\\n\\r\\n                                                                    <button\\r\\n                                                                        data-toggle=\\\"tooltip\\\"\\r\\n                                                                        title=\\\"Trash\\\"\\r\\n                                                         