    snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                    const Merchants = {\\r\\n                        Sno: i++,\\r\\n                        orderId: childSnapShot\\r\\n                            .val()\\r\\n                            .orderId,\\r\\n                        customerName: childSnapShot\\r\\n                            .val()\\r\\n                            .first_name + \\\" \\\" + childSnapShot\\r\\n                            .val()\\r\\n                            .last_name,\\r\\n                        items: childSnapShot\\r\\n                            .val()\\r\\n                            .cartDataItems\\r\\n                            .length,\\r\\n                        totalAmount: childSnapShot\\r\\n                            .val()\\r\\n                            .totalAmount,\\r\\n                        status: childSnapShot\\r\\n                            .val()\\r\\n                            .status,\\r\\n                        order_on: childSnapShot\\r\\n                            .val()\\r\\n                            .created_on,\\r\\n                        emailId: childSnapShot\\r\\n                            .val()\\r\\n                            .email_id\\r\\n                    };\\r\\n\\r\\n                    data.push(Merchants);\\r\\n                });\\r\\n                this.setState({orderDateList: data, loading: false});\\r\\n                console.log(this.state.orderDateList);\\r\\n\\r\\n                this\\r\\n                    .csvLink\\r\\n                    .link\\r\\n                    .click();\\r\\n\\r\\n            });\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n    render() {\\r\\n        \\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\