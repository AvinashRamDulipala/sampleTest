 $ from 'jquery';\\r\\n$.DataTable = require('datatables.net');\\r\\n\\r\\nclass Coupons extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            gstList: [],\\r\\n            loading: false,\\r\\n            count: 1,\\r\\n            activePage: \\\"1\\\",\\r\\n            countPage: \\\"\\\",\\r\\n            usePermList: \\\"\\\"\\r\\n        };\\r\\n\\r\\n    }\\r\\n    componentWillMount() {\\r\\n        var sessionId = sessionStorage.getItem(\\\"userRole\\\");\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"Role\\\")\\r\\n            .orderByChild(\\\"Role\\\")\\r\\n            .equalTo(sessionId);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const orderStatus = {\\r\\n                    RoleId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n\\r\\n                    coupons: childSnapShot\\r\\n                        .val()\\r\\n                        .coupons\\r\\n                };\\r\\n\\r\\n                data.push(orderStatus);\\r\\n            });\\r\\n\\r\\n            this.setState({usePermList: data[0].coupons, loading: false});\\r\\n            console.log(this.state.usePermList);\\r\\n\\r\\n        });\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"Coupons/\\\")\\r\\n            .orderByChild(\\\"sessionId\\\")\\r\\n            .equalTo(sessionId);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n                    cmsId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    couponCode: childSnapShot\\r\\n                        .val()\\r\\n                        .couponCode,\\r\\n                    couponDescription: childSnapShot\\r\\n                        .val()\\r\\n                        .couponDescription,\\r\\n                    type: childSnapShot\\r\\n                        .val()\\r\\n                        .type,\\r\\n                    validFrom: childSnapShot\\r\\n                        .val()\\r\\n                        .validFrom,\\r\\n                    validTo: childSnapShot\\r\\n                        .val()\\r\\n                        .validTo,\\r\\n                    numberOfCoupons: childSnapShot\\r\\n                        .val()\\r\\n                        .numberOfCoupons,\\r\\n                    usageType: childSnapShot\\r\\n                        .val()\\r\\n                        .usageType,\\r\\n                    numberOfUsages: childSnapShot\\r\\n                        .val()\\r\\n                        .numberOfUsages,\\r\\n                    offerValue: childSnapShot\\r\\n                        .val()\\r\\n                        .offerValue,\\r\\n                    offerType: childSnapShot\\r\\n                        .val()\\r\\n                        .offerType,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({gstList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.gstList);\\r\\n\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destroy(true);\\r\\n        clearTimeout(this.interval);\\r\\n    }\\r\\n    deleteItem = id => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(id);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/Coupons/${id}`);\\r\\n                playersRef.remove();\\r\\n\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Coupons/Deleted\\\");\\r\\n\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n    }\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({activePage: pageNumber});\\r\\n    }\\r\\n\\r\\n    render() {\\r\\n\\r\\n        var pathname = this\\r\\n            .props\\r\\n            .location\\r\\n            .pathname\\r\\n            .split(\\\"/\\\");\\r\\n        var alertName = pathname[2];\\r\\n        console.log(alertName);\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                     