              message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        const {locationId} = this.props.match.params;\\r\\n        console.log(locationId);\\r\\n        var sessionId = sessionStorage.getItem(\\\"RoleId\\\"); \\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`designation/${locationId}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var categories = snapshot.val();\\r\\n\\r\\n            // console.log(categories)\\r\\n\\r\\n            this.setState({designation: categories.designation, sessionId:sessionId,loading: false});\\r\\n            console.log(this.state.location);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref('designation/')\\r\\n            .orderByChild(\\\"designation\\\")\\r\\n            .equalTo(event.target.value);\\r\\n        ref.on('value', snapshot => {\\r\\n            var user_exist = snapshot.numChildren();\\r\\n            console.log(user_exist);\\r\\n\\r\\n            if (user_exist > 0 && this.state.validError!=true) {\\r\\n\\r\\n                this.setState({error_message: \\\"designation Name is already exist\\\", validError: false});\\r\\n            } else {\\r\\n                this.setState({error_message: \\\"\\\", validError: true});\\r\\n\\r\\n            }\\r\\n\\r\\n        })\\r\\n\\r\\n    };\\r\\n   \\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid() && this.state.validError == true) {\\r\\n         \\r\\n\\r\\n                var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n               \\r\\n\\r\\n                const {locationId} = this.props.match.params;\\r\\n            let dbCon = firebase\\r\\n                .database()\\r\\n                .ref(`/designation/${locationId}`);\\r\\n                dbCon\\r\\n                    .update({designation: this.state.designation, \\r\\n                        created_on: this.state.date, sessionId: sessionId})\\r\\n                    .then(response => {\\r\\n                        try {\\r\\n                            console.log(response);\\r\\n\\r\\n                            this\\r\\n                                .props\\r\\n                                .history\\r\\n                                .push(\\\"/DesignationList/Updated\\\");\\r\\n\\r\\n                        } catch (error) {\\r\\n                            console.log(error.config);\\r\\n                        }\\r\\n                    });\\r\\n\\r\\n            \\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Designations</h2>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit Designation</h4>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Add-category\\\">\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\