                                       {this.state.stateName > 0 && this.state.stateName.map((data) =>{\\r\\n                                       \\r\\n                                       \\r\\n                                       return(\\r\\n                                           <option value={data} key={data}>{data}</option>\\r\\n                                       )\\r\\n                                       {this.validator.message(\\r\\n                                        \\\"State\\\",\\r\\n                                        data,\\r\\n                                        \\\"required\\\"\\r\\n                                      )}\\r\\n\\r\\n                                   } )}                                      \\r\\n                                                     </select>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>City Name<span style={{color: 'red' }}>*</span></label>\\r\\n                                                        <select\\r\\n                                                            name=\\\"selectCityName\\\"\\r\\n                                                            className=\\\"form-control pro-edt-select form-control-primary\\\" name=\\\"name\\\"\\r\\n                                                            onChange={this.setCityName}>\\r\\n\\r\\n                                                            <option value=\\\"\\\">Select City Name</option>\\r\\n                                                            {this.state.cityName > 0&& this.state.cityName.map((data) =>{\\r\\n                                       \\r\\n                                       \\r\\n                                       return(\\r\\n                                           <option value={data} key={data}>{data}</option>\\r\\n                                       )\\r\\n                                       {this.validator.message(\\r\\n                                        \\\"Country\\\",\\r\\n                                        data,\\r\\n                                        \\\"required\\\"\\r\\n                                      )}\\r\\n\\r\\n                                   } )}                                      \\r\\n                                                     </select>\\r\\n\\r\\n                                                        {/* <input type=\\\"text\\\" className=\\\"form-control\\\" placeholder=\\\"\\\"/> */}\\r\\n\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                                <div className=\\\"row m-b-20\\\">\\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>Pincode<span style={{color: 'red' }}>*</span></label>\\r\\n                                                        <input type=\\\"text\\\" className=\\\"form-control\\\" placeholder=\\\"\\\"  name=\\\"pincode\\\"\\r\\n                                                            onChange={this.onChange}\\r\\n                                                            value={this.state.pincode}/>\\r\\n                                 {this.validator.message(\\\"ZIP Code\\\", this.state.zip, \\\"required|min:5|max:6\\\")}\\r\\n\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>Address<span style={{color: 'red' }}>*</span></label>\\r\\n                                                       