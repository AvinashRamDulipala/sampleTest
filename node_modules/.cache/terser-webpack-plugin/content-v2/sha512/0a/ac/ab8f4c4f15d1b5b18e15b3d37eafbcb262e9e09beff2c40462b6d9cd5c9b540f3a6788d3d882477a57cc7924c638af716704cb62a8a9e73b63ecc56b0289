\\\">\\r\\n                                    <div className=\\\"product-status-wrap\\\">\\r\\n                                        <h4>Admin Staff</h4>\\r\\n                                        <div className=\\\"add-product\\\">\\r\\n                                            <Link to='/AddAdminStaff'>Add Admin Staff</Link>\\r\\n                                        </div>\\r\\n                                        {alertName\\r\\n                                            ? <div\\r\\n                                                    className=\\\"alert alert-success display-none\\\"\\r\\n                                                    style={{\\r\\n                                                    display: \\\"block\\\"\\r\\n                                                }}>\\r\\n                                                    <a data-dismiss=\\\"alert\\\" href=\\\"#\\\" aria-hidden=\\\"true\\\" className=\\\"close\\\"></a>\\r\\n                                                    Admin Staff {alertName}\\r\\n                                                    Successfully\\r\\n                                                </div>\\r\\n                                            : ''}\\r\\n                                        <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                                            <thead>\\r\\n                                                <tr>\\r\\n                                                    <th>S.No</th>\\r\\n                                                    <th>Full Name</th>\\r\\n                                                    <th>Email</th>\\r\\n                                                    <th>Username</th>\\r\\n                                                    <th>Role</th>\\r\\n                                                    <th>Status</th>\\r\\n                                                    <th>Created on</th>\\r\\n                                                    <th>Action</th>\\r\\n                                                </tr>\\r\\n                                            </thead>\\r\\n                                            <tbody>\\r\\n\\r\\n                                                {this.state.gstList && this\\r\\n                                                    .state\\r\\n                                                    .gstList\\r\\n                                                    .map((data, index) => {\\r\\n                                                        console.log(data);\\r\\n\\r\\n                                                        return (\\r\\n                                                            <tr key={index}>\\r\\n                                                                <td>{index + 1}</td>\\r\\n                                                                <td>{data.fullName}</td>\\r\\n                                                                <td>{data.email}\\r\\n                                                                </td>\\r\\n                                                                <td>{data.username}</td>\\r\\n                                                                <td>{data.Role}</td>\\r\\n                                                                <td>\\r\\n                                                                    <select\\r\\n                                                                        name=\\\"status\\\"\\r\\n                                                                        onChange={this.onChange}\\r\\n                                                                        id={data.cmsId}\\r\\n                                                                        className=\\\"form-control pro-edt-select form-control-primary active\\\">\\r\\n                                                                        <option\\r\\n                            