                                                   name=\\\"Country\\\"\\r\\n                                                            onChange={this.setCountryName}>\\r\\n\\r\\n                                                            <option >Select Country</option>\\r\\n                                                            {this.state.countryName.map((data,index) =>{\\r\\n                                       \\r\\n                                       \\r\\n                                        return(\\r\\n                                            <option value={data.name} id={data.shortName} key={index}>{data.name}</option>\\r\\n                                        )\\r\\n                                        {this.validator.message(\\r\\n                                            \\\"Country\\\",\\r\\n                                            data.name,\\r\\n                                            \\\"required\\\"\\r\\n                                          )}\\r\\n\\r\\n                                    } )}\\r\\n                                  \\r\\n                                                        </select>\\r\\n\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n\\r\\n                                                <div className=\\\"row m-b-20\\\">\\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>Select State</label>\\r\\n                                                        <select\\r\\n                                                            name=\\\"select\\\"\\r\\n                                                            className=\\\"form-control pro-edt-select form-control-primary\\\" name=\\\"name\\\"\\r\\n                                                            onChange={this.setStateName}>\\r\\n\\r\\n                                                            <option value=\\\"\\\">Select State Name</option>\\r\\n                                                            {this.state.stateName && this.state.stateName.map((data) =>{\\r\\n                                       \\r\\n                                       \\r\\n                                       return(\\r\\n                                           <option value={data} key={data}>{data}</option>\\r\\n                                       )\\r\\n                                       {this.validator.message(\\r\\n                                        \\\"State\\\",\\r\\n                                        data,\\r\\n                                        \\\"required\\\"\\r\\n                                      )}\\r\\n\\r\\n                                   } )}                                      \\r\\n                                                     </select>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>City Name</label>\\r\\n                                                        <select\\r\\n                                                            name=\\\"selectCityName\\\"\\r\\n                                                            className=\\\"form-control pro-edt-select form-control-primary\\\" name=\\\"name\\\"\\r\\n                                                            onChange={this.setCityName}>\\r\\n\\r\\n                                                            <option value=\\\"\\\">Select City Name</option>\\r\\n                                                            {this.state.cityName && this.state.cityName.map((data) =>{\\r\\n                                       \\r\\n                                      