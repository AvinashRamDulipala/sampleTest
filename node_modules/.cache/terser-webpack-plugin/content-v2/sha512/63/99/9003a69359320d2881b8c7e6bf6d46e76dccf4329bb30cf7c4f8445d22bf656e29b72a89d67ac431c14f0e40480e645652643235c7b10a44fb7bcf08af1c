';\\r\\nimport $ from 'jquery';\\r\\n import {getDesignations} from './SelectDropdown';\\r\\n$.DataTable = require('datatables.net');\\r\\n\\r\\nclass DesignationList extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.datatable = null;\\r\\n        this.state = {\\r\\n            designationList: [],\\r\\n            loading: false,\\r\\n            count: 1,\\r\\n            activePage: \\\"1\\\",\\r\\n            countPage: \\\"\\\",\\r\\n            usePermList: \\\"\\\"\\r\\n        };\\r\\n\\r\\n    }\\r\\n   \\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        //  console.log(getDesignations);\\r\\n\\r\\n         \\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"designation/\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            // console.log(snapshot); console.log(snapshot.val);\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const Categories = {\\r\\n                    locationId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                        designation: childSnapShot\\r\\n                        .val()\\r\\n                        .designation,\\r\\n                   \\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.unshift(Categories);\\r\\n            });\\r\\n\\r\\n            this.setState({designationList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.designationList);\\r\\n            // this.$el = $(this.el);\\r\\n\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destro