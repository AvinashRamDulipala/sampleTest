               \\r\\n                                                            <span\\r\\n                                                                style={{\\r\\n                                                                color: 'red'\\r\\n                                                            }}/>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"form-group setting-row\\\">\\r\\n                                                        <label htmlFor=\\\"name\\\" className=\\\"col-sm-3 control-label\\\">Offer</label>\\r\\n                                                        <div className=\\\"col-sm-3\\\">\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                placeholder=\\\"Offer\\\"\\r\\n                                                                name=\\\"offerValue\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.offerValue}\\r\\n                                                                id=\\\"offerValue\\\"\\r\\n                                                                className=\\\"form-control required\\\"/>\\r\\n                                                        </div>\\r\\n                                                        <div className=\\\"col-sm-6\\\">\\r\\n                                                            <div>\\r\\n                                                                <label className=\\\"radio-inline no-padd\\\">\\r\\n                                                                    <input\\r\\n                                 