ast 6 and at most 30 with 1 numeric,1 special charac\\\" +\\r\\n                            \\\"ter and 1 alphabet.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^(?=.*\\\\d)(?=.*[!@#$%^&*])(?=.*[a-z]).{6,30}$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                passwordMismatch: {\\r\\n                    message: \\\"confirm password must match with password field.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        return document\\r\\n                            .getElementById(\\\"password_input\\\")\\r\\n                            .value === val\\r\\n                            ? true\\r\\n                            : false;\\r\\n                    }\\r\\n                },\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n\\r\\n                zip: {\\r\\n                    message: \\\"Invalid Pin Code\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^(\\\\d{5}(\\\\d{4})?)?$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                website: {\\r\\n                    message: \\\"The Url should be example.com \\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                Fax: {\\r\\n                    message: \\\"Invalid fax number \\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        return (validator.helpers.testRegex(val, /^([\\\\w-\\\\.]+)@((\\\\[[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.)|(([\\\\w-]+\\\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\\\\]?)$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        \\r\\n        const {CMSID} = this.props.match.params;\\r\\n      \\r\\n        this.setState({ loading: true });\\r\\n        var ref = firebase.database().ref(`MerchantAccountDetails/${CMSID}`);\\r\\n       \\r\\n        \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            if(snapshot.numChildren()>0){\\r\\n\\r\\n           \\r\\n            var accountDetails = snapshot.val();\\r\\n            console.log(accountDetails);\\r\\n\\r\\n          \\r\\n      \\r\\n            this.setState({  \\r\\n                businessName: accountDetails.businessName ,\\r\\n                gstValue: accountDetails.gstValue,\\r\\n                tanValue: accountDetails.tanValue,\\r\\n                signature: accountDetails.avatarURL ,\\r\\n                mailingAddress: accountDetails.mailingAddress,\\r\\n                pickupAddress: accountDetails.pickupAddress,\\r\\n                pincode: accountDetails.pincode,\\r\\n                city: accountDetails.city,\\r\\n                state: accountDetails.state,\\r\\n                emailAddress: accountDetails.emailAddress,\\r\\n    \\r\\n                accountName: accountDetails.accountName,\\r\\n                accountNumber: accountDetails.accountNumber,\\r\\n                ifscCode: accountDetails.ifscCode,\\r\\n                bankName: accountDetails.bankName,\\r\\n                branchName: accountDetails.branchName,\\r\\n                cityName: accountDetails.cityName,\\r\\n                stateName: accountDetails.stateName,\\r\\n    \\r\\n                businessType: accountDetails.businessType,\\r\\n                personalPAN: accountDetails.personalPAN,\\r\\n                addressProof: accountDetails.addressProof,\\r\\n                cancelledCheque: accountDetails.cancelledCheque,\\r\\n                displayName: accountDetails.displayName,\\r\\n                businessDesc: accountDetails.businessDesc,\\r\\n                productType: accountDetails.productType,\\r\\n                holidays: accountDetails.holidays,\\r\\n                productNames: accountDetails.productNames,\\r\\n                place: accountDetails.place,\\r\\n                avatarURL: accountDetails.avatarURL,\\r\\n                loading: false ,\\r\\n                countNumber:accountDetails});\\r\\n            }\\r\\n            else{\\r\\n                \\r\\n            }\\r\\n            \\r\\n          });\\r\\n          \\r\\n\\r\\n          \\r\\n\\r\\n        }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n\\r\\n        console.log(event.target.name +\\\"=\\\"+ event.target.value);\\r\\n       \\r\\n    };\\r\\n    \\r\\n    handleChange = (value) => {\\r\\n        console.log(value);\\r\\n        this.setState({description: value})\\r\\n    };\\r\\n\\r\\n    handleUploadStart = () => this.setState({isUploading: true, uploadProgress: 0});\\r\\n    handleFrontImageUploadStart = () => this.setState({isUploading: true, uploadProgress: 0, avatarURL: ''});\\r\\n    handleProgress = progress => this.setState({uploadProgress: progress});\\r\\n\\r\\n    handleUploadError = error => {\\r\\n        this.setState({\\r\\n            isUploading: false\\r\\n            // Todo: handle error\\r\\n        });\\r\\n        console.error(error);\\r\\n    };\\r\\n    handleUploadSuccess = filename => {\\r\\n        this.setState({ avatar: filename, progress: 100, isUploading: false });\\r\\n        firebase\\r\\n          .storage()\\r\\n          .ref(\\\"images\\\")\\r\\n          .child(filename)\\r\\n          .getDownloadURL()\\r\\n          .then(url => this.setState({ avatarURL: url }));\\r\\n      };\\r\\n    handlePanSuccess = (filename) => {\\r\\n\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({personalPAN: url}));\\r\\n    };\\r\\n    handleAddressProofSuccess = (filename) => {\\r\\n\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({addressProof: url}));\\r\\n    };\\r\\n    handleChequeSuccess = (filename) => {\\r\\n\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({cancelledCheque: url}));\\r\\n    };\\r\\n    \\r\\n\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n         if (this.validator.allValid()) {\\r\\n           \\r\\n           \\r\\n            var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n            var userName = sessionStorage.getItem(\\\"userName\\\");\\r\\n\\r\\n            var ref = firebase\\r\\n            .database()\\r\\n            .ref(`MerchantAccountDetails/${sessionId}`);\\r\\n           console.log(this.state.businessName);\\r\\n            ref.update({\\r\\n           \\r\\n                businessName: this.state.businessName ,\\r\\n                gstValue: this.state.gstValue,\\r\\n                tanValue: this.state.tanValue,\\r\\n                signature: this.state.avatarURL ,\\r\\n                mailingAddress: this.state.mailingAddress,\\r\\n                pickupAddress: this.state.pickupAddress,\\r\\n                pincode: this.state.pincode,\\r\\n                city: this.state.city,\\r\\n                state: this.state.state,\\r\\n                emailAddress: this.state.emailAddress,\\r\\n    \\r\\n                accountName: this.state.accountName,\\r\\n                accountNumber: this.state.accountNumber,\\r\\n                ifscCode: this.state.ifscCode,\\r\\n                bankName: this.state.bankName,\\r\\n                branchName: this.state.branchName,\\r\\n                cityName: this.state.cityName,\\r\\n                stateName: this.state.stateName,\\r\\n    \\r\\n                businessType: this.state.businessType,\\r\\n                personalPAN: this.state.personalPAN,\\r\\n                addressProof: this.state.addressProof,\\r\\n                cancelledCheque: this.state.cancelledCheque,\\r\\n                displayName: this.state.displayName,\\r\\n                businessDesc: this.state.businessDesc,\\r\\n                productType: this.state.productType,\\r\\n                holidays: this.state.holidays,\\r\\n                productNames: this.state.productNames,\\r\\n                place: this.state.place,\\r\\n                avatarURL: this.state.avatarURL,\\r\\n                created_on: this.state.date,\\r\\n                sessionId: sessionId\\r\\n            });\\r\\n            this\\r\\n                .props\\r\\n                .history\\r\\n                .push(\\\"/Dashboard\\\");\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n        const divStyle = {\\r\\n            height: \\\"100px\\\",\\r\\n            width: \\\"150px\\\"\\r\\n        };\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\