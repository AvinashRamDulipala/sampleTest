f(\\\"images\\\")\\r\\n          .child(filename)\\r\\n          .getDownloadURL()\\r\\n          .then(url => this.setState({ avatarURL: url }));\\r\\n      };\\r\\n    handlePanSuccess = (filename) => {\\r\\n\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({personalPAN: url}));\\r\\n    };\\r\\n    handleAddressProofSuccess = (filename) => {\\r\\n\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({addressProof: url}));\\r\\n    };\\r\\n    handleChequeSuccess = (filename) => {\\r\\n\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({cancelledCheque: url}));\\r\\n    };\\r\\n    \\r\\n\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n         if (this.validator.allValid()) {\\r\\n           \\r\\n           \\r\\n            var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n            var userName = sessionStorage.getItem(\\\"userName\\\");\\r\\n\\r\\n            var ref = firebase\\r\\n            .database()\\r\\n            .ref(`MerchantAccountDetails/${sessionId}`);\\r\\n           console.log(this.state.businessName);\\r\\n            ref.update({\\r\\n           \\r\\n                businessName: this.state.businessName ,\\r\\n                gstValue: this.state.gstValue,\\r\\n                tanValue: this.state.tanValue,\\r\\n                signature: this.state.avatarURL ,\\r\\n                mailingAddress: this.state.mailingAddress,\\r\\n                pickupAddress: this.state.pickupAddress,\\r\\n                pincode: this.state.pincode,\\r\\n              