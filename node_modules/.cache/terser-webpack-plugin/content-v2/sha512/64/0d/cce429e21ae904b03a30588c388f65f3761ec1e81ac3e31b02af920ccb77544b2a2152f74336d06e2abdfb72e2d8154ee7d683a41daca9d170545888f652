                                           type=\\\"radio\\\"\\r\\n                                                                                    name=\\\"notification_type\\\"\\r\\n                                                                                    value=\\\"Users\\\"\\r\\n                                                                                    onChange={this.onChange}\\r\\n                                                                                    checked={this.state.notification_type === 'Users'}/>\\r\\n                                                                                Users\\r\\n                                                                            </label>\\r\\n                                                                        </div>\\r\\n\\r\\n                                                                        <div className=\\\"col-sm-3\\\">\\r\\n                                                                            <label className=\\\"radio-inline no-padd\\\">\\r\\n                                                                                <input\\r\\n                                                                                    type=\\\"radio\\\"\\r\\n                                                                                    name=\\\"notification_type\\\"\\r\\n                                                                                    value=\\\"Merchants\\\"\\r\\n                                                                                    onChange={this.onChange}\\r\\n                                                                                    checked={this.state.notification_type === 'Merchants'}/>\\r\\n                                                                                Merchants\\r\\n                                                                            </label>\\r\\n                                                             