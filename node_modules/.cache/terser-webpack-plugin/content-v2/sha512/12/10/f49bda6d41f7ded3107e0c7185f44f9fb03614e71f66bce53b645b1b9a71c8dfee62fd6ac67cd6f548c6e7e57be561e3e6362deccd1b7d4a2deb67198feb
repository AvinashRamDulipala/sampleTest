        'underline',\\r\\n        'strike',\\r\\n        'blockquote',\\r\\n        'list',\\r\\n        'bullet',\\r\\n        'indent',\\r\\n        'link',\\r\\n        'image',\\r\\n        'color',\\r\\n        'background'\\r\\n    ];\\r\\n    componentDidMount() {\\r\\n        const {CMSID} = this.props.match.params;\\r\\n        console.log(CMSID);\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`FooterLinks/${CMSID}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var gstData = snapshot.val();\\r\\n            console.log(gstData)\\r\\n            this.setState({pageTitle: gstData.pageTitle,pageName: gstData.pageName, content: gstData.content, loading: false});\\r\\n            //console.log(this.state.pageTitle);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({  [event.target.name]: event.target.value});\\r\\n    };\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n    handleSubmit = (event) => {\\r\\n        if (this.validator.allValid()) {\\r\\n            const {CMSID} = this.props.match.params;\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0) {\\r\\n                event.preventDefault();\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref(`/FooterLinks/${CMSID}`);\\r\\n                console.log(dbCon);\\r\\n                dbCon.update({pageTitle: this.state.pageTitle,  pageName: this.state.pageName, content: this.state.content, created_on: this.state.date});\\r\\n\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/FooterLinks\\\");\\r\\n\\r\\n            }\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Settings</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Edit Footer Links Page</h4>\\r\\n\\r\\n                                            <ReactNotification ref={this.notificationDOMRef}/>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Edit-GST\\\">\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Page Title*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageTitle\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageTitle}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Title\\\", this.state.pageTitle, \\\"required|specialChar|min:3|max:70\\\")}\\r\\n                                                            <div className=\\\"text-danger\\\">\\r\\n                                                                {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Page Name*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageName\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageName}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Name\\\", this.state.pageName, \\\"required|specialChar|min:3|max:70\\\")}\\r\\n                                                            <div className=\\\"text-danger\\\">\\r\\n                                                                {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <ReactQuill\\r\\n                                                                theme={this.state.theme}\\r\\n                                                                value={this.state.content}\\r\\n                                                                modules={this.modules}\\r\\n                                                                formats={this.formats}\\r\\n                                                                placeholder='Enter Text'\\r\\n                                                                onChange={this.handleChange}\\r\\n                                                                className=\\\"add-new-post__editor mb-1\\\"\\r\\n                                                                style={divStyle}/>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\"></div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        {this\\r\\n                                                            .validator\\r\\n                                                            .message(\\\"Page Content\\\", this.state.content, \\\"required|min:150\\\")}\\r\\n                                                        <div className=\\\"text-danger\\\">\\r\\n                                                            {\\\" \\\"}\\r\\n                                                            {this.state.employer_sevice_message}\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                            <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n\\r\\n                                                                <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Add</button>\\r\\n                                                                <Link to=\\\"/FooterLinks\\\">\\r\\n                                                                    <button type=\\\"button\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Cancel\\r\\n                                                                    </button>\\r\\n                                                                </Link>\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                </Form>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\nexport default EditFooterLInks;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Form, Input, Button, Alert} from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport ReactQuill from \\\"react-quill\\\";\\r\\nimport 'react-quill/dist/quill.snow.css';\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\n\\r\\nimport FileUploader from \\\"react-firebase-file-uploader\\\";\\r\\n\\r\\nclass AddSlider extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n\\r\\n        this.state = {\\r\\n            pageTitle: '',\\r\\n            category:'',\\r\\n            date: new Date().toLocaleString(),\\r\\n            sliderUrl: '',\\r\\n            content: '',\\r\\n            theme: 'snow',\\r\\n            uploadImage: '',\\r\\n            avatar: \\\"\\\",\\r\\n            isUploading: false,\\r\\n            progress: 0,\\r\\n            avatarURL: \\\"\\\",\\r\\n            categoryList:'',\\r\\n        }\\r\\n        this.handleChange = this\\r\\n            .handleChange\\r\\n            .bind(this);\\r\\n            this.addNotification = this.addNotification.bind(this);\\r\\n            this.notificationDOMRef = React.createRef();\\r\\n        this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                website: {\\r\\n                    message: \\\"The Url should be example.com \\\",\\r\\n                    rule: function(val, params, validator) {\\r\\n                      //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n                      return (\\r\\n                        validator.helpers.testRegex(\\r\\n                          val,\\r\\n                          /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g\\r\\n                        ) && params.indexOf(val) === -1\\r\\n                      );\\r\\n                    }\\r\\n                  },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    modules = {\\r\\n        toolbar: [\\r\\n          [{ 'header': [1, 2,3,4,5,6, false] }],\\r\\n          ['bold', 'italic', 'underline','strike', 'blockquote'],\\r\\n          [{'list': 'ordered'}, {'list': 'bullet'}, {'indent': '-1'}, {'indent': '+1'}],\\r\\n          ['link', 'image'],\\r\\n          ['clean'],\\r\\n          [{ 'color': [] }, { 'background': [] }], \\r\\n        ],\\r\\n      };\\r\\n     \\r\\n      formats = [\\r\\n        'header',\\r\\n        'bold', 'italic', 'underline', 'strike', 'blockquote',\\r\\n        'list', 'bullet', 'indent',\\r\\n        'link', 'image','color','background'\\r\\n      ];\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n    componentDidMount() {\\r\\n        this.setState({ loading: true });\\r\\n        var ref = firebase.database().ref(\\\"category/\\\");\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n          \\r\\n    \\r\\n            snapshot.forEach(childSnapShot => {\\r\\n               \\r\\n              const Categories = {\\r\\n                categoryId: childSnapShot.key.toString(),\\r\\n                category: childSnapShot.val().category,\\r\\n                created_on: childSnapShot.val().created_on,\\r\\n              };\\r\\n      \\r\\n              data.push(Categories);\\r\\n            });\\r\\n      \\r\\n            this.setState({ categoryList:data,loading: false });\\r\\n            console.log(this.state.categoryList);\\r\\n          }); }\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n    addNotification() {\\r\\n        this.notificationDOMRef.current.addNotification({\\r\\n          title: \\\"Success\\\",\\r\\n          message: \\\"Succesfully added!\\\",\\r\\n          type: \\\"success\\\",\\r\\n          insert: \\\"bottom\\\",\\r\\n          container: \\\"top-center\\\",\\r\\n          animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n          animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n          dismiss: { duration: 2000 },\\r\\n          dismissable: { click: true }\\r\\n        });\\r\\n      }\\r\\n    handleUploadStart = () => this.setState({isUploading: true, progress: 0});\\r\\n    handleProgress = progress => this.setState({progress});\\r\\n    handleUploadError = error => {\\r\\n        this.setState({isUploading: false});\\r\\n        console.error(error);\\r\\n    };\\r\\n    handleUploadSuccess = filename => {\\r\\n        this.setState({avatar: filename, progress: 100, isUploading: false});\\r\\n        console.log(this.state.avatar);\\r\\n        firebase\\r\\n            .storage()\\r\\n            .ref(\\\"images\\\")\\r\\n            .child(filename)\\r\\n            .getDownloadURL()\\r\\n            .then(url => this.setState({avatarURL: url}));\\r\\n    };\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid()) {\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0 && this.state.sliderUrl.trim().length !== 0) {\\r\\n                this.addNotification();\\r\\n                event.preventDefault();\\r\\n                var userName = sessionStorage.getItem(\\\"userName\\\");\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref('/Slider');\\r\\n                dbCon.push({pageTitle: this.state.pageTitle,\\r\\n                    category: this.state.category,\\r\\n                    userName:userName,content: this.state.content, \\r\\n                    uploadImage: this.state.avatarURL,\\r\\n                     sliderUrl: this.state.sliderUrl,\\r\\n                     status:\\\"Active\\\", \\r\\n                     created_on: this.state.date});\\r\\n\\r\\n                this.setState({pageTitle: '', sliderUrl: '',content: ''});\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Slider\\\");\\r\\n            }\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Slider</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Add Slider</h4>\\r\\n                                            <ReactNotification ref={this.notificationDOMRef}/>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Edit-GST\\\">\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Slider Name*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageTitle\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageTitle}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Title\\\", this.state.pageTitle, \\\"required|min:3|max:500\\\")}\\r\\n                                                            <div className=\\\"text-danger\\\">\\r\\n                                                                {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                    <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                           \\r\\n                            <select\\r\\n                                                            name=\\\"select\\\"\\r\\n                                                            className=\\\"form-control pro-edt-select form-control-primary\\\"\\r\\n                                                            name=\\\"category\\\"\\r\\n                                                            onChange={this.onChange}>\\r\\n                                                             <option>Select Category</option>\\r\\n                                                             {this.state.categoryList && this.state.categoryList.map((data,index) =>{\\r\\n                                       \\r\\n                                       \\r\\n                                        return(\\r\\n                                            <option value={data.category} key={index}>{data.category}</option>\\r\\n                                        )\\r\\n                                       \\r\\n\\r\\n                                    } )}\\r\\n                                     \\r\\n                                     \\r\\n                                                           \\r\\n                                                        </select>\\r\\n                                                        {this.validator.message(\\r\\n                                            \\\"Select category\\\",\\r\\n                                            this.state.category,\\r\\n                                            \\\"required\\\"\\r\\n                                          )}\\r\\n                           \\r\\n                          \\r\\n                            </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n\\r\\n                                                         \\r\\n\\r\\n                                                                <input type=\\\"text\\\"                                  \\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Slider Url*\\\"\\r\\n                                                                name=\\\"sliderUrl\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.sliderUrl}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Slider Url\\\", this.state.sliderUrl, \\\"required|website\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                   \\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\