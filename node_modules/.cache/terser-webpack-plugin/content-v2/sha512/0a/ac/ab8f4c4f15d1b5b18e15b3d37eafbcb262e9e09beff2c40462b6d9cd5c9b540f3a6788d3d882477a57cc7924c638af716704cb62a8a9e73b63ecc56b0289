 = [];\\r\\n            // console.log(snapshot); console.log(snapshot.val);\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n                    socilaId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    gst: childSnapShot\\r\\n                        .val()\\r\\n                        .gst,\\r\\n                    title: childSnapShot\\r\\n                        .val()\\r\\n                        .title,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({gstList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.gstList);\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        clearTimeout(this.interval);\\r\\n    }\\r\\n    deleteItem = id => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(id);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/SocialAccounts/${id}`);\\r\\n                playersRef.remove();\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/PincodeList/Deleted\\\");\\r\\n\\r\\n            } else {}\\r\\n\\r\\n        });\\r\\n\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        var pathname = this\\r\\n            .props\\r\\n            .location\\r\\n            .pathname\\r\\n            .split(\\\"/\\\");\\r\\n        var 