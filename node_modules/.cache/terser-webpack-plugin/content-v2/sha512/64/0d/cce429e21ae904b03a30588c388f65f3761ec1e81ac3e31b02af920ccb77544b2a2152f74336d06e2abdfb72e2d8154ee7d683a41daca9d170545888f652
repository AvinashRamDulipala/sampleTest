: true});\\r\\n      const {emailContentId}=this.props.match.params;\\r\\n      console.log(emailContentId);\\r\\n      \\r\\n      var ref = firebase.database().ref(`emailContent/${emailContentId}`);\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            var services = snapshot.val();\\r\\n          \\r\\n           \\r\\n            this.setState({ emailContentName:services.emailContentName,\\r\\n                emailDesc:services.emailDesc,\\r\\n                 });\\r\\n            console.log(this.state.emailContentName);\\r\\n          });\\r\\n     \\r\\n  }\\r\\n \\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({ [event.target.name]: event.target.value });\\r\\n  };\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n    if (this.validator.allValid()) {\\r\\n    const {emailContentId}=this.props.match.params;\\r\\n    this.addNotification();\\r\\n        event.preventDefault();\\r\\n        let dbCon = firebase.database().ref(`/emailContent/${emailContentId}`);\\r\\n        dbCon.update ({\\r\\n            emailContentName: this.state.emailContentName,\\r\\n            emailDesc: this.state.emailDesc,\\r\\n            \\r\\n            created_on: this.state.date\\r\\n        });\\r\\n\\r\\n      \\r\\n      this.setState({\\r\\n        emailContentName:'',\\r\\n        emailDesc:'',\\r\\n      });\\r\\n      this.props.history.push(\\\"/EmailContentListPage\\\");\\r\\n    } else {\\r\\n      this\\r\\n          .validator\\r\\n          .showMessages();\\r\\n      this.forceUpdate();\\r\\n    }\\r\\n  };\\r\\n  handleEditorChange = (emailDesc) => {\\r\\n    this.setState({\\r\\n        emailDesc,\\r\\n    });\\r\\n  };componentWillUnmount() {\\r\\n    this.setState({loading: false});\\r\\n  }\\r\\n  \\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\