', 'blockquote',\\r\\n        'list', 'bullet', 'indent',\\r\\n        'link', 'image','color','background'\\r\\n      ];\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid()) {\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0 && this.state.content.trim().length !== 0) {\\r\\n                this.addNotification();\\r\\n                event.preventDefault();\\r\\n\\r\\n              \\r\\n\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref('/CMSPages');\\r\\n                dbCon.push({pageTitle: this.state.pageTitle, pageName: this.state.pageName, content: this.state.content, created_on: this.state.date});\\r\\n\\r\\n                this.setState({pageTitle: '', content: '',pageName:''});\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/CMSPages\\\");\\r\\n            }\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n    addNotification() {\\r\\n        this\\r\\n            .notificationDOMRef\\r\\n            .current\\r\\n            .addNotification({\\r\\n                title: \\\"Success\\\",\\r\\n                message: \\\"Succesfully added!\\\",\\r\\n                type: \\\"success\\\",\\r\\n                insert: \\\"bottom\\\",\\r\\n                container: \\\"top-center\\\",\\r\\n                animationIn: [\\r\\n                    \\\"animated\\\", \\\"fadeIn\\\"\\r\\n                ],\\r\\n                animationOut: [\\r\\n                    \\\"animated\\\", \\\"fadeOut\\\"\\r\\n                ],\\r\\n                dismiss: {\\r\\n                    duration: 2000\\r\\n                },\\r\\n                dismissable: {\\r\\n                    click: true\\r\\n                }\\r\\n            });\\r\\n    }\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Settings</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n                            <div className=\\\"container-fluid\\\">\\r\\n                                <div className=\\\"row\\\">\\r\\n                                    <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                        <div className=\\\"review-tab-pro-inner\\\">\\r\\n\\r\\n                                            <h4>Add CMS Page</h4>\\r\\n\\r\\n                                            <ReactNotification ref={this.notificationDOMRef}/>\\r\\n\\r\\n                                            <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                                                <Form onSubmit={this.handleSubmit} id=\\\"Edit-GST\\\">\\r\\n\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>Page Title<span style={{color: 'red' }}>*</span></label> \\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Page Title*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageTitle\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageTitle}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Title\\\", this.state.pageTitle, \\\"required|specialChar|min:3|max:70\\\")}\\r\\n                                                            <div className=\\\"text-danger\\\">\\r\\n                                                                {\\\" \\\"}\\r\\n                                                                {this.state.employer_sevice_message}\\r\\n                                                            </div>\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                        <label>Page Name<span style={{color: 'red' }}>*</span></label> \\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"Page Name*\\\"\\r\\n                                                                id=\\\"add_GST\\\"\\r\\n                                                                name=\\\"pageName\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.pageName}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"Page Name\\\", this.state.pageName, \\\"required|specialChar|min:3|max:70\\\")}\\r\\n                                               