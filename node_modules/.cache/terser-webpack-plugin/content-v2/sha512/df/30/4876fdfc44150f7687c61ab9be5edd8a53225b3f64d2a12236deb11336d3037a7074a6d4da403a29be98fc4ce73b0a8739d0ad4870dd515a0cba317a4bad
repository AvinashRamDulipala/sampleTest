ategory,\\r\\n                content: gstData.content,\\r\\n                avatarURL: gstData.uploadImage,\\r\\n                sliderUrl: gstData.sliderUrl,\\r\\n                loading: false\\r\\n            });\\r\\n            //console.log(this.state.pageTitle);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n    //   handleChange(value) {       if(value){         this.setState({content:\\r\\n    // value});       } };\\r\\n    modules = {\\r\\n        toolbar: [\\r\\n            [\\r\\n                {\\r\\n                    'header': [\\r\\n                        1,\\r\\n                        2,\\r\\n                        3,\\r\\n                        4,\\r\\n                        5,\\r\\n                        6,\\r\\n                        false\\r\\n                    ]\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'bold', 'italic', 'underline', 'strike', 'blockquote'\\r\\n            ],\\r\\n            [\\r\\n                {\\r\\n                    'list': 'ordered'\\r\\n                }, {\\r\\n                    'list': 'bullet'\\r\\n                }, {\\r\\n                    'indent': '-1'\\r\\n                }, {\\r\\n                    'indent': '+1'\\r\\n                }\\r\\n            ],\\r\\n            [\\r\\n                'link', 'image'\\r\\n            ],\\r\\n            ['clean'],\\r\\n            [\\r\\n                {\\r\\n                    'color': []\\r\\n                }, {\\r\\n                    'background': []\\r\\n                }\\r\\n            ]\\r\\n        ]\\r\\n    };\\r\\n\\r\\n    formats = [\\r\\n        'header',\\r\\n        'bold',\\r\\n        'italic',\\r\\n        'underline',\\r\\n        'strike',\\r\\n        'blockquote',\\r\\n        'list',\\r\\n        'bullet',\\r\\n        'indent',\\r\\