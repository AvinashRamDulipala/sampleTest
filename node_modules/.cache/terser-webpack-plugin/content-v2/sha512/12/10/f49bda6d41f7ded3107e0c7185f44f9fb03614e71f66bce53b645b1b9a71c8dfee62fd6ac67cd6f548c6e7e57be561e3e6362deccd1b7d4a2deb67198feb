pping: {},\\r\\n            cartDataItems: [],\\r\\n            date: new Date().toLocaleString(),\\r\\n            ShippingValue: '',\\r\\n\\r\\n            totalSellingPrice: ''\\r\\n        }\\r\\n\\r\\n    }\\r\\n    componentDidMount() {\\r\\n\\r\\n      \\r\\n\\r\\n        const {orderID} = this.props.match.params;\\r\\n        console.log(orderID);\\r\\n        let thiss = this;\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`orders/${orderID}`);\\r\\n        ref.on('value', function (childSnapShot) {\\r\\n            console.log(childSnapShot);\\r\\n            const orders = {\\r\\n                orderId: orderID,\\r\\n                cartDataItems: childSnapShot\\r\\n                    .val()\\r\\n                    .cartDataItems,\\r\\n                invoiceId: childSnapShot\\r\\n                    .val()\\r\\n                    .orderId,\\r\\n                customerName: childSnapShot\\r\\n                    .val()\\r\\n                    .name,\\r\\n                items: childSnapShot\\r\\n                    .val()\\r\\n                    .items,\\r\\n                shipping: childSnapShot\\r\\n                    .val()\\r\\n                    .shipping,\\r\\n                status: childSnapShot\\r\\n                    .val()\\r\\n                    .status,\\r\\n                subTotal: childSnapShot\\r\\n                    .val()\\r\\n                    .subTotal,\\r\\n                totalDiscount: childSnapShot\\r\\n                    .val()\\r\\n                    .totalDiscount,\\r\\n                totalGST: childSnapShot\\r\\n                    .val()\\r\\n                    .totalGST,\\r\\n                totalAmount: childSnapShot\\r\\n                    .val()\\r\\n                    .totalAmount,\\r\\n                GrandTotal: parseFloat(childSnapShot.val().totalAmount) + parseFloat(childSnapShot.val().totalDiscount) + parseFloat(childSnapShot.val().totalGST),\\r\\n            