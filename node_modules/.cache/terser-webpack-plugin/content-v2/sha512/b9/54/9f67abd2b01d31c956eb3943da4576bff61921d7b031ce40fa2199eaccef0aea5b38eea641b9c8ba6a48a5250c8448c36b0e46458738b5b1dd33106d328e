                 <input\\r\\n                                                                                    type=\\\"radio\\\"\\r\\n                                                                                    name=\\\"permission_type\\\"\\r\\n                                                                                    value=\\\"specific_empl\\\"\\r\\n                                                                                    onChange={this.handleChange}\\r\\n                                                                                    checked={this.state.permission_type === 'specific_empl'}/>\\r\\n                                                                                Specific Employees\\r\\n                                                                            </label>\\r\\n                                                                        </div>\\r\\n\\r\\n                                                                    </div>\\r\\n                                                                    {this\\r\\n                                                                        .validator\\r\\n                                                                        .message(\\\"permission\\\", this.state.permission_type, \\\"required\\\")}\\r\\n                                                                </div>\\r\\n                                                            </div>\\r\\n\\r\\n                                                        </div>\\r\\n\\r\\n                                                    </div>\\r\\n                                                   \\r\\n                                                    {this.state.permission_type === 'specific_empl'\\r\\n                                                        ? <div className=\\\"row m-b-20\\\">\\r\\n                                                                <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\