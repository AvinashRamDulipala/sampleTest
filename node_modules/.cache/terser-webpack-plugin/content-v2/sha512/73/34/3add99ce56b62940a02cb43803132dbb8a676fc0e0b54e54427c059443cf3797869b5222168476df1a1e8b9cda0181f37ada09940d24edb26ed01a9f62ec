laceholder=\\\"\\\"\\r\\n                                                                name=\\\"gstValue\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.gstValue}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"GST\\\", this.state.gstValue, \\\"required|whitespace|specialChar|min:2|max:70\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n                                                    </div>\\r\\n                                                    <div className=\\\"row m-b-20\\\">\\r\\n                                                        <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                                                            <label>TAN</label>\\r\\n\\r\\n                                                            <input\\r\\n                                                                type=\\\"text\\\"\\r\\n                                                                className=\\\"form-control\\\"\\r\\n                                                                placeholder=\\\"\\\"\\r\\n                                                                name=\\\"tanValue\\\"\\r\\n                                                                onChange={this.onChange}\\r\\n                                                                value={this.state.tanValue}/> {this\\r\\n                                                                .validator\\r\\n                                                                .message(\\\"TAN\\\", this.state.tanValue, \\\"required|whitespace|specialChar|min:2|max:70\\\")}\\r\\n\\r\\n                                                        </div>\\r\\n                                    