                                              display: \\\"block\\\"\\r\\n                                                }}>\\r\\n                                                    <a data-dismiss=\\\"alert\\\" href=\\\"#\\\" aria-hidden=\\\"true\\\" className=\\\"close\\\"></a>\\r\\n                                                    Notifications {alertName}\\r\\n                                                    Successfully\\r\\n                                                </div>\\r\\n                                            : ''}\\r\\n                                        <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                                            <thead>\\r\\n                                                <tr>\\r\\n                                                    <th>S.No</th>\\r\\n                                                    <th>Sent On</th>\\r\\n                                                    <th>\\r\\n                                                        title</th>\\r\\n                                                    <th>Sent To</th>\\r\\n                                                    <th>Action</th>\\r\\n                                                </tr>\\r\\n                                            </thead>\\r\\n                                            <tbody>\\r\\n\\r\\n                                                {this.state.gstList && this\\r\\n                                                    .state\\r\\n                                                    .gstList\\r\\n                                                    .map((data, index) => {\\r\\n                                                        console.log(data);\\r\\n\\r\\n                                                        return (\\r\\n                                                            <tr key={index}>\\r\\n                                                                <td>{index + 1}</td>\\