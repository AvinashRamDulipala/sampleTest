(this.state.validFrom).toLocaleString();\\r\\n            var validTo = new Date(this.state.validTo).toLocaleString();\\r\\n\\r\\n            var ref = firebase\\r\\n                .database()\\r\\n                .ref(\\\"merchantPayments/data\\\")\\r\\n                .orderByChild(\\\"created_on\\\")\\r\\n                .startAt(validFrom)\\r\\n                .endAt(validTo);\\r\\n\\r\\n            ref.once('value', snapshot => {\\r\\n                const data = [];\\r\\n                console.log(snapshot.val());\\r\\n                var i = 1;\\r\\n                snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                    const Merchants = {\\r\\n                        Sno: i++,\\r\\n                        orderId: childSnapShot\\r\\n                            .val()\\r\\n                            .orderId,\\r\\n                        customerName: childSnapShot\\r\\n                            .val()\\r\\n                            .first_name + \\\" \\\" + childSnapShot\\r\\n                            .val()\\r\\n                            .last_name,\\r\\n                        items: childSnapShot\\r\\n                            .val()\\r\\n                            .cartDataItems\\r\\n                            .length,\\r\\n                        totalAmount: childSnapShot\\r\\n                            .val()\\r\\n                            .totalAmount,\\r\\n                        status: childSnapShot\\r\\n                            .val()\\r\\n                            .status,\\r\\n                        order_on: childSnapShot\\r\\n                            .val()\\r\\n                            .created_on,\\r\\n                        emailId: childSnapShot\\r\\n                            .val()\\r\\n                            .email_id\\r\\n                    };\\r\\n\\r\\n                    data.push(Merchants);\\r\\n                });\\r\\n                this.setState({orderDateList: data, loading: f