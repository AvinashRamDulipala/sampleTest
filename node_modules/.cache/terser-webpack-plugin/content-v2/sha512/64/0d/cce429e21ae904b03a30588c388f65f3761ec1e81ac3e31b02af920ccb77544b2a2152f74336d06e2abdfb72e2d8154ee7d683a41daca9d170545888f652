>\\r\\n                            <table className=\\\"table table-bordered paginationTable\\\">\\r\\n                            <thead>\\r\\n                                <tr>\\r\\n                                    <th>S.No</th>\\r\\n                                    <th>Name</th>\\r\\n                                    <th>Description</th>\\r\\n\\t\\t\\t\\t\\t\\t\\t\\t\\t <th>Created on</th>\\r\\n                                <th>Action</th>\\r\\n                                </tr>\\r\\n                                </thead>\\r\\n                                <tbody>\\r\\n                                 \\r\\n                                    \\r\\n                                    {this.state.emailContentList && this.state.emailContentList.map((data,index) =>{\\r\\n                                        console.log(data);\\r\\n                                       \\r\\n                                        return(\\r\\n                                         <tr key={index}>\\r\\n                                         <td>{index+1}</td>\\r\\n                                         <td>{data.emailContentName}</td>\\r\\n                                         <td>{data.emailDesc}</td>\\r\\n                                         <td>{data.created_on}\\t</td>\\r\\n                                         <td>\\t\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                                            \\r\\n                                         <Link to={`/EditEmailContentPage/${data.emailContentId}`}>\\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Edit\\\" className=\\\"pd-setting-ed\\\"><i className=\\\"fa fa-pencil-square-o\\\" aria-hidden=\\\"true\\\"></i></button>\\r\\n                                         </Link>\\t\\t\\t\\t\\t\\t\\t\\t\\r\\n                     \\r\\n                                         <button data-toggle=\\\"tooltip\\\" title=\\\"Trash\\\" className=\\\"pd-setting-ed\\\" 