        loading: false,\\r\\n            count: 1,\\r\\n            activePage: \\\"1\\\",\\r\\n            countPage: \\\"\\\",\\r\\n            status: '',\\r\\n            usePermList: \\\"\\\"\\r\\n        };\\r\\n        this.deleteItem = this\\r\\n            .deleteItem\\r\\n            .bind(this);\\r\\n\\r\\n    }\\r\\n    componentWillMount() {\\r\\n        var sessionId = sessionStorage.getItem(\\\"userRole\\\");\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"Role\\\")\\r\\n            .orderByChild(\\\"Role\\\")\\r\\n            .equalTo(sessionId);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const orderStatus = {\\r\\n                    RoleId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n\\r\\n                    products: childSnapShot\\r\\n                        .val()\\r\\n                        .products\\r\\n                };\\r\\n\\r\\n                data.push(orderStatus);\\r\\n            });\\r\\n\\r\\n            this.setState({usePermList: data[0].products, loading: false});\\r\\n            console.log(this.state.usePermList);\\r\\n\\r\\n        });\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        this.setState({loading: true});\\r\\n        var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n        var userRole = sessionStorage.getItem(\\\"userRole\\\");\\r\\n        if (userRole == \\\"Merchant\\\") {\\r\\n            var ref = firebase\\r\\n                .database()\\r\\n                .ref(\\\"products/\\\")\\r\\n                .orderByChild(\\\"sessionId\\\")\\r\\n                .equalTo(sessionId);\\r\\n\\r\\n        } else {\\r\\n            var ref = firebase\\r\\n                .database()\\r\\n                .ref(\\\"products/\\\");\\r\\n        }\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n\\r\\n            const data = [];\\r\\n            // console.log(snapshot); console.log(snapshot.val);\\r\\n\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const Products = {\\r\\n                    productId: childSnapShot\\r\\n                        .key\\r\\n                        .toString(),\\r\\n                    productName: childSnapShot\\r\\n                        .val()\\r\\n                        .productName,\\r\\n                    category: childSnapShot\\r\\n                        .val()\\r\\n                        .category,\\r\\n                    userName: childSnapShot\\r\\n                        .val()\\r\\n                        .userName,\\r\\n                    price: childSnapShot\\r\\n                        .val()\\r\\n                        .price,\\r\\n                    sellingPrice: childSnapShot\\r\\n                        .val()\\r\\n                        .sellingPrice,\\r\\n                    status: childSnapShot\\r\\n                        .val()\\r\\n                        .status,\\r\\n                    stock: childSnapShot\\r\\n                        .val()\\r\\n                        .stock,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(Products);\\r\\n            });\\r\\n\\r\\n            this.setState({productList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.productList);\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destroy(true);\\r\\n        clearTimeout(this.interval);\\r\\n    }\\r\\n    onChange = (event) => {\\r\\n\\r\\n        var productId = event.target.id;\\r\\n        console.log(productId);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n        console.log(event.target.value);\\r\\n\\r\\n        let dbCon = firebase\\r\\n            .database()\\r\\n            .ref(`/products/${productId}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n    onStockChange = (event) => {\\r\\n\\r\\n        var productId = event.target.id;\\r\\n\\r\\n        let dbCon = firebase\\r\\n            .database()\\r\\n            .ref(`/products/${productId}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update({stock: event.target.value});\\r\\n    };\\r\\n\\r\\n    deleteItem = id => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(id);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/products/${id}`);\\r\\n                playersRef.remove();\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/ProductListPage/Deleted\\\");\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n    }\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({activePage: pageNumber});\\r\\n    }\\r\\n\\r\\n    render() {\\r\\n\\r\\n        var pathname = this\\r\\n            .props\\r\\n            .location\\r\\n            .pathname\\r\\n            .split(\\\"/\\\");\\r\\n        var alertName = pathname[2];\\r\\n        console.log(alertName);\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n     