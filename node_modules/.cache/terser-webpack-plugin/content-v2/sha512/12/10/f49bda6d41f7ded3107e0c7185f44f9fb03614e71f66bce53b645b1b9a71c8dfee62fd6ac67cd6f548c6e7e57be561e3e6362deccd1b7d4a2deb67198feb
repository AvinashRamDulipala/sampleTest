\\r\\n    \\r\\n      </div>\\r\\n    );\\r\\n  }\\r\\n}\\r\\n\\r\\nexport default CategoryListPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\n\\r\\nimport { Form } from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\n\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\n\\r\\n\\r\\n\\r\\n\\r\\nclass EditCategoryPage extends React.Component {\\r\\n  constructor(props) {\\r\\n    super(props);\\r\\n    this.state = {\\r\\n        category:'',\\r\\n        date:new Date().toLocaleString(),\\r\\n\\r\\n    }\\r\\n    this.addNotification = this.addNotification.bind(this);\\r\\n        this.notificationDOMRef = React.createRef();\\r\\n    this.validator = new SimpleReactValidator({\\r\\n        className: \\\"text-danger\\\",\\r\\n        validators: {\\r\\n  \\r\\n            whitespace: {\\r\\n                message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                rule: function (val, params, validator) {\\r\\n                    // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                    // params.indexOf(val) === -1\\r\\n                    return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                }\\r\\n            },\\r\\n            specialChar: {\\r\\n                message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                rule: function (val, params, validator) {\\r\\n                    // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                    // params.indexOf(val) === -1\\r\\n                    return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                }\\r\\n            },\\r\\n            specialCharText: {\\r\\n                message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                rule: function (val, params, validator) {\\r\\n                    // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                    // params.indexOf(val) === -1\\r\\n                    return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                }\\r\\n            }\\r\\n        }\\r\\n    });\\r\\n     \\r\\n\\r\\n  }\\r\\n  componentDidMount(){\\r\\n      const {categoryID}=this.props.match.params;\\r\\n      console.log(categoryID);\\r\\n      \\r\\n      var ref = firebase.database().ref(`category/${categoryID}`);\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            var categories = snapshot.val();\\r\\n            // console.log(categories)\\r\\n           \\r\\n            this.setState({ category:categories.category,loading: false });\\r\\n            console.log(this.state.category);\\r\\n          });\\r\\n     \\r\\n  }\\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({ category: event.target.value });\\r\\n  };\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n    event.preventDefault();\\r\\n    if (this.validator.allValid()) {\\r\\n    const {categoryID}=this.props.match.params;\\r\\n    var userName = sessionStorage.getItem(\\\"userName\\\");\\r\\n    if (this.state.category && this.state.category.trim().length !== 0) {\\r\\n        this.addNotification();\\r\\n        event.preventDefault();\\r\\n        let dbCon = firebase.database().ref(`/category/${categoryID}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update ({\\r\\n            category: this.state.category,\\r\\n            userName:userName,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n        \\r\\n      this.props.history.push(\\\"/CategoryListPage\\\");\\r\\n    }} else {\\r\\n        this\\r\\n            .validator\\r\\n            .showMessages();\\r\\n        this.forceUpdate();\\r\\n      }\\r\\n  };\\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\\"bottom\\\",\\r\\n      container: \\\"top-center\\\",\\r\\n      animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n      animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n      dismiss: { duration: 2000 },\\r\\n      dismissable: { click: true }\\r\\n    });\\r\\n  }\\r\\n  render() {\\r\\n    \\r\\n\\r\\n    return (\\r\\n        <div>\\r\\n       <SidebarPage/>\\r\\n     \\r\\n      <div className=\\\"all-content-wrapper\\\">\\r\\n      <HeaderPage/>\\r\\n     \\r\\n            \\r\\n              <div className=\\\"breadcome-area\\\">\\r\\n                  <div className=\\\"container-fluid\\\">\\r\\n                      <div className=\\\"row\\\">\\r\\n                          <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                              <div className=\\\"breadcome-list\\\">\\r\\n                                  <div className=\\\"row\\\">\\r\\n                                      <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                          <div className=\\\"breadcomb-wp\\\">\\r\\n                                          \\r\\n                                              <div className=\\\"breadcomb-ctn\\\">\\r\\n                                              <h2>Categories</h2>\\r\\n                                                  </div>\\r\\n                                          </div>\\r\\n                                      </div>\\r\\n                            \\r\\n                                  </div>\\r\\n                              </div>\\r\\n                          </div>\\r\\n                      </div>\\r\\n                  </div>\\r\\n              </div>\\r\\n        \\r\\n       \\r\\n              <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n            \\r\\n              <div className=\\\"single-pro-review-area\\\">\\r\\n                  <div className=\\\"container-fluid\\\">\\r\\n                      <div className=\\\"row\\\">\\r\\n                          <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                              <div className=\\\"review-tab-pro-inner\\\">\\r\\n                     \\r\\n                     <h4>Edit Category</h4>\\r\\n                     \\r\\n                     <ReactNotification ref={this.notificationDOMRef} />\\r\\n                              \\r\\n                              <div className=\\\"product-tab-list tab-pane fade active in\\\">\\r\\n                              <Form\\r\\n        onSubmit={this.handleSubmit}\\r\\n          id=\\\"Edit-category\\\"\\r\\n        >\\r\\n                              \\r\\n                              <div className=\\\"row m-b-20\\\">\\r\\n                              <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-12\\\">\\r\\n                           \\r\\n                              <input type=\\\"text\\\" className=\\\"form-control\\\" placeholder=\\\"Category Name*\\\"  id=\\\"Edit_category\\\"\\r\\n                              name=\\\"category\\\" onChange={ this.onChange } value={ this.state.category } />\\r\\n                               {this.validator.message(\\\"Category Name\\\", this.state.category, \\\"required|whitespace|specialChar|min:3|max:20\\\")}\\r\\n                            \\r\\n                              </div>\\r\\n  \\r\\n                              </div>\\r\\n  \\r\\n                                          <div className=\\\"row\\\">\\r\\n                                              <div className=\\\"col-lg-6 col-md-6 col-sm-12 col-xs-12\\\">\\r\\n                                                  <div className=\\\"text-left custom-pro-edt-ds\\\">\\r\\n                                                      <button type=\\\"submit\\\" className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Save\\r\\n                                                          </button>\\r\\n                                                          <Link to=\\\"/CategoryListPage\\\"> <button type=\\\"button\\\"  className=\\\"btn btn-ctl-bt waves-effect waves-light\\\">Cancel\\r\\n                                                            </button></Link>\\r\\n                                                   \\r\\n                                                  </div>\\r\\n                                              </div>\\r\\n                                          </div>\\r\\n                                          </Form>\\r\\n                                      </div>\\r\\n                                   \\r\\n  \\r\\n              \\r\\n                                  \\r\\n                          \\r\\n                              </div>\\r\\n                          </div>\\r\\n                      </div>\\r\\n                  </div>\\r\\n              </div>\\r\\n          </div>\\r\\n          </div>\\r\\n           <FooterPage/>\\r\\n      \\r\\n        </div>\\r\\n      );\\r\\n    }\\r\\n  }\\r\\nexport default EditCategoryPage;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport {Form} from 'reactstrap';\\r\\n// import FileUploader from 'react-firebase-file-uploader';\\r\\nimport ReactNotification from \\\"react-notifications-component\\\";\\r\\nimport \\\"react-notifications-component/dist/theme.css\\\";\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\nconst getCountriesList = require('countrycitystatejson');\\r\\n\\r\\nclass AddMerchantPage extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            merchantName: '',\\r\\n            contactNumber: '',\\r\\n            emailId: '',\\r\\n            countryName: getCountriesList.getCountries(),\\r\\n            stateName: [],\\r\\n            cityName: [],\\r\\n            selectedCountry:'',\\r\\n            selectedCountryName:'',\\r\\n            selectedstateName:'',\\r\\n            selectedcityName:'',\\r\\n            pincode: '',\\r\\n            address: '',\\r\\n            latitude: '',\\r\\n            longitude: '',\\r\\n            location: '',\\r\\n            shopType: '',\\r\\n            branches: '',\\r\\n            radius: '',\\r\\n            startTime: \\\"\\\",\\r\\n            endTime: false,\\r\\n            gstValue: 0,\\r\\n            serviceCharge: \\\"\\\",\\r\\n            deliveryFee: 0,\\r\\n            minOrder: \\\"\\\",\\r\\n            uploadImage: '',\\r\\n            date: new Date().toLocaleString()\\r\\n        }\\r\\n        this.addNotification = this.addNotification.bind(this);\\r\\n        this.notificationDOMRef = React.createRef();this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n              passwordvalid: {\\r\\n                message:\\r\\n                  \\\"The :attribute must be at least 6 and at most 30 with 1 numeric,1 special character and 1 alphabet.\\\",\\r\\n                rule: function(val, params, validator) {\\r\\n                  //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n                  return (\\r\\n                    validator.helpers.testRegex(\\r\\n                      val,\\r\\n                      /^(?=.*\\\\d)(?=.*[!@#$%^&*])(?=.*[a-z]).{6,30}$/i\\r\\n                    ) && params.indexOf(val) === -1\\r\\n                  );\\r\\n                }\\r\\n              },\\r\\n              passwordMismatch: {\\r\\n                message: \\\"confirm password must match with password field.\\\",\\r\\n                rule: function(val, params, validator) {\\r\\n                  return document.getElementById(\\\"password_input\\\").value === val\\r\\n                    ? true\\r\\n                    : false;\\r\\n                }\\r\\n              },\\r\\n              whitespace: {\\r\\n                message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                rule: function(val, params, validator) {\\r\\n                  //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n                  return (\\r\\n                    validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) &&\\r\\n                    params.indexOf(val) === -1\\r\\n                  );\\r\\n                }\\r\\n              },\\r\\n              specialChar: {\\r\\n                message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                rule: function(val, params, validator) {\\r\\n                  //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n                  return (\\r\\n                    validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) &&\\r\\n                    params.indexOf(val) === -1\\r\\n                  );\\r\\n                }\\r\\n              },\\r\\n              specialCharText: {\\r\\n                message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                rule: function(val, params, validator) {\\r\\n                  //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n                  return (\\r\\n                    validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) &&\\r\\n                    params.indexOf(val) === -1\\r\\n                  );\\r\\n                }\\r\\n              },\\r\\n      \\r\\n              // zip: {\\r\\n              //   message: \\\"Invalid zip code\\\",\\r\\n              //   rule: function(val, params, validator) {\\r\\n              //     //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n              //     return (\\r\\n              //       validator.helpers.testRegex(val, /^(\\\\d{5}(\\\\d{4})?)?$/i) &&\\r\\n              //       params.indexOf(val) === -1\\r\\n              //     );\\r\\n              //   }\\r\\n              // },\\r\\n              website: {\\r\\n                message: \\\"The Url should be example.com \\\",\\r\\n                rule: function(val, params, validator) {\\r\\n                  //return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) && params.indexOf(val) === -1\\r\\n                  return (\\r\\n                    validator.helpers.testRegex(\\r\\n                      val,\\r\\n                      /(http(s)?:\\\\/\\\\/.)?(www\\\\.)?[-a-zA-Z0-9@:%._\\\\+~#=]{2,256}\\\\.[a-z]{2,6}\\\\b([-a-zA-Z0-9@:%_\\\\+.~#?&//=]*)/g\\r\\n                    ) && params.indexOf(val) === -1\\r\\n                  );\\r\\n                }\\r\\n              },\\r\\n              Fax: {\\r\\n                message: \\\"Invalid fax number \\\",\\r\\n                rule: function(val, params, validator) {\\r\\n                  return (\\r\\n                    validator.helpers.testRegex(\\r\\n                      val,\\r\\n                      /^([\\\\w-\\\\.]+)@((\\\\[[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.[0-9]{1,3}\\\\.)|(([\\\\w-]+\\\\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\\\\]?)$/i\\r\\n                    ) && params.indexOf(val) === -1\\r\\n                  );\\r\\n                }\\r\\n              }\\r\\n            }\\r\\n          });\\r\\n        \\r\\n\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        const {merchantID}=this.props.match.params;\\r\\n        console.log(merchantID);\\r\\n        this.setState({ loading: true });\\r\\n        var ref = firebase.database().ref(`merchant/${merchantID}`);\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            var merchants = snapshot.val();\\r\\n          \\r\\n      \\r\\n            this.setState({  \\r\\n                merchantName: merchants.merchantName,\\r\\n                contactNumber: merchants.contactNumber,\\r\\n                emailId: merchants.emailId,\\r\\n                countryName: merchants.countryName,\\r\\n                stateName: merchants.stateName,\\r\\n                cityName: merchants.cityName,\\r\\n                pincode: merchants.pincode,\\r\\n                address: merchants.address,\\r\\n                latitude: merchants.latitude,\\r\\n                longitude: merchants.longitude,\\r\\n                location: merchants.location,\\r\\n                shopType: merchants.shopType,\\r\\n                branches: merchants.branches,\\r\\n                radius: merchants.radius,\\r\\n                startTime: merchants.startTime,\\r\\n                endTime: merchants.endTime,\\r\\n                gstValue: merchants.gstValue,\\r\\n                serviceCharge: merchants.serviceCharge,\\r\\n                location: merchants.location,\\r\\n                deliveryFee: merchants.deliveryFee,\\r\\n                minOrder: merchants.minOrder,\\r\\n                uploadImage: merchants.uploadImage,\\r\\n                loading: false });\\r\\n            \\r\\n          });\\r\\n        }\\r\\n    \\r\\n\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n    setCountryName = (e) => {\\r\\n        let index = e.target.selectedIndex;\\r\\n    let el = e.target.childNodes[index]\\r\\n    let option =  el.getAttribute('id'); \\r\\n    let selectedCountryName =   e.target.value; \\r\\n\\r\\n\\r\\n    \\r\\n        const statesList=getCountriesList.getStatesByShort(option);\\r\\n       // console.log(statesList);\\r\\n\\r\\n        \\r\\n\\r\\n        this.setState({ stateName: statesList,selectedCountry:option,selectedCountryName}, () => {\\r\\n            console.log(this.state.stateName, 'stateName');\\r\\n          });\\r\\n\\r\\n        //console.log(this.state.stateName);\\r\\n        \\r\\n        \\r\\n    };\\r\\n    setStateName = (e) => {\\r\\n        let stateValue = e.target.value;\\r\\n        let option = this.state.selectedCountry;\\r\\n       // console.log(option);\\r\\n        const cityList=getCountriesList.getCities(option,stateValue);\\r\\n       // console.log(cityList);\\r\\n        this.setState({ cityName: cityList,selectedstateName:stateValue}, () => {\\r\\n            console.log(this.state.cityName, 'stateName');\\r\\n          });\\r\\n\\r\\n        //console.log(this.state.stateName);\\r\\n        \\r\\n        \\r\\n    };\\r\\n    setStateName = (e) => {\\r\\n        let stateValue = e.target.value;\\r\\n        let option = this.state.selectedCountry;\\r\\n       // console.log(option);\\r\\n        const cityList=getCountriesList.getCities(option,stateValue);\\r\\n       // console.log(cityList);\\r\\n        this.setState({ cityName: cityList,selectedstateName:stateValue}, () => {\\r\\n            console.log(this.state.cityName, 'cityName');\\r\\n          });\\r\\n\\r\\n        //console.log(this.state.stateName);\\r\\n        \\r\\n        \\r\\n    };\\r\\n    setCityName = (e) => {\\r\\n        let cityValue = e.target.value;\\r\\n        \\r\\n        this.setState({ selectedcityName: cityValue}, () => {\\r\\n            console.log(this.state.selectedcityName, 'selectedcityName');\\r\\n          });\\r\\n\\r\\n        //console.log(this.state.stateName);\\r\\n        \\r\\n        \\r\\n    };\\r\\n    addNotification() {\\r\\n        this.notificationDOMRef.current.addNotification({\\r\\n          title: \\\"Success\\\",\\r\\n          message: \\\"Succesfully added!\\\",\\r\\n          type: \\\"success\\\",\\r\\n          insert: \\\"bottom\\\",\\r\\n          container: \\\"top-center\\\",\\r\\n          animationIn: [\\\"animated\\\", \\\"fadeIn\\\"],\\r\\n          animationOut: [\\\"animated\\\", \\\"fadeOut\\\"],\\r\\n          dismiss: { duration: 2000 },\\r\\n          dismissable: { click: true }\\r\\n        });\\r\\n      }\\r\\n  \\r\\n    handleSubmit = (event) => {\\r\\n        if (this.validator.allValid()) {\\r\\n            this.addNotification();\\r\\n        event.preventDefault();\\r\\n        const {merchantID}=this.props.match.params;\\r\\n        let dbCon = firebase\\r\\n            .database()\\r\\n            .ref(`/merchant/${merchantID}`);\\r\\n        dbCon.update({\\r\\n            merchantName: this.state.merchantName,\\r\\n            contactNumber: this.state.contactNumber,\\r\\n            emailId: this.state.emailId,\\r\\n            countryName: this.state.selectedCountryName,\\r\\n            stateName: this.state.selectedstateName,\\r\\n            cityName: this.state.selectedcityName,\\r\\n            pincode: this.state.pincode,\\r\\n            address: this.state.address,\\r\\n            latitude: this.state.latitude,\\r\\n            longitude: this.state.longitude,\\r\\n            location: this.state.location,\\r\\n            shopType: this.state.shopType,\\r\\n            branches: this.state.branches,\\r\\n            radius: this.state.radius,\\r\\n            startTime: this.state.startTime,\\r\\n            endTime: this.state.endTime,\\r\\n            gstValue: this.state.gstValue,\\r\\n            serviceCharge: this.state.serviceCharge,\\r\\n            location: this.state.location,\\r\\n            deliveryFee: this.state.deliveryFee,\\r\\n            minOrder: this.state.minOrder,\\r\\n            uploadImage: this.state.uploadImage,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n        this.props.history.push(\\\"/MerchantListPage\\\");\\r\\n    } else {\\r\\n        this.validator.showMessages();\\r\\n        this.forceUpdate();\\r\\n      }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Merchants</h2>\\r\\n                                                    </div>\\r\\n                                                </div>\\r\\n                                            </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                    <div className=\\\"single-product-tab-area mg-b-30\\\">\\r\\n\\r\\n                        <div className=\\\"single-pro-review-area\\\">\\r\\n      