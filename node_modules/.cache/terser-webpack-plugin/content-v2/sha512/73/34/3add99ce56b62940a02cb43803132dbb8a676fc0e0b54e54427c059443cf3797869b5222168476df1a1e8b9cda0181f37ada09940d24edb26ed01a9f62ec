\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    modules = {\\r\\n        toolbar: [\\r\\n          [{ 'header': [1, 2,3,4,5,6, false] }],\\r\\n          ['bold', 'italic', 'underline','strike', 'blockquote'],\\r\\n          [{'list': 'ordered'}, {'list': 'bullet'}, {'indent': '-1'}, {'indent': '+1'}],\\r\\n          ['link', 'image'],\\r\\n          ['clean'],\\r\\n          [{ 'color': [] }, { 'background': [] }], \\r\\n        ],\\r\\n      };\\r\\n     \\r\\n      formats = [\\r\\n        'header',\\r\\n        'bold', 'italic', 'underline', 'strike', 'blockquote',\\r\\n        'list', 'bullet', 'indent',\\r\\n        'link', 'image','color','background'\\r\\n      ];\\r\\n    onChange = (event) => {\\r\\n\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n    };\\r\\n\\r\\n    handleChange(value) {\\r\\n        this.setState({content: value})\\r\\n    };\\r\\n\\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid()) {\\r\\n            if (this.state.pageTitle && this.state.pageTitle.trim().length !== 0 && this.state.content.trim().length !== 0) {\\r\\n                this.addNotification();\\r\\n                event.preventDefault();\\r\\n\\r\\n              \\r\\n\\r\\n                let dbCon = firebase\\r\\n                    .database()\\r\\n                    .ref('/FooterLinks');\\r\\n                dbCon.push({pageTitle: this.state.pageTitle, pageName: this.state.pageName, content: this.state.content, created_on: this.state.date});\\r\\n\\r\\n                this.setState({pageTitle: '', content: '',pageName:''});\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/FooterLinks\\\");\\r\\n            }\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n         