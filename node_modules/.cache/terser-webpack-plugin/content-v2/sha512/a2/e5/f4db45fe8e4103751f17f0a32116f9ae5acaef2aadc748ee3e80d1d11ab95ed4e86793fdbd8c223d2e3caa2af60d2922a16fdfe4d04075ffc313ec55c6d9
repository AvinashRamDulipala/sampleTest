                couponCode: childSnapShot\\r\\n                        .val()\\r\\n                        .couponCode,\\r\\n                    couponDescription: childSnapShot\\r\\n                        .val()\\r\\n                        .couponDescription,\\r\\n                    type: childSnapShot\\r\\n                        .val()\\r\\n                        .type,\\r\\n                    validFrom: childSnapShot\\r\\n                        .val()\\r\\n                        .validFrom,\\r\\n                    validTo: childSnapShot\\r\\n                        .val()\\r\\n                        .validTo,\\r\\n                    numberOfCoupons: childSnapShot\\r\\n                        .val()\\r\\n                        .numberOfCoupons,\\r\\n                    usageType: childSnapShot\\r\\n                        .val()\\r\\n                        .usageType,\\r\\n                    numberOfUsages: childSnapShot\\r\\n                        .val()\\r\\n                        .numberOfUsages,\\r\\n                    offerValue: childSnapShot\\r\\n                        .val()\\r\\n                        .offerValue,\\r\\n                    offerType: childSnapShot\\r\\n                        .val()\\r\\n                        .offerType,\\r\\n                    created_on: childSnapShot\\r\\n                        .val()\\r\\n                        .created_on\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({gstList: data, countPage: data.length, loading: false});\\r\\n            console.log(this.state.gstList);\\r\\n\\r\\n            this.interval = setTimeout(() => $('.paginationTable').DataTable(), 1000);\\r\\n        });\\r\\n    }\\r\\n    componentWillUnmount() {\\r\\n        //this.dataTable.destroy(true);\\r\\n        clearTimeout(this.interval);\\r\\n    }\\r\\n    deleteItem = id => {\\r\\n\\r\\n        swal({title: \\\"Are you sure?\\\", text: \\\"Do your really want to remove?\\\", icon: \\\"warning\\\", buttons: true, dangerMode: true}).then(willDelete => {\\r\\n            if (willDelete) {\\r\\n                console.log(id);\\r\\n                var playersRef = firebase\\r\\n                    .database()\\r\\n                    .ref(`/Coupons/${id}`);\\r\\n                playersRef.remove();\\r\\n\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Coupons/Deleted\\\");\\r\\n\\r\\n            } else {}\\r\\n            // window.location.reload(\\\"/Home\\\");\\r\\n\\r\\n        });\\r\\n    }\\r\\n    handlePageChange(pageNumber) {\\r\\n        console.log(`active page is ${pageNumber}`);\\r\\n        this.setState({activePage: pageNumber});\\r\\n    }\\r\\n\\r\\n    render() {\\r\\n\\r\\n        var pathname = this\\r\\n            .props\\r\\n            .location\\r\\n            .pathname\\r\\n            .split(\\\"/\\\");\\r\\n        var alertName = pathname[2];\\r\\n        console.log(alertName);\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"br