testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n              }\\r\\n          }\\r\\n      }\\r\\n  });\\r\\n    \\r\\n\\r\\n  }\\r\\n  componentDidMount(){\\r\\n      const {GstID}=this.props.match.params;\\r\\n      console.log(GstID);\\r\\n      \\r\\n      var ref = firebase.database().ref(`ShippingCharges/${GstID}`);\\r\\n    \\r\\n    \\r\\n        ref.on('value', snapshot => {\\r\\n            var gstData = snapshot.val();\\r\\n           \\r\\n           \\r\\n            this.setState({ ShippingValue:gstData.ShippingValue,loading: false });\\r\\n            console.log(this.state.ShippingValue);\\r\\n          });\\r\\n     \\r\\n  }\\r\\n  onChange = (event) => {\\r\\n      \\r\\n    this.setState({ [event.target.name]: event.target.value });\\r\\n  };\\r\\n\\r\\n  handleSubmit = (event) => {\\r\\n      event.preventDefault();\\r\\n    if (this.validator.allValid()) {\\r\\n    const {GstID}=this.props.match.params;\\r\\n    \\r\\n    if (this.state.ShippingValue && this.state.ShippingValue.trim().length !== 0) {\\r\\n       \\r\\n        let dbCon = firebase.database().ref(`/ShippingCharges/${GstID}`);\\r\\n        console.log(dbCon);\\r\\n        dbCon.update ({\\r\\n            ShippingValue: this.state.ShippingValue,\\r\\n            created_on: this.state.date\\r\\n        });\\r\\n        this.addNotification();\\r\\n      \\r\\n        \\r\\n\\r\\n      \\r\\n      this.setState({\\r\\n        ShippingValue: ''\\r\\n      });\\r\\n      this.props.history.push(\\\"/ShippingCharges\\\");\\r\\n    }\\r\\n  } else {\\r\\n    this\\r\\n        .validator\\r\\n        .showMessages();\\r\\n    this.forceUpdate();\\r\\n  }\\r\\n  };\\r\\n  addNotification() {\\r\\n    this.notificationDOMRef.current.addNotification({\\r\\n      title: \\\"Success\\\",\\r\\n      message: \\\"Succesfully added!\\\",\\r\\n      type: \\\"success\\\",\\r\\n      insert: \\\"bottom\\\",\\r\\n      container: \\\"top-center\\\",\\r\\n      animatio