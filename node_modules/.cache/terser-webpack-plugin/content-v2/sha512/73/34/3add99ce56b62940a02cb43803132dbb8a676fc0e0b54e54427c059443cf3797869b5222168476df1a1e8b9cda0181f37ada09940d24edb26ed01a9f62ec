\\r\\n\\r\\n                this\\r\\n                    .props\\r\\n                    .history\\r\\n                    .push(\\\"/Notifications\\\");\\r\\n\\r\\n            }\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n   \\r\\n    \\r\\n    \\r\\n      handleMultiChange = selectedOption => {\\r\\n        this.setState({ multiSelect: selectedOption});\\r\\n        console.log(`Option selected:`, selectedOption);\\r\\n      };\\r\\n    componentWillMount() {\\r\\n        this.setState({loading: true});\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(\\\"admin_users/\\\")\\r\\n            .orderByChild(\\\"Role\\\")\\r\\n            .equalTo(\\\"Merchant\\\");\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n\\r\\n                const GSTData = {\\r\\n\\r\\n                    label: childSnapShot\\r\\n                    .val()\\r\\n                    .email,\\r\\n                    \\r\\n                    value: childSnapShot\\r\\n                .val()\\r\\n                .email,\\r\\n                };\\r\\n\\r\\n                data.push(GSTData);\\r\\n            });\\r\\n\\r\\n            this.setState({merchantList: data});\\r\\n            console.log(this.state.merchantList);\\r\\n        });\\r\\n    }\\r\\n\\r\\n    render() {\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '300px'\\r\\n        };\\r\\n        const selectedOptionsStyles = {\\r\\n            color: \\\"#3c763d\\\",\\r\\n            backgroundColor: \\\"#dff0d8\\\"\\r\\n        };\\r\\n        const optionsListStyles = {\\r\\n            backgroundColor: \\\"#dff0d8\\\",\\r\\n            color: \\\"#3c763d\\\"\\r\\n        };\\r\\n\\r\\n        return (\\r\\n            <div>\\r\\n                <Side