\\\">\\r\\n                    <Route exact strict path=\\\"/\\\" component={Login}/>\\r\\n\\r\\n                    <Route path=\\\"/login\\\" component={Login}/>\\r\\n                    <Route path=\\\"/Signup\\\" component={Signup}/>\\r\\n                    <PrivateRoute path=\\\"/MyProfile\\\" component={MyProfile}/>\\r\\n                    <PrivateRoute path=\\\"/ChangePassword\\\" component={ChangePassword}/>\\r\\n\\r\\n                    <PrivateRoute path=\\\"/Dashboard\\\" component={Dashboard}/>\\r\\n                    <PrivateRoute path=\\\"/AddMerchantPage\\\" component={AddMerchantPage}/>\\r\\n                    <PrivateRoute path=\\\"/AddProductPage\\\" component={AddProductPage}/>\\r\\n                    <PrivateRoute path=\\\"/AddCategoryPage\\\" component={AddCategoryPage}/>\\r\\n                    <PrivateRoute path=\\\"/MerchantListPage\\\" component={MerchantListPage}/>\\r\\n                    <PrivateRoute path=\\\"/ProductListPage\\\" component={ProductListPage}/>\\r\\n\\r\\n                    <PrivateRoute path=\\\"/CategoryListPage\\\" component={CategoryListPage}/>\\r\\n                    <PrivateRoute path=\\\"/AddGSTPage\\\" component={AddGSTPage}/>\\r\\n                    <PrivateRoute path=\\\"/GSTListPage\\\" component={GSTListPage}/>\\r\\n                    <PrivateRoute path=\\\"/EditGSTPage/:GstID\\\" component={EditGSTPage}/>\\r\\n\\r\\n                    <PrivateRoute path=\\\"/AddRolePage\\\" component={AddRolePage}/>\\r\\n                    <PrivateRoute path=\\\"/RoleListPage\\\" component={RoleListPage}/>\\r\\n                    <PrivateRoute path=\\\"/EditRolePage/:GstID\\\" component={EditRolePage}/>\\r\\n\\r\\n                    <PrivateRoute path=\\\"/AddPinCode\\\" component={AddPinCode}/>\\r\\n                    <PrivateRoute path=\\\"/PincodeList\\\" component={PincodeList}/>\\r\\n                    <PrivateRoute path=\\\"/EditPinCode/:GstID\\\" component={EditPinCode}/>\\r\\n\\r\\n                    