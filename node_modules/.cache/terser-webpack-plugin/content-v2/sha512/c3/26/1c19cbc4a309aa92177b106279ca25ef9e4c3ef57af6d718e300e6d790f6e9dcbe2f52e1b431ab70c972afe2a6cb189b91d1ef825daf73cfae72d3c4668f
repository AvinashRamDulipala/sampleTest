           .ref('departments/')\\r\\n            .orderByChild(\\\"department\\\")\\r\\n            .equalTo(event.target.value);\\r\\n        ref.on('value', snapshot => {\\r\\n            var user_exist = snapshot.numChildren();\\r\\n            console.log(user_exist);\\r\\n\\r\\n            if (user_exist > 0 && this.state.validError!=true) {\\r\\n\\r\\n                this.setState({error_message: \\\"department Name is already exist\\\", validError: false});\\r\\n            } else {\\r\\n                this.setState({error_message: \\\"\\\", validError: true});\\r\\n\\r\\n            }\\r\\n\\r\\n        })\\r\\n\\r\\n    };\\r\\n   \\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid() && this.state.validError == true) {\\r\\n         \\r\\n\\r\\n                var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n               \\r\\n\\r\\n                const {locationId} = this.props.match.params;\\r\\n            let dbCon = firebase\\r\\n                .database()\\r\\n                .ref(`/departments/${locationId}`);\\r\\n                dbCon\\r\\n                    .update({department: this.state.department, \\r\\n                        created_on: this.state.date, sessionId: sessionId})\\r\\n                    .then(response => {\\r\\n                        try {\\r\\n                            console.log(response);\\r\\n\\r\\n                            this\\r\\n                                .props\\r\\n                                .history\\r\\n                                .push(\\\"/Departments/Updated\\\");\\r\\n\\r\\n                        } catch (error) {\\r\\n                            console.log(error.config);\\r\\n                        }\\r\\n                    });\\r\\n\\r\\n            \\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n  