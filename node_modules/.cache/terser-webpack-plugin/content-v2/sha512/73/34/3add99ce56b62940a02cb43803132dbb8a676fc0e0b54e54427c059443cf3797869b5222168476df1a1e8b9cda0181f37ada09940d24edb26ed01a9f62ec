\r\\n\\r\\n                                                        var totalGSTVal = ((data.quantity) * (data.sellingPrice)) + ((data.quantity) * gstCaluculate);\\r\\n                                                        packingCostVal = data.quantity * data.packagingCost;\\r\\n                                                        console.log(totalGSTVal);\\r\\n                                                        totalcart += (totalGSTVal);\\r\\n                                                        PackingCost += packingCostVal;\\r\\n\\r\\n                                                            return (\\r\\n                                                                <tr key={index}>\\r\\n                                                                    <td>{index + 1}</td>\\r\\n                                                                    {/* <td class=\\\"img-product\\\"><img src={data.uploadImage} width=\\\"70px\\\"/></td> */}\\r\\n                                                                    <td class=\\\"\\\">{data.productName}</td>\\r\\n                                                                    <td>\\r\\n                                                                        <span class=\\\"amount\\\">\\r\\n                                                                            <i class=\\\"fa fa-inr\\\" aria-hidden=\\\"true\\\"></i>{data.sellingPrice}</span>\\r\\n                                                                    </td>\\r\\n                                                                    <td class=\\\"quantity\\\">{data.quantity}</td>\\r\\n                                                                    <td className=\\\"product-subtotal\\\">\\r\\n                                                                    <span className=\\\"amount\\\">\\r\\n                                                                        <i className=\\\"fa fa-inr\\\" aria-hidden=\\\"true\\\"></i>\\r\\n                                                                        {parseFloat((data.quantity) * gstCaluculate).toFixed(2)}\\r\\n                                                                        ({(data.gstValue)}%)\\r\\n                                                                    </span>\\r\\n                                                                </td>\\r\\n                                                                    <td>{data.created_on}</td>\\r\\n                                                                    <td>\\r\\n                                                                    \\r\\n                                                                    {parseFloat(((data.quantity) * (data.sellingPrice)) + ((data.quantity) * gstCaluculate)).toFixed(2)}</td>\\r\\n                                                                </tr>\\r\\n\\r\\n                                                            )\\r\\n                                                        })}\\r\\n\\r\\n                                                </tbody>\\r\\n                                            </table>\\r\\n\\r\\n                                        </div>\\r\\n\\r\\n                                    </td>\\r\\n                                </tr>\\r\\n\\r\\n                                <tr>\\r\\n                                    <td\\r\\n                                        align=\\\"right\\\"\\r\\n                                        style={{\\r\\n                                        color: \\\"#000\\\",\\r\\n                                        padding: \\\"15px\\\",\\r\\n                                        borderBottom: \\\"solid 1px #dedede\\\"\\r\\n                                    }}\\r\\n                                        colSpan=\\\"2\\\">\\r\\n                                        <p>Subtotal: Rs.{parseFloat(totalcart).toFixed(2)}\\r\\n                                        </p>\\r\\n                                        <p>Shipping charges: Rs.{parseInt(this.state.ShippingValue).toFixed(2)}</p>\\r\\n                                        \\r\\n                                        <h6>Order Total: Rs.{(totalcart + parseInt(this.state.ShippingValue)).toFixed(2)}</h6>\\r\\n                                    </td>\\r\\n\\r\\n                                </tr>\\r\\n\\r\\n                                <tr>\\r\\n                                    <td\\r\\n                                        colSpan=\\\"2\\\"\\r\\n                                        style={{\\r\\n                                        textAlign: \\\"left\\\",\\r\\n                                        padding: \\\"15px\\\"\\r\\n                                    }}>\\r\\n                                        <p><img src=\\\"./images/logo2.png\\\" alt=\\\"\\\"/></p>\\r\\n                                        <p><b>Gootes Essen India Pvt Ltd </b><br/>\\r\\n                                            <b>Address:</b>\\r\\n                                            17, 7th a cross, ganesh nagar layout,<br/> virupakshapura,Bangalore, Karnataka <br/>\\r\\n                                            India 560097<br/>\\r\\n                                            <b>Email:</b>\\r\\n                                            support@gootes.com<br/>\\r\\n                                            <b>Monday - Friday:</b>\\r\\n                                            9:30 a.m. - 6:30 p.m.</p>\\r\\n                                    </td>\\r\\n                                </tr>\\r\\n                            </tbody>\\r\\n\\r\\n                        </table>\\r\\n\\r\\n                    </div>\\r\\n                </div>\\r\\n                </PdfContainer>\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\nexport default ViewInvoice;\\r\\n\",\"import React from \\\"react\\\";\\r\\n