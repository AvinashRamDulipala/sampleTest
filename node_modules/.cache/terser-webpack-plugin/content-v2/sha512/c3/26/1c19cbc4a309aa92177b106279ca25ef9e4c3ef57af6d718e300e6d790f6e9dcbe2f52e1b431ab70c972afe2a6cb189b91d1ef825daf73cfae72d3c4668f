   </div>\\r\\n\\r\\n                                        </div>\\r\\n                                    </div>\\r\\n                                </div>\\r\\n                            </div>\\r\\n                        </div>\\r\\n                    </div>\\r\\n                </div>\\r\\n                <FooterPage/>\\r\\n\\r\\n            </div>\\r\\n        );\\r\\n    }\\r\\n}\\r\\n\\r\\nexport default AddSalesTracker;\\r\\n\",\"import React from \\\"react\\\";\\r\\nimport {Link} from \\\"react-router-dom\\\";\\r\\nimport {Form} from 'reactstrap';\\r\\nimport HeaderPage from \\\"./header\\\";\\r\\nimport FooterPage from \\\"./footer\\\";\\r\\nimport SidebarPage from \\\"./sidebar\\\";\\r\\nimport firebase from '../config';\\r\\nimport SimpleReactValidator from \\\"simple-react-validator\\\";\\r\\nimport FileUploader from \\\"react-firebase-file-uploader\\\";\\r\\nimport Select from 'react-select';\\r\\nimport Map from './map';\\r\\nimport {connect} from 'react-redux';\\r\\nclass EditSalesTracker extends React.Component {\\r\\n    constructor(props) {\\r\\n        super(props);\\r\\n        this.state = {\\r\\n            prospect_Name: '',\\r\\n            company_Name:'',\\r\\n            city:'',\\r\\n            contact_Number:'',\\r\\n            email:'',\\r\\n            source_Of_Enquiry:'',\\r\\n            contacted:'',\\r\\n            email_Sent:'',\\r\\n            follow_Up_Date:'',\\r\\n            follow_Up_Time:'',\\r\\n            remarks:'',\\r\\n            error_message: '',\\r\\n            created_on: new Date().toLocaleString(),\\r\\n            validError: true,\\r\\n            employer_sevice_message:'',\\r\\n        }\\r\\n\\r\\n        this.validator = new SimpleReactValidator({\\r\\n            className: \\\"text-danger\\\",\\r\\n            validators: {\\r\\n\\r\\n                whitespace: {\\r\\n                    message: \\\"The :attribute not allowed first whitespace   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /[^\\\\s\\\\\\\\]/) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialChar: {\\r\\n                    message: \\\"The :attribute not allowed special   characters.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z0-9_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                },\\r\\n                specialCharText: {\\r\\n                    message: \\\"The :attribute may only contain letters, dot and spaces.\\\",\\r\\n                    rule: function (val, params, validator) {\\r\\n                        // return validator.helpers.testRegex(val,/^[a-zA-Z0-9]{6,30}$/i) &&\\r\\n                        // params.indexOf(val) === -1\\r\\n                        return (validator.helpers.testRegex(val, /^[ A-Za-z_@./#&+-]*$/i) && params.indexOf(val) === -1);\\r\\n                    }\\r\\n                }\\r\\n            }\\r\\n        });\\r\\n\\r\\n    }\\r\\n    componentDidMount() {\\r\\n        const {salestrackerId} = this.props.match.params;\\r\\n        console.log(salestrackerId);\\r\\n        var sessionId = sessionStorage.getItem(\\\"RoleId\\\"); \\r\\n        var userName = sessionStorage.getItem(\\\"user_name\\\");\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref(`SalesTracker/${salestrackerId}`);\\r\\n\\r\\n        ref.on('value', snapshot => {\\r\\n            var categories = snapshot.val();\\r\\n\\r\\n             console.log(categories);\\r\\n\\r\\n            this.setState({\\r\\n                prospect_Name:categories.prospect_Name,\\r\\n                company_Name: categories.company_Name,\\r\\n                city: categories.city,\\r\\n                contact_Number: categories.contact_Number,\\r\\n                email: categories.email,\\r\\n                source_Of_Enquiry: categories.source_Of_Enquiry,\\r\\n                contacted: categories.contacted,\\r\\n                email_Sent: categories.email_Sent,\\r\\n                follow_Up_Date: categories.follow_Up_Date,\\r\\n                follow_Up_Time: categories.follow_Up_Time,\\r\\n                remarks: categories.remarks,\\r\\n                 created_on: categories.created_on, \\r\\n                userName:userName,\\r\\n                sessionId: sessionId,\\r\\n\\r\\n\\r\\n               \\r\\n                 loading: false\\r\\n                });\\r\\n            console.log(this.state.SalesTracker);\\r\\n\\r\\n            this.employeeSListView();\\r\\n           \\r\\n        });\\r\\n        this.city();\\r\\n\\r\\n    }\\r\\n    city=()=> {\\r\\n       \\r\\n        var ref = firebase.database().ref(\\\"City/\\\");\\r\\n        ref.on('value', snapshot => {\\r\\n            const data = [];\\r\\n            snapshot.forEach(childSnapShot => {\\r\\n               \\r\\n              const cities = {\\r\\n                cityId: childSnapShot.key.toString(),\\r\\n                city: childSnapShot.val().city,\\r\\n              };\\r\\n              data.push(cities);\\r\\n            });\\r\\n            this.setState({ City:data,loading: false });\\r\\n            console.log(this.state.City);\\r\\n          }); \\r\\n         \\r\\n    }\\r\\n\\r\\n    employeeSListView() {\\r\\n\\r\\n\\r\\n        var ref = firebase\\r\\n            .database()\\r\\n            .ref('Employees/');\\r\\n        ref.once(\\\"value\\\", snapshot => {\\r\\n            const data = [];\\r\\n            console.log(snapshot.val());\\r\\n            snapshot.forEach(element => {\\r\\n                const usersData = {\\r\\n\\r\\n                    value: element\\r\\n                        .val()\\r\\n                        .email,\\r\\n                    label: element\\r\\n                        .val()\\r\\n                        .first_name + \\\" \\\" + element\\r\\n                        .val()\\r\\n                        .last_name + \\\"(\\\" + element\\r\\n                        .val()\\r\\n                        .email + \\\")\\\"\\r\\n                }\\r\\n                data.unshift(usersData);\\r\\n            });\\r\\n\\r\\n            this.setState({\\r\\n                EmployeesList: data\\r\\n            }, () => {\\r\\n                console.log(this.state.EmployeesList, 'Employees');\\r\\n\\r\\n            });\\r\\n\\r\\n        })\\r\\n    }\\r\\n    handleChange = (event) => {\\r\\n        console.log(this.state.validError);\\r\\n        this.setState({\\r\\n            [event.target.name]: event.target.value\\r\\n        });\\r\\n\\r\\n       \\r\\n\\r\\n    };\\r\\n\\r\\n   \\r\\n    handleSubmit = (event) => {\\r\\n        event.preventDefault();\\r\\n        if (this.validator.allValid() && this.state.validError == true) {\\r\\n            const {salestrackerId} = this.props.match.params;\\r\\n            var sessionId = sessionStorage.getItem(\\\"RoleId\\\");\\r\\n            var userName = sessionStorage.getItem(\\\"email\\\");\\r\\n            let dbCon = firebase\\r\\n                .database()\\r\\n                .ref(`/SalesTracker/${salestrackerId}`);\\r\\n            console.log(dbCon);\\r\\n            dbCon.update({\\r\\n                prospect_Name:this.state.prospect_Name,\\r\\n            company_Name:this.state.company_Name,\\r\\n            city:this.state.city,\\r\\n            contact_Number:this.state.contact_Number,\\r\\n            email:this.state.email,\\r\\n            source_Of_Enquiry:this.state.source_Of_Enquiry,\\r\\n            contacted:this.state.contacted,\\r\\n            email_Sent:this.state.email_Sent,\\r\\n            follow_Up_Date:this.state.follow_Up_Date,\\r\\n            follow_Up_Time:this.state.follow_Up_Time,\\r\\n            remarks:this.state.remarks,\\r\\n           created_on:this.state.created_on,\\r\\n            sessionId: sessionId,\\r\\n            userName:userName,\\r\\n            });\\r\\n\\r\\n            this\\r\\n                .props\\r\\n                .history\\r\\n                .push(\\\"/SalesTracker/Updated\\\");\\r\\n\\r\\n        } else {\\r\\n            this\\r\\n                .validator\\r\\n                .showMessages();\\r\\n            this.forceUpdate();\\r\\n        }\\r\\n    };\\r\\n\\r\\n    render() {\\r\\n        const {EmployeesList, selectedOption} = this.state;\\r\\n\\r\\n        const divStyle = {\\r\\n            'height': '40px',\\r\\n            'resize':'none',\\r\\n        };\\r\\n        return (\\r\\n            <div>\\r\\n                <SidebarPage/>\\r\\n\\r\\n                <div className=\\\"all-content-wrapper\\\">\\r\\n                    <HeaderPage/>\\r\\n\\r\\n                    <div className=\\\"breadcome-area\\\">\\r\\n                        <div className=\\\"container-fluid\\\">\\r\\n                            <div className=\\\"row\\\">\\r\\n                                <div className=\\\"col-lg-12 col-md-12 col-sm-12 col-xs-12\\\">\\r\\n                                    <div className=\\\"breadcome-list\\\">\\r\\n                                        <div className=\\\"row\\\">\\r\\n                                            <div className=\\\"col-lg-6 col-md-6 col-sm-6 col-xs-6\\\">\\r\\n                                                <div className=\\\"breadcomb-wp\\\">\\r\\n\\r\\n                                                    <div className=\\\"breadcomb-ctn\\\">\\r\\n                                                        <h2>Sales Tracker